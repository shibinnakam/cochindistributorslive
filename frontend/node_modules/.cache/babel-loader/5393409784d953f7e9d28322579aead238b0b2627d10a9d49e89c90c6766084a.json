{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nexport default {\n  data() {\n    return {\n      products: [{\n        _id: 1,\n        name: \"Bingo\",\n        price: 10,\n        stock: 20,\n        description: \"Bingo Red Chilli\",\n        image: \"https://m.media-amazon.com/images/I/61VZZYQzJ4L.jpg\"\n      }, {\n        _id: 2,\n        name: \"Infosys Springboard\",\n        price: 23,\n        stock: 15,\n        description: \"xdawq\",\n        image: \"https://upload.wikimedia.org/wikipedia/commons/thumb/a/a2/St._George_Aruvithura_Church_Statue.jpg/800px-St._George_Aruvithura_Church_Statue.jpg\"\n      }]\n    };\n  },\n  methods: {\n    toggleStock(product) {\n      product.stock = product.stock > 0 ? 0 : 10; // Example toggle\n    },\n    deleteProduct(id) {\n      this.products = this.products.filter(p => p._id !== id);\n    },\n    editProduct(product) {\n      alert(`Edit product: ${product.name}`);\n    }\n  }\n};","map":{"version":3,"names":["data","products","_id","name","price","stock","description","image","methods","toggleStock","product","deleteProduct","id","filter","p","editProduct","alert"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\ProductList.vue"],"sourcesContent":["<template>\r\n  <div class=\"product-list\">\r\n    <div v-for=\"product in products\" :key=\"product._id\" class=\"product-card\">\r\n      <img :src=\"product.image\" alt=\"Product Image\" class=\"product-image\" />\r\n\r\n      <h3>\r\n        {{ product.name }} — ₹{{ product.price }}\r\n      </h3>\r\n\r\n      <p :class=\"product.stock > 0 ? 'in-stock' : 'out-stock'\">\r\n        {{ product.stock > 0 ? \"(In Stock)\" : \"(Out of Stock)\" }}\r\n      </p>\r\n\r\n      <p class=\"desc\">{{ product.description }}</p>\r\n\r\n      <div class=\"actions\">\r\n        <button @click=\"toggleStock(product)\" class=\"btn\">Turn Off</button>\r\n        <button @click=\"deleteProduct(product._id)\" class=\"btn delete\">Delete</button>\r\n        <button @click=\"editProduct(product)\" class=\"btn\">Edit</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      products: [\r\n        {\r\n          _id: 1,\r\n          name: \"Bingo\",\r\n          price: 10,\r\n          stock: 20,\r\n          description: \"Bingo Red Chilli\",\r\n          image: \"https://m.media-amazon.com/images/I/61VZZYQzJ4L.jpg\",\r\n        },\r\n        {\r\n          _id: 2,\r\n          name: \"Infosys Springboard\",\r\n          price: 23,\r\n          stock: 15,\r\n          description: \"xdawq\",\r\n          image: \"https://upload.wikimedia.org/wikipedia/commons/thumb/a/a2/St._George_Aruvithura_Church_Statue.jpg/800px-St._George_Aruvithura_Church_Statue.jpg\",\r\n        },\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    toggleStock(product) {\r\n      product.stock = product.stock > 0 ? 0 : 10; // Example toggle\r\n    },\r\n    deleteProduct(id) {\r\n      this.products = this.products.filter((p) => p._id !== id);\r\n    },\r\n    editProduct(product) {\r\n      alert(`Edit product: ${product.name}`);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.product-list {\r\n  display: flex;\r\n  gap: 20px;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n.product-card {\r\n  width: 250px;\r\n  padding: 15px;\r\n  border-radius: 20px;\r\n  box-shadow: 0px 4px 10px rgba(0,0,0,0.2);\r\n  text-align: center;\r\n  background: white;\r\n}\r\n\r\n.product-image {\r\n  width: 150px;\r\n  height: 150px;\r\n  object-fit: contain;\r\n  margin: auto;\r\n  display: block;\r\n}\r\n\r\nh3 {\r\n  font-weight: bold;\r\n}\r\n\r\n.in-stock {\r\n  color: green;\r\n  font-weight: bold;\r\n}\r\n\r\n.out-stock {\r\n  color: red;\r\n  font-weight: bold;\r\n}\r\n\r\n.desc {\r\n  margin: 5px 0;\r\n}\r\n\r\n.actions {\r\n  margin-top: 10px;\r\n}\r\n\r\n.btn {\r\n  background-color: #002b5b;\r\n  color: white;\r\n  border: none;\r\n  padding: 6px 12px;\r\n  margin: 5px;\r\n  border-radius: 20px;\r\n  cursor: pointer;\r\n}\r\n\r\n.delete {\r\n  background-color: darkred;\r\n}\r\n</style>\r\n"],"mappings":";;AAyBA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,CACR;QACEC,GAAG,EAAE,CAAC;QACNC,IAAI,EAAE,OAAO;QACbC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAE,EAAE;QACTC,WAAW,EAAE,kBAAkB;QAC/BC,KAAK,EAAE;MACT,CAAC,EACD;QACEL,GAAG,EAAE,CAAC;QACNC,IAAI,EAAE,qBAAqB;QAC3BC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAE,EAAE;QACTC,WAAW,EAAE,OAAO;QACpBC,KAAK,EAAE;MACT,CAAC;IAEL,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAACC,OAAO,EAAE;MACnBA,OAAO,CAACL,KAAI,GAAIK,OAAO,CAACL,KAAI,GAAI,IAAI,IAAI,EAAE,EAAE;IAC9C,CAAC;IACDM,aAAaA,CAACC,EAAE,EAAE;MAChB,IAAI,CAACX,QAAO,GAAI,IAAI,CAACA,QAAQ,CAACY,MAAM,CAAEC,CAAC,IAAKA,CAAC,CAACZ,GAAE,KAAMU,EAAE,CAAC;IAC3D,CAAC;IACDG,WAAWA,CAACL,OAAO,EAAE;MACnBM,KAAK,CAAC,iBAAiBN,OAAO,CAACP,IAAI,EAAE,CAAC;IACxC;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}