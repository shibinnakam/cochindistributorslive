{"ast":null,"code":"import axios from \"axios\";\nimport { useRoute } from \"vue-router\";\nexport default {\n  setup() {\n    const route = useRoute();\n    return {\n      route\n    };\n  },\n  data() {\n    return {\n      name: \"\",\n      address: \"\",\n      phone: \"\",\n      password: \"\",\n      message: \"\"\n    };\n  },\n  methods: {\n    async register() {\n      const token = this.route.query.token; // get token from URL\n      if (!token) {\n        this.message = \"Invalid or expired link\";\n        return;\n      }\n      try {\n        const res = await axios.post(\"http://localhost:5000/api/staff/register\", {\n          token,\n          name: this.name,\n          address: this.address,\n          phone: this.phone,\n          password: this.password\n        });\n        this.message = res.data.message;\n      } catch (err) {\n        this.message = err.response?.data?.message || \"Error submitting registration\";\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","useRoute","setup","route","data","name","address","phone","password","message","methods","register","token","query","res","post","err","response"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\StaffRegister.vue"],"sourcesContent":["<template>\n  <div class=\"staff-register\">\n    <h2>Complete Your Registration</h2>\n    <form @submit.prevent=\"register\">\n      <input v-model=\"name\" placeholder=\"Name\" required />\n      <input v-model=\"address\" placeholder=\"Address\" required />\n      <input v-model=\"phone\" placeholder=\"Phone\" required />\n      <input\n        type=\"password\"\n        v-model=\"password\"\n        placeholder=\"Password\"\n        required\n      />\n      <button type=\"submit\">Submit</button>\n    </form>\n    <p v-if=\"message\">{{ message }}</p>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { useRoute } from \"vue-router\";\n\nexport default {\n  setup() {\n    const route = useRoute();\n\n    return { route };\n  },\n  data() {\n    return {\n      name: \"\",\n      address: \"\",\n      phone: \"\",\n      password: \"\",\n      message: \"\",\n    };\n  },\n  methods: {\n    async register() {\n      const token = this.route.query.token; // get token from URL\n      if (!token) {\n        this.message = \"Invalid or expired link\";\n        return;\n      }\n\n      try {\n        const res = await axios.post(\n          \"http://localhost:5000/api/staff/register\",\n          {\n            token,\n            name: this.name,\n            address: this.address,\n            phone: this.phone,\n            password: this.password,\n          }\n        );\n        this.message = res.data.message;\n      } catch (err) {\n        this.message =\n          err.response?.data?.message || \"Error submitting registration\";\n      }\n    },\n  },\n};\n</script>\n"],"mappings":"AAoBA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,QAAO,QAAS,YAAY;AAErC,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIF,QAAQ,CAAC,CAAC;IAExB,OAAO;MAAEE;IAAM,CAAC;EAClB,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,OAAO,EAAE,EAAE;MACXC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,QAAQA,CAAA,EAAG;MACf,MAAMC,KAAI,GAAI,IAAI,CAACT,KAAK,CAACU,KAAK,CAACD,KAAK,EAAE;MACtC,IAAI,CAACA,KAAK,EAAE;QACV,IAAI,CAACH,OAAM,GAAI,yBAAyB;QACxC;MACF;MAEA,IAAI;QACF,MAAMK,GAAE,GAAI,MAAMd,KAAK,CAACe,IAAI,CAC1B,0CAA0C,EAC1C;UACEH,KAAK;UACLP,IAAI,EAAE,IAAI,CAACA,IAAI;UACfC,OAAO,EAAE,IAAI,CAACA,OAAO;UACrBC,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CACF,CAAC;QACD,IAAI,CAACC,OAAM,GAAIK,GAAG,CAACV,IAAI,CAACK,OAAO;MACjC,EAAE,OAAOO,GAAG,EAAE;QACZ,IAAI,CAACP,OAAM,GACTO,GAAG,CAACC,QAAQ,EAAEb,IAAI,EAAEK,OAAM,IAAK,+BAA+B;MAClE;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}