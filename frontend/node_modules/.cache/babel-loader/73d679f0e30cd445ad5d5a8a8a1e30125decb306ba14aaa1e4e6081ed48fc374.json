iv>\r\n      <div class=\"overlay-text\">\r\n        <h1 class=\"typing-text\">Secure Access Starts Here: Login or Sign Up</h1>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Right Side with Form -->\r\n    <div class=\"right-side\">\r\n      <div class=\"form-box\">\r\n        <h2>{{ isLogin ? 'Login' : 'Sign Up' }}</h2>\r\n        <form @submit.prevent=\"handleSubmit\">\r\n          <input v-model=\"email\" type=\"email\" placeholder=\"Email\" required />\r\n          <input v-model=\"password\" type=\"password\" placeholder=\"Password\" required />\r\n          <button type=\"submit\">{{ isLogin ? 'Login' : 'Sign Up' }}</button>\r\n        </form>\r\n        <p v-if=\"error\" class=\"error\">{{ error }}</p>\r\n        <p class=\"switch-link\">\r\n          {{ isLogin ? \"Don't have an account?\" : \"Already have an account?\" }}\r\n          <span @click=\"toggleMode\">{{ isLogin ? 'Sign Up' : 'Login' }}</span>\r\n        </p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      email: '',\r\n      password: '',\r\n      error: '',\r\n      isLogin: true,\r\n    };\r\n  },\r\n  methods: {\r\n    async handleSubmit() {\r\n      try {\r\n        // Shortcut Admin Login\r\n        if (this.email === 'admin@gmail.com' && this.password === 'admin') {\r\n          alert('Welcome Admin!');\r\n          this.$router.push('/admin');\r\n          return;\r\n        }\r\n\r\n        // Prevent registering with admin email\r\n        if (!this.isLogin && this.email === 'admin@gmail.com') {\r\n          this.error = 'This email is reserved for admin.';\r\n          return;\r\n        }\r\n\r\n        if (this.isLogin) {\r\n          const response = await axios.post('http://localhost:5000/api/auth/login', {\r\n            email: this.email,\r\n            password: this.password,\r\n          });\r\n          localStorage.setItem('token', response.data.token);\r\n          alert('Login Successful!');\r\n          this.$router.push('/dashboard');\r\n        } else {\r\n          await axios.post('http://localhost:5000/api/auth/register', {\r\n            email: this.email,\r\n            password: this.password,\r\n          });\r\n          alert('Signup successful! Now you can log in.');\r\n          this.isLogin = true;\r\n        }\r\n      } catch (err) {\r\n        this.error = err.response?.data?.msg || 'Something went wrong.';\r\n      }\r\n    },\r\n    toggleMode() {\r\n      this.isLogin = !this.isLogin;\r\n      this.error = '';\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.auth-container {\r\n  display: flex;\r\n  height: 100vh;\r\n  font-family: 'Segoe UI', sans-serif;\r\n  overflow: hidden;\r\n}\r\n\r\n.left-side {\r\n  position: relative;\r\n  width: 50%;\r\n  overflow: hidden;\r\n}\r\n\r\n.left-side img {\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: cover;\r\n}\r\n\r\n.overlay {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  height: 100%;\r\n  width: 100%;\r\n  background: rgba(0, 0, 0, 0.5);\r\n  z-index: 1;\r\n}\r\n\r\n.overlay-text {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  z-index: 2;\r\n  text-align: center;\r\n  color: #fff;\r\n}\r\n\r\n.typing-text {\r\n  font-size: 1.8rem;\r\n  white-space: nowrap;\r\n  overflow: hidden;\r\n  border-right: 2px solid white;\r\n  width: 0;\r\n  animation: typing 4s steps(40, end) forwards, blink 0.75s step-end infinite;\r\n}\r\n\r\n@keyframes typing {\r\n  from {\r\n    width: 0;\r\n  }\r\n  to {\r\n    width: 100%;\r\n  }\r\n}\r\n\r\n@keyframes blink {\r\n  50% {\r\n    border-color: transparent;\r\n  }\r\n}\r\n\r\n.right-side {\r\n  width: 50%;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  background: #f5f5f5;\r\n  animation: slideIn 1s ease-out forwards;\r\n}\r\n\r\n@keyframes slideIn {\r\n  from {\r\n    opacity: 0;\r\n    transform: translateX(50px);\r\n  }\r\n  to {\r\n    opacity: 1;\r\n    transform: translateX(0);\r\n  }\r\n}\r\n\r\n.form-box {\r\n  background: white;\r\n  padding: 2.5rem;\r\n  border-radius: 16px;\r\n  box-shadow: 0 12px 25px rgba(0, 0, 0, 0.1);\r\n  width: 330px;\r\n  text-align: center;\r\n  transition: transform 0.3s ease;\r\n}\r\n\r\n.form-box:hover {\r\n  transform: scale(1.02);\r\n}\r\n\r\ninput {\r\n  display: block;\r\n  margin: 0.8rem 0;\r\n  padding: 0.6rem;\r\n  width: 100%;\r\n  border: 1px solid #ccc;\r\n  border-radius: 6px;\r\n  transition: border-color 0.3s, box-shadow 0.3s;\r\n}\r\n\r\ninput:focus {\r\n  outline: none;\r\n  border-color: #007bff;\r\n  box-shadow: 0 0 5px rgba(0, 123, 255, 0.5);\r\n}\r\n\r\nbutton {\r\n  background-color: #007BFF;\r\n  color: white;\r\n  padding: 0.7rem 1rem;\r\n  border: none;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  width: 100%;\r\n  font-weight: bold;\r\n  transition: background-color 0.3s ease;\r\n  margin-top: 1rem;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #0056b3;\r\n}\r\n\r\n.error {\r\n  color: red;\r\n  margin-top: 0.5rem;\r\n  font-size: 0.9rem;\r\n}\r\n\r\n.switch-link {\r\n  margin-top: 1rem;\r\n  font-size: 0.95rem;\r\n  color: #333;\r\n}\r\n\r\n.switch-link span {\r\n  color: #007BFF;\r\n  cursor: pointer;\r\n  text-decoration: underline;\r\n  margin-left: 5px;\r\n  transition: color 0.3s;\r\n}\r\n\r\n.switch-link span:hover {\r\n  color: #0056b3;\r\n}\r\n</style>\r\n"],"mappings":";;EAEOA,KAAK,EAAC;AAAgB;;EAEpBA,KAAK,EAAC;AAAW;;;EASjBA,KAAK,EAAC;AAAY;;EAChBA,KAAK,EAAC;AAAU;;EAKTC,IAAI,EAAC;AAAQ;;;EAEPD,KAAK,EAAC;;;EACnBA,KAAK,EAAC;AAAa;;uBApB5BE,mBAAA,CA0BM,OA1BNC,UA0BM,GAzBJC,mBAAA,sCAAyC,EACzCC,mBAAA,CAMM,OANNC,UAMM,GALJD,mBAAA,CAAsE;IAAhEE,GAAG,EAAEC,OAAO;IAA2BC,GAAG,EAAC;iEACjDJ,mBAAA,CAA2B;IAAtBL,KAAK,EAAC;EAAS,4B,0BACpBK,mBAAA,CAEM;IAFDL,KAAK,EAAC;EAAc,IACvBK,mBAAA,CAAwE;IAApEL,KAAK,EAAC;EAAa,GAAC,6CAA2C,E,uBAIvEI,mBAAA,0BAA6B,EAC7BC,mBAAA,CAcM,OAdNK,UAcM,GAbJL,mBAAA,CAYM,OAZNM,UAYM,GAXJN,