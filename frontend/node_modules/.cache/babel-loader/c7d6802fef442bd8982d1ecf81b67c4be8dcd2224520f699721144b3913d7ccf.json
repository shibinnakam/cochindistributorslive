,"triggerImageUpload","title","ref","onChange","handleImageSearch","accept","style","_hoisted_5","_hoisted_6","_toDisplayString","userName","_hoisted_7","href","_withModifiers","showProfile","logout","showCart","cartCount","_hoisted_8","_hoisted_9","_Fragment","_renderList","categories","cat","_normalizeClass","active","selectedCategory","_id","key","selectCategory","_hoisted_11","image","src","getImageUrl","alt","name","showWelcome","_hoisted_13","_hoisted_14","_hoisted_15","_hoisted_16","_hoisted_17","loading","_hoisted_18","filteredProducts","length","_hoisted_19","_hoisted_20","product","active3DProductId","_hoisted_21","_createVNode","_component_ThreeDBox","imageFront","imageSide","imageBack","imageTop","imageBottom","model3D","shape","toggle3D","_hoisted_22","has3DImages","_hoisted_24","_hoisted_25","_hoisted_26","_hoisted_27","_hoisted_28","discountPrice","_hoisted_29","originalPrice","_hoisted_30","calculateDiscount","addToCart","_hoisted_31","_component_ProfilePage","_component_CartPage","onCartUpdated","onOrderPlaced","_component_ChatBot","products"],"sources":["C:\\Users\\LENOVO\\Music\\OneDrive\\Pictures\\distribution-agency-project\\frontend\\src\\views\\UserPage.vue"],"sourcesContent":["<OrdersPage v-if=\"showOrders\" @close=\"showOrders = false\" />\n=======\n<template>\n  <div class=\"flipkart-layout\">\n    <!-- Orders Modal -->\n    <OrdersPage v-if=\"showOrders\" @close=\"showOrders = false\" />\n\n    <!-- Wallet Modal -->\n    <WalletPage v-if=\"showWallet\" @close=\"showWallet = false\" />\n    <nav class=\"navbar\">\n      <div class=\"nav-container\">\n        <div class=\"logo-section\">\n          <span class=\"brand-name\">Distribution Agency</span>\n        </div>\n\n        <div class=\"search-bar\">\n          <input\n            type=\"text\"\n            placeholder=\"Search for products, brands and more\"\n            v-model=\"searchQuery\"\n          />\n          <button\n            class=\"camera-btn\"\n            @click=\"triggerImageUpload\"\n            title=\"Search by Image\"\n          >\n            üì∑\n          </button>\n          <button class=\"search-btn\">üîç</button>\n          <input\n            type=\"file\"\n            ref=\"imageInput\"\n            @change=\"handleImageSearch\"\n            accept=\"image/*\"\n            style=\"display: none\"\n          />\n        </div>\n\n        <div class=\"nav-links\">\n          <div class=\"nav-item user-menu\">\n            <span>{{ userName || \"User\" }}</span>\n            <div class=\"dropdown\">\n              <a href=\"#\" @click.prevent=\"showProfile = true\">My Profile</a>\n              <a href=\"#\" @click.prevent=\"showOrders = true\">My Orders</a>\n              <a href=\"#\" @click.prevent=\"showWallet = true\">My Wallet</a>\n              <a href=\"#\" @click.prevent=\"logout\">Logout</a>\n            </div>\n          </div>\n          <div class=\"nav-item\">\n            <span>Become a Seller</span>\n          </div>\n          <div class=\"nav-item\">\n            <span>More</span>\n          </div>\n          <div class=\"nav-item wallet\" @click=\"showWallet = true\">\n            <span>üí∞ Wallet</span>\n          </div>\n          <div class=\"nav-item cart\" @click=\"showCart = true\">\n            <span\n              >üõí Cart\n              <span v-if=\"cartCount > 0\" class=\"cart-badge\">{{\n                cartCount\n              }}</span></span\n            >\n          </div>\n        </div>\n      </div>\n    </nav>\n\n    <!-- Categories Bar -->\n    <div class=\"categories-bar\">\n      <div\n        class=\"category-item\"\n        v-for=\"cat in categories\"\n        :key=\"cat._id\"\n        @click=\"selectCategory(cat)\"\n        :class=\"{ active: selectedCategory === cat._id }\"\n      >\n        <div class=\"cat-img-placeholder\">\n          <img\n            v-if=\"cat.image\"\n            :src=\"getImageUrl(cat.image)\"\n            :alt=\"cat.name\"\n            class=\"cat-img\"\n          />\n        </div>\n        <span>{{ cat.name }}</span>\n      </div>\n    </div>\n\n    <!-- Welcome Banner -->\n    <div v-if=\"showWelcome\" class=\"welcome-banner\">\n      <div class=\"welcome-content\">\n        <span class=\"welcome-text\"\n          >Welcome back, <b>{{ userName }}</b\n          >! Explore our latest collection.</span\n        >\n        <button class=\"close-welcome\" @click=\"showWelcome = false\">‚úï</button>\n      </div>\n    </div>\n\n    <!-- Main Content -->\n    <main class=\"main-content\">\n      <!-- Banner (Placeholder) -->\n      <div class=\"banner-section\">\n        <div class=\"banner\">\n          <h2>Big Savings on Distribution Products!</h2>\n          <p>Shop now for the best deals.</p>\n        </div>\n      </div>\n\n      <!-- Product Grid -->\n      <div class=\"product-section\">\n        <h3>Latest Products</h3>\n\n        <div v-if=\"loading\" class=\"loading\">\n          <div class=\"spinner\"></div>\n          <p>Loading products...</p>\n        </div>\n\n        <div v-else-if=\"filteredProducts.length === 0\" class=\"no-products\">\n          <p>No products found.</p>\n        </div>\n\n        <div v-else class=\"products-grid\">\n          <div\n            v-for=\"product in filteredProducts\"\n            :key=\"product._id\"\n            class=\"product-card\"\n          >\n            <div class=\"image-wrapper\" :class=\"{ 'expanded': active3DProductId === product._id }\">\n              <!-- 3D View Toggle -->\n              <div\n                v-if=\"active3DProductId === product._id\"\n                class=\"model-container\"\n              >\n                <ThreeDBox\n                  :image=\"getImageUrl(product.image)\"\n                  :image-front=\"getImageUrl(product.imageFront)\"\n                  :image-side=\"getImageUrl(product.imageSide)\"\n                  :image-back=\"getImageUrl(product.imageBack)\"\n                  :image-top=\"getImageUrl(product.imageTop)\"\n                  :image-bottom=\"getImageUrl(product.imageBottom)\"\n                  :model3D=\"getImageUrl(product.model3D)\"\n                  :shape=\"product.shape\"\n                />\n                <button class=\"close-3d\" @click.stop=\"toggle3D(product._id)\">\n                  ‚úï\n                </button>\n              </div>\n              <template v-else>\n                <img\n                  :src=\"getImageUrl(product.image || product.imageFront)\"\n                  :alt=\"product.name\"\n                  class=\"product-img\"\n                />\n                <button\n                  class=\"view-3d-btn\"\n                  @click.stop=\"toggle3D(product._id)\"\n                  v-if=\"has3DImages(product)\"\n                >\n                  3D View\n                </button>\n              </template>\n\n              <div class=\"wishlist-icon\">‚ù§</div>\n            </div>\n\n            <div class=\"product-details\">\n              <h3 class=\"product-title\" :title=\"product.name\">\n                {{ product.name }}\n              </h3>\n              <div class=\"rating-badge\">4.4 ‚òÖ</div>\n              <div class=\"price-row\">\n                <span class=\"current-price\">‚Çπ{{ product.discountPrice }}</span>\n                <span class=\"original-price\">‚Çπ{{ product.originalPrice }}</span>\n                <span class=\"discount\"\n                  >{{\n                    calculateDiscount(\n                      product.originalPrice,\n                      product.discountPrice\n                    )\n                  }}% off</span\n                >\n              </div>\n              <div class=\"offers\">\n                <span>Free delivery</span>\n              </div>\n              <button class=\"add-to-cart-btn\" @click.stop=\"addToCart(product)\">\n                ADD TO CART\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </main>\n\n    <!-- Profile Modal -->\n    <ProfilePage v-if=\"showProfile\" @close=\"showProfile = false\" />\n\n    <!-- Cart Modal -->\n    <CartPage\n      v-if=\"showCart\"\n      @close=\"showCart = false\"\n      @cart-updated=\"cartCount = $event\"\n      @order-placed=\"showOrders = true\"\n    />\n\n    <!-- Orders Modal -->\n    <OrdersPage v-if=\"showOrders\" @close=\"showOrders = false\" />\n\n    <!-- ChatBot -->\n    <ChatBot :categories=\"categories\" :products=\"products\" />\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport ThreeDBox from \"@/components/ThreeDBox.vue\";\nimport ChatBot from \"@/components/ChatBot.vue\";\nimport ProfilePage from \"@/views/ProfilePage.vue\";\nimport CartPage from \"@/views/CartPage.vue\";\nimport OrdersPage from \"@/views/OrdersPage.vue\";\nimport WalletPage from \"@/views/WalletPage.vue\";\n\nexport default {\n  name: \"UserPage\",\n  components: {\n    ThreeDBox,\n    ChatBot,\n    ProfilePage,\n    CartPage,\n    OrdersPage,\n    WalletPage,\n  },\n  data() {\n    return {\n      products: [],\n      categories: [],\n      loading: false,\n      searchQuery: \"\",\n      userName: \"User\",\n      active3DProductId: null,\n      cartCount: 0,\n      showProfile: false,\n      showCart: false,\n      showOrders: false,\n      showWallet: false,\n      showWelcome: true,\n      selectedCategory: null,\n    };\n  },\n  computed: {\n    filteredProducts() {\n      let result = this.products;\n\n      // Filter by Category\n      if (this.selectedCategory) {\n        result = result.filter(\n          (p) => p.category && p.category._id === this.selectedCategory\n        );\n      }\n\n      // Filter by Search Query\n      if (this.searchQuery) {\n        const query = this.searchQuery.toLowerCase();\n        result = result.filter(\n          (p) =>\n            p.name.toLowerCase().includes(query) ||\n            p.description.toLowerCase().includes(query)\n        );\n      }\n\n      return result;\