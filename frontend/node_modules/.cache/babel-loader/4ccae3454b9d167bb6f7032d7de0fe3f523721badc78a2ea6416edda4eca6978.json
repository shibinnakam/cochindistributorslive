{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"LoginPage\",\n  data() {\n    return {\n      isSignup: false,\n      email: \"\",\n      password: \"\",\n      isFilled: false,\n      emailError: \"\",\n      passwordError: \"\",\n      loading: false\n    };\n  },\n  computed: {\n    formTitle() {\n      return this.isSignup ? \"Sign Up\" : \"Login\";\n    },\n    toggleText() {\n      return this.isSignup ? \"Already have an account?\" : \"Don’t have an account?\";\n    },\n    toggleLinkText() {\n      return this.isSignup ? \"Login\" : \"Create an account\";\n    }\n  },\n  methods: {\n    toggleMode() {\n      this.isSignup = !this.isSignup;\n      this.resetForm();\n    },\n    resetForm() {\n      this.email = \"\";\n      this.password = \"\";\n      this.isFilled = false;\n      this.emailError = \"\";\n      this.passwordError = \"\";\n    },\n    checkFilled() {\n      this.isFilled = this.email.trim() !== \"\" && this.password.trim() !== \"\";\n    },\n    validateEmail(email) {\n      const emailRegex = /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/;\n      return emailRegex.test(email);\n    },\n    async handleSubmit() {\n      this.emailError = \"\";\n      this.passwordError = \"\";\n      if (!this.validateEmail(this.email)) {\n        this.emailError = \"Please enter a valid email address.\";\n      }\n      if (this.isSignup && this.password.length < 6) {\n        this.passwordError = \"Password must be at least 6 characters.\";\n      }\n      if (this.emailError || this.passwordError) return;\n      this.loading = true;\n      try {\n        const endpoint = this.isSignup ? \"/api/auth/register\" : \"/api/auth/login\";\n        const {\n          data\n        } = await axios.post(endpoint, {\n          email: this.email,\n          password: this.password\n        });\n        if (this.isSignup) {\n          alert(\"Registered successfully. Please login.\");\n          this.toggleMode();\n        } else {\n          localStorage.setItem(\"token\", data.token);\n          alert(\"Login successful!\");\n          this.$router.push(\"/admin\");\n        }\n      } catch (err) {\n        console.error(\"Auth error:\", err);\n        alert(err.response?.data?.msg || \"Something went wrong.\");\n      } finally {\n        this.loading = false;\n      }\n    },\n    async forgotPassword() {\n      const emailInput = prompt(\"Please enter your registered email address:\");\n      if (!emailInput || !this.validateEmail(emailInput)) {\n        alert(\"Please enter a valid email address.\");\n        return;\n      }\n      try {\n        await axios.post(\"/api/auth/forgot-password\", {\n          email: emailInput\n        });\n        alert(\"If the email exists, a reset link has been sent.\");\n      } catch (err) {\n        console.error(\"Forgot password error:\", err);\n        alert(err.response?.data?.msg || \"Error sending reset email.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","isSignup","email","password","isFilled","emailError","passwordError","loading","computed","formTitle","toggleText","toggleLinkText","methods","toggleMode","resetForm","checkFilled","trim","validateEmail","emailRegex","test","handleSubmit","length","endpoint","post","alert","localStorage","setItem","token","$router","push","err","console","error","response","msg","forgotPassword","emailInput","prompt"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\Login.vue"],"sourcesContent":["<template>\n  <div class=\"login-container\">\n    <div class=\"overlay\"></div>\n\n    <div class=\"login-form-section\">\n      <div :class=\"['login-box', isFilled ? 'box-filled' : '']\">\n        <img src=\"@/assets/logo.jpeg\" alt=\"Company Logo\" class=\"logo\" />\n\n        <h2 class=\"form-title\">{{ formTitle }}</h2>\n\n        <p class=\"signup-link\">\n          {{ toggleText }}\n          <a href=\"#\" @click.prevent=\"toggleMode\">{{ toggleLinkText }}</a>\n        </p>\n\n        <form\n          class=\"login-form\"\n          @submit.prevent=\"handleSubmit\"\n          @input=\"checkFilled\"\n          novalidate\n        >\n          <label for=\"email\" class=\"sr-only\">Email</label>\n          <input\n            type=\"email\"\n            id=\"email\"\n            v-model.trim=\"email\"\n            placeholder=\"Enter email\"\n            required\n            autocomplete=\"username\"\n          />\n          <p v-if=\"emailError\" class=\"error\">{{ emailError }}</p>\n\n          <label for=\"password\" class=\"sr-only\">Password</label>\n          <input\n            type=\"password\"\n            id=\"password\"\n            v-model.trim=\"password\"\n            placeholder=\"Enter password\"\n            required\n            :autocomplete=\"isSignup ? 'new-password' : 'current-password'\"\n          />\n          <p v-if=\"passwordError\" class=\"error\">{{ passwordError }}</p>\n\n          <a\n            v-if=\"!isSignup\"\n            href=\"#\"\n            class=\"forgot-password\"\n            @click.prevent=\"forgotPassword\"\n          >\n            Forgot your password?\n          </a>\n\n          <button type=\"submit\" class=\"login-button\" :disabled=\"loading\">\n            <span v-if=\"loading\">Processing...</span>\n            <span v-else>{{ isSignup ? \"Sign Up\" : \"Login\" }}</span>\n          </button>\n        </form>\n\n        <footer class=\"form-footer\">\n          <small>© 2025 YourCompany. All rights reserved.</small>\n        </footer>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"LoginPage\",\n  data() {\n    return {\n      isSignup: false,\n      email: \"\",\n      password: \"\",\n      isFilled: false,\n      emailError: \"\",\n      passwordError: \"\",\n      loading: false,\n    };\n  },\n  computed: {\n    formTitle() {\n      return this.isSignup ? \"Sign Up\" : \"Login\";\n    },\n    toggleText() {\n      return this.isSignup\n        ? \"Already have an account?\"\n        : \"Don’t have an account?\";\n    },\n    toggleLinkText() {\n      return this.isSignup ? \"Login\" : \"Create an account\";\n    },\n  },\n  methods: {\n    toggleMode() {\n      this.isSignup = !this.isSignup;\n      this.resetForm();\n    },\n    resetForm() {\n      this.email = \"\";\n      this.password = \"\";\n      this.isFilled = false;\n      this.emailError = \"\";\n      this.passwordError = \"\";\n    },\n    checkFilled() {\n      this.isFilled = this.email.trim() !== \"\" && this.password.trim() !== \"\";\n    },\n    validateEmail(email) {\n      const emailRegex = /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/;\n      return emailRegex.test(email);\n    },\n    async handleSubmit() {\n      this.emailError = \"\";\n      this.passwordError = \"\";\n\n      if (!this.validateEmail(this.email)) {\n        this.emailError = \"Please enter a valid email address.\";\n      }\n      if (this.isSignup && this.password.length < 6) {\n        this.passwordError = \"Password must be at least 6 characters.\";\n      }\n      if (this.emailError || this.passwordError) return;\n\n      this.loading = true;\n      try {\n        const endpoint = this.isSignup\n          ? \"/api/auth/register\"\n          : \"/api/auth/login\";\n\n        const { data } = await axios.post(endpoint, {\n          email: this.email,\n          password: this.password,\n        });\n\n        if (this.isSignup) {\n          alert(\"Registered successfully. Please login.\");\n          this.toggleMode();\n        } else {\n          localStorage.setItem(\"token\", data.token);\n          alert(\"Login successful!\");\n          this.$router.push(\"/admin\");\n        }\n      } catch (err) {\n        console.error(\"Auth error:\", err);\n        alert(err.response?.data?.msg || \"Something went wrong.\");\n      } finally {\n        this.loading = false;\n      }\n    },\n    async forgotPassword() {\n      const emailInput = prompt(\"Please enter your registered email address:\");\n      if (!emailInput || !this.validateEmail(emailInput)) {\n        alert(\"Please enter a valid email address.\");\n        return;\n      }\n\n      try {\n        await axios.post(\"/api/auth/forgot-password\", { email: emailInput });\n        alert(\"If the email exists, a reset link has been sent.\");\n      } catch (err) {\n        console.error(\"Forgot password error:\", err);\n        alert(err.response?.data?.msg || \"Error sending reset email.\");\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* same styles as your current one */\n</style>\n"],"mappings":";AAmEA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,KAAK;MACfC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,KAAK;MACfC,UAAU,EAAE,EAAE;MACdC,aAAa,EAAE,EAAE;MACjBC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,SAASA,CAAA,EAAG;MACV,OAAO,IAAI,CAACR,QAAO,GAAI,SAAQ,GAAI,OAAO;IAC5C,CAAC;IACDS,UAAUA,CAAA,EAAG;MACX,OAAO,IAAI,CAACT,QAAO,GACf,0BAAyB,GACzB,wBAAwB;IAC9B,CAAC;IACDU,cAAcA,CAAA,EAAG;MACf,OAAO,IAAI,CAACV,QAAO,GAAI,OAAM,GAAI,mBAAmB;IACtD;EACF,CAAC;EACDW,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACZ,QAAO,GAAI,CAAC,IAAI,CAACA,QAAQ;MAC9B,IAAI,CAACa,SAAS,CAAC,CAAC;IAClB,CAAC;IACDA,SAASA,CAAA,EAAG;MACV,IAAI,CAACZ,KAAI,GAAI,EAAE;MACf,IAAI,CAACC,QAAO,GAAI,EAAE;MAClB,IAAI,CAACC,QAAO,GAAI,KAAK;MACrB,IAAI,CAACC,UAAS,GAAI,EAAE;MACpB,IAAI,CAACC,aAAY,GAAI,EAAE;IACzB,CAAC;IACDS,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACX,QAAO,GAAI,IAAI,CAACF,KAAK,CAACc,IAAI,CAAC,MAAM,EAAC,IAAK,IAAI,CAACb,QAAQ,CAACa,IAAI,CAAC,MAAM,EAAE;IACzE,CAAC;IACDC,aAAaA,CAACf,KAAK,EAAE;MACnB,MAAMgB,UAAS,GAAI,4BAA4B;MAC/C,OAAOA,UAAU,CAACC,IAAI,CAACjB,KAAK,CAAC;IAC/B,CAAC;IACD,MAAMkB,YAAYA,CAAA,EAAG;MACnB,IAAI,CAACf,UAAS,GAAI,EAAE;MACpB,IAAI,CAACC,aAAY,GAAI,EAAE;MAEvB,IAAI,CAAC,IAAI,CAACW,aAAa,CAAC,IAAI,CAACf,KAAK,CAAC,EAAE;QACnC,IAAI,CAACG,UAAS,G