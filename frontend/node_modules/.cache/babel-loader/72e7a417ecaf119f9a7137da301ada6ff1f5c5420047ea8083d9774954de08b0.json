{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      email: '',\n      password: '',\n      error: '',\n      isLogin: true\n    };\n  },\n  methods: {\n    async handleSubmit() {\n      try {\n        if (this.email === 'admin@gmail.com' && this.password === 'admin') {\n          alert('Welcome Admin!');\n          this.$router.push('/admin');\n          return;\n        }\n        if (!this.isLogin && this.email === 'admin@gmail.com') {\n          this.error = 'This email is reserved for admin.';\n          return;\n        }\n        if (this.isLogin) {\n          const response = await axios.post('http://localhost:5000/api/auth/login', {\n            email: this.email,\n            password: this.password\n          });\n          localStorage.setItem('token', response.data.token);\n          alert('Login Successful!');\n          this.$router.push('/dashboard');\n        } else {\n          await axios.post('http://localhost:5000/api/auth/register', {\n            email: this.email,\n            password: this.password\n          });\n          alert('Signup successful! Now you can log in.');\n          this.isLogin = true;\n        }\n      } catch (err) {\n        this.error = err.response?.data?.msg || 'Something went wrong.';\n      }\n    },\n    toggleMode() {\n      this.isLogin = !this.isLogin;\n      this.error = '';\n    }\n  }\n};","map":{"version":3,"names":["axios","data","email","password","error","isLogin","methods","handleSubmit","alert","$router","push","response","post","localStorage","setItem","token","err","msg","toggleMode"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\Login.vue"],"sourcesContent":["<!-- eslint-disable vue/multi-word-component-names -->\r\n<template>\r\n  <div class=\"auth-container\">\r\n    <!-- Left Side with Image and Overlay -->\r\n    <div class=\"left-side\">\r\n      <img :src=\"require('@/assets/login-bg.jpg')\" alt=\"Login Background\" />\r\n      <div class=\"overlay\"></div>\r\n      <div class=\"overlay-text\">\r\n        <h1 class=\"typing-text\">Secure Access Starts Here: Login or Sign Up</h1>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Right Side with Form -->\r\n    <div class=\"right-side\">\r\n      <div class=\"form-box\">\r\n        <h2>{{ isLogin ? 'Login' : 'Sign Up' }}</h2>\r\n        <form @submit.prevent=\"handleSubmit\">\r\n          <input v-model=\"email\" type=\"email\" placeholder=\"Email\" required />\r\n          <input v-model=\"password\" type=\"password\" placeholder=\"Password\" required />\r\n          <button type=\"submit\">{{ isLogin ? 'Login' : 'Sign Up' }}</button>\r\n        </form>\r\n        <p v-if=\"error\" class=\"error\">{{ error }}</p>\r\n        <p class=\"switch-link\">\r\n          {{ isLogin ? \"Don't have an account?\" : \"Already have an account?\" }}\r\n          <span @click=\"toggleMode\">{{ isLogin ? 'Sign Up' : 'Login' }}</span>\r\n        </p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      email: '',\r\n      password: '',\r\n      error: '',\r\n      isLogin: true,\r\n    };\r\n  },\r\n  methods: {\r\n    async handleSubmit() {\r\n      try {\r\n        if (this.email === 'admin@gmail.com' && this.password === 'admin') {\r\n          alert('Welcome Admin!');\r\n          this.$router.push('/admin');\r\n          return;\r\n        }\r\n\r\n        if (!this.isLogin && this.email === 'admin@gmail.com') {\r\n          this.error = 'This email is reserved for admin.';\r\n          return;\r\n        }\r\n\r\n        if (this.isLogin) {\r\n          const response = await axios.post('http://localhost:5000/api/auth/login', {\r\n            email: this.email,\r\n            password: this.password,\r\n          });\r\n          localStorage.setItem('token', response.data.token);\r\n          alert('Login Successful!');\r\n          this.$router.push('/dashboard');\r\n        } else {\r\n          await axios.post('http://localhost:5000/api/auth/register', {\r\n            email: this.email,\r\n            password: this.password,\r\n          });\r\n          alert('Signup successful! Now you can log in.');\r\n          this.isLogin = true;\r\n        }\r\n      } catch (err) {\r\n        this.error = err.response?.data?.msg || 'Something went wrong.';\r\n      }\r\n    },\r\n    toggleMode() {\r\n      this.isLogin = !this.isLogin;\r\n      this.error = '';\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.auth-container {\r\n  display: flex;\r\n  height: 100vh;\r\n  font-family: 'Segoe UI', sans-serif;\r\n}\r\n\r\n.left-side {\r\n  position: relative;\r\n  width: 50%;\r\n  overflow: hidden;\r\n}\r\n\r\n.left-side img {\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: cover;\r\n}\r\n\r\n.overlay {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  height: 100%;\r\n  width: 100%;\r\n  background: rgba(0, 0, 0, 0.6); /* Black shade */\r\n  z-index: 1;\r\n}\r\n\r\n.overlay-text {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  z-index: 2;\r\n  text-align: center;\r\n  color: #fff;\r\n}\r\n\r\n.typing-text {\r\n  font-size: 1.8rem;\r\n  white-space: nowrap;\r\n  overflow: hidden;\r\n  border-right: 2px solid white;\r\n  width: 0;\r\n  animation: typing 4s steps(40, end) forwards, blink 0.75s step-end infinite;\r\n}\r\n\r\n@keyframes typing {\r\n  from {\r\n    width: 0;\r\n  }\r\n  to {\r\n    width: 100%;\r\n  }\r\n}\r\n\r\n@keyframes blink {\r\n  50% {\r\n    border-color: transparent;\r\n  }\r\n}\r\n\r\n.right-side {\r\n  width: 50%;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  background: #f5f5f5;\r\n}\r\n\r\n.form-box {\r\n  background: white;\r\n  padding: 2rem;\r\n  border-radius: 12px;\r\n  box-shadow: 0 0 10px rgba(0,0,0,0.1);\r\n  width: 300px;\r\n  text-align: center;\r\n}\r\n\r\ninput {\r\n  display: block;\r\n  margin: 0.5rem 0;\r\n  padding: 0.5rem;\r\n  width: 100%;\r\n  border: 1px solid #ccc;\r\n  border-radius: 6px;\r\n}\r\n\r\nbutton {\r\n  background-color: #007BFF;\r\n  color: white;\r\n  padding: 0.6rem 1rem;\r\n  border: none;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  width: 100%;\r\n  margin-top: 1rem;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #0056b3;\r\n}\r\n\r\n.error {\r\n  color: red;\r\n  margin-top: 0.5rem;\r\n}\r\n\r\n.switch-link {\r\n  margin-top: 1rem;\r\n  font-size: 0.9rem;\r\n}\r\n\r\n.switch-link span {\r\n  color: #007BFF;\r\n  cursor: pointer;\r\n  text-decoration: underline;\r\n}\r\n</style>\r\n"],"mappings":";AAgCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,IAAI;QACF,IAAI,IAAI,CAACL,KAAI,KAAM,iBAAgB,IAAK,IAAI,CAACC,QAAO,KAAM,OAAO,EAAE;UACjEK,KAAK,CAAC,gBAAgB,CAAC;UACvB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;UAC3B;QACF;QAEA,IAAI,CAAC,IAAI,CAACL,OAAM,IAAK,IAAI,CAACH,KAAI,KAAM,iBAAiB,EAAE;UACrD,IAAI,CAACE,KAAI,GAAI,mCAAmC;UAChD;QACF;QAEA,IAAI,IAAI,CAACC,OAAO,EAAE;UAChB,MAAMM,QAAO,GAAI,MAAMX,KAAK,CAACY,IAAI,CAAC,sCAAsC,EAAE;YACxEV,KAAK,EAAE,IAAI,CAACA,KAAK;YACjBC,QAAQ,EAAE,IAAI,CAACA;UACjB,CAAC,CAAC;UACFU,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEH,QAAQ,CAACV,IAAI,CAACc,KAAK,CAAC;UAClDP,KAAK,CAAC,mBAAmB,CAAC;UAC1B,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,YAAY,CAAC;QACjC,OAAO;UACL,MAAMV,KAAK,CAACY,IAAI,CAAC,yCAAyC,EAAE;YAC1DV,KAAK,EAAE,IAAI,CAACA,KAAK;YACjBC,QAAQ,EAAE,IAAI,CAACA;UACjB,CAAC,CAAC;UACFK,KAAK,CAAC,wCAAwC,CAAC;UAC/C,IAAI,CAACH,OAAM,GAAI,IAAI;QACrB;MACF,EAAE,OAAOW,GAAG,EAAE;QACZ,IAAI,CAACZ,KAAI,GAAIY,GAAG,CAACL,QAAQ,EAAEV,IAAI,EAAEgB,GAAE,IAAK,uBAAuB;MACjE;IACF,CAAC;IACDC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACb,OAAM,GAAI,CAAC,IAAI,CAACA,OAAO;MAC5B,IAAI,CAACD,KAAI,GAAI,EAAE;IACjB;EACF;A