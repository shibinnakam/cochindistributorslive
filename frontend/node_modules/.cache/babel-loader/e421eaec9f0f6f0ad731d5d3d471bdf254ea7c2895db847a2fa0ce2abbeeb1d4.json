{"ast":null,"code":"export default {\n  name: 'HomePage',\n  data() {\n    return {\n      isScrolled: false,\n      animate: false,\n      backgroundIndex: 0,\n      backgrounds: [require('@/assets/bg.jpg'), require('@/assets/login-bg.jpg')],\n      visibleProduct: [],\n      productImages: [require('@/assets/dorito.png'), require('@/assets/lays.jpg'), require('@/assets/kurkure.png'), require('@/assets/cheetos1.png')],\n      currentProductIndex: 0\n    };\n  },\n  computed: {\n    backgroundStyle() {\n      return {\n        backgroundImage: `url(${this.backgrounds[this.backgroundIndex]})`,\n        backgroundSize: 'cover',\n        backgroundPosition: 'center',\n        height: '100vh',\n        position: 'relative'\n      };\n    }\n  },\n  mounted() {\n    window.addEventListener('scroll', this.handleScroll);\n\n    // Animate content\n    setTimeout(() => {\n      this.animate = true;\n    }, 100);\n\n    // Background slideshow\n    this.bgInterval = setInterval(() => {\n      this.backgroundIndex = (this.backgroundIndex + 1) % this.backgrounds.length;\n    }, 5000);\n\n    // Product slideshow\n    this.visibleProduct = [this.productImages[0]];\n    this.productInterval = setInterval(() => {\n      this.currentProductIndex = (this.currentProductIndex + 1) % this.productImages.length;\n      this.visibleProduct = [this.productImages[this.currentProductIndex]];\n    }, 3000);\n  },\n  beforeUnmount() {\n    window.removeEventListener('scroll', this.handleScroll);\n    clearInterval(this.bgInterval);\n    clearInterval(this.productInterval);\n  },\n  methods: {\n    handleScroll() {\n      this.isScrolled = window.scrollY > 50;\n    }\n  }\n};","map":{"version":3,"names":["name","data","isScrolled","animate","backgroundIndex","backgrounds","require","visibleProduct","productImages","currentProductIndex","computed","backgroundStyle","backgroundImage","backgroundSize","backgroundPosition","height","position","mounted","window","addEventListener","handleScroll","setTimeout","bgInterval","setInterval","length","productInterval","beforeUnmount","removeEventListener","clearInterval","methods","scrollY"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\Home.vue"],"sourcesContent":["<template>\r\n  <div class=\"home-page\" :style=\"backgroundStyle\">\r\n    <!-- Navbar -->\r\n    <header :class=\"{ scrolled: isScrolled }\">\r\n      <nav class=\"navbar\">\r\n        <div class=\"logo-area\">\r\n          <img src=\"@/assets/logo.png\" alt=\"Logo\" />\r\n        </div>\r\n        <ul class=\"nav-links\">\r\n          <li><a href=\"#\">Home</a></li>\r\n          <li><a href=\"#\">Products</a></li>\r\n          <li><a href=\"#\">About</a></li>\r\n          <li><a href=\"#\">Contact</a></li>\r\n        </ul>\r\n      </nav>\r\n    </header>\r\n\r\n    <!-- Hero Section -->\r\n    <section class=\"hero\">\r\n      <div class=\"hero-content\">\r\n        <h1 class=\"title\">Welcome to Cochin Distributors</h1>\r\n        <p class=\"description\">Delivering quality products to your doorstep.</p>\r\n      </div>\r\n\r\n      <!-- Product Animation Section -->\r\n      <div class=\"product-carousel-overlay\">\r\n        <transition-group name=\"product-slide\" tag=\"div\" class=\"carousel-track\">\r\n          <img\r\n            v-for=\"(image, index) in visibleProduct\"\r\n            :key=\"index\"\r\n            :src=\"image\"\r\n            class=\"carousel-image\"\r\n          />\r\n        </transition-group>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'HomePage',\r\n  data() {\r\n    return {\r\n      isScrolled: false,\r\n      animate: false,\r\n      backgroundIndex: 0,\r\n      backgrounds: [\r\n        require('@/assets/bg.jpg'),\r\n        require('@/assets/login-bg.jpg'),\r\n      ],\r\n      visibleProduct: [],\r\n      productImages: [\r\n        require('@/assets/dorito.png'),\r\n        require('@/assets/lays.jpg'),\r\n        require('@/assets/kurkure.png'),\r\n        require('@/assets/cheetos1.png'),\r\n      ],\r\n      currentProductIndex: 0,\r\n    };\r\n  },\r\n  computed: {\r\n    backgroundStyle() {\r\n      return {\r\n        backgroundImage: `url(${this.backgrounds[this.backgroundIndex]})`,\r\n        backgroundSize: 'cover',\r\n        backgroundPosition: 'center',\r\n        height: '100vh',\r\n        position: 'relative',\r\n      };\r\n    },\r\n  },\r\n  mounted() {\r\n    window.addEventListener('scroll', this.handleScroll);\r\n\r\n    // Animate content\r\n    setTimeout(() => {\r\n      this.animate = true;\r\n    }, 100);\r\n\r\n    // Background slideshow\r\n    this.bgInterval = setInterval(() => {\r\n      this.backgroundIndex = (this.backgroundIndex + 1) % this.backgrounds.length;\r\n    }, 5000);\r\n\r\n    // Product slideshow\r\n    this.visibleProduct = [this.productImages[0]];\r\n    this.productInterval = setInterval(() => {\r\n      this.currentProductIndex = (this.currentProductIndex + 1) % this.productImages.length;\r\n      this.visibleProduct = [this.productImages[this.currentProductIndex]];\r\n    }, 3000);\r\n  },\r\n  beforeUnmount() {\r\n    window.removeEventListener('scroll', this.handleScroll);\r\n    clearInterval(this.bgInterval);\r\n    clearInterval(this.productInterval);\r\n  },\r\n  methods: {\r\n    handleScroll() {\r\n      this.isScrolled = window.scrollY > 50;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.home-page {\r\n  font-family: 'Arial', sans-serif;\r\n  color: #fff;\r\n  overflow: hidden;\r\n  position: relative;\r\n}\r\n\r\n/* Navbar */\r\n.navbar {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  padding: 1rem 2rem;\r\n  position: fixed;\r\n  width: 100%;\r\n  z-index: 1000;\r\n  transition: background 0.3s ease;\r\n}\r\n\r\n.navbar img {\r\n  height: 40px;\r\n}\r\n\r\n.nav-links {\r\n  display: flex;\r\n  gap: 2rem;\r\n  list-style: none;\r\n}\r\n\r\n.nav-links li a {\r\n  color: #fff;\r\n  text-decoration: none;\r\n  font-weight: bold;\r\n}\r\n\r\nheader.scrolled {\r\n  background: rgba(0, 0, 0, 0.7);\r\n}\r\n\r\n/* Hero */\r\n.hero {\r\n  height: 100vh;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  text-align: center;\r\n  position: relative;\r\n  z-index: 1;\r\n}\r\n\r\n.title {\r\n  font-size: 3rem;\r\n  font-weight: bold;\r\n  margin-bottom: 1rem;\r\n  animation: fadeInDown 1s ease;\r\n}\r\n\r\n.description {\r\n  font-size: 1.5rem;\r\n  animation: fadeInUp 1s ease;\r\n}\r\n\r\n/* Product Animation */\r\n.product-carousel-overlay {\r\n  position: absolute;\r\n  bottom: 10%;\r\n  display: flex;\r\n  justify-content: center;\r\n  width: 100%;\r\n  pointer-events: none;\r\n}\r\n\r\n.carousel-track {\r\n  display: flex;\r\n  gap: 1rem;\r\n  justify-content: center;\r\n}\r\n\r\n.carousel-image {\r\n  width: 120px;\r\n  height: 120px;\r\n  object-fit: contain;\r\n  animation: zoomInOut 3s ease-in-out infinite;\r\n  transition: transform 0.5s ease;\r\n}\r\n\r\n@keyframes zoomInOut {\r\n  0% {\r\n    transform: scale(1);\r\n    opacity: 0;\r\n  }\r\n  50% {\r\n    transform: scale(1.2);\r\n    opacity: 1;\r\n  }\r\n  100% {\r\n    transform: scale(1);\r\n    opacity: 0;\r\n  }\r\n}\r\n\r\n/* Animations */\r\n@keyframes fadeInDown {\r\n  from {\r\n    opacity: 0;\r\n    transform: translateY(-30px);\r\n  }\r\n  to {\r\n    opacity: 1;\r\n    transform: translateY(0);\r\n  }\r\n}\r\n\r\n@keyframes fadeInUp {\r\n  from {\r\n    opacity: 0;\r\n    transform: translateY(30px);\r\n  }\r\n  to {\r\n    opacity: 1;\r\n    transform: translateY(0);\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AAwCA,eAAe;EACbA,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,KAAK;MACjBC,OAAO,EAAE,KAAK;MACdC,eAAe,EAAE,CAAC;MAClBC,WAAW,EAAE,CACXC,OAAO,CAAC,iBAAiB,CAAC,EAC1BA,OAAO,CAAC,uBAAuB,CAAC,CACjC;MACDC,cAAc,EAAE,EAAE;MAClBC,aAAa,EAAE,CACbF,OAAO,CAAC,qBAAqB,CAAC,EAC9BA,OAAO,