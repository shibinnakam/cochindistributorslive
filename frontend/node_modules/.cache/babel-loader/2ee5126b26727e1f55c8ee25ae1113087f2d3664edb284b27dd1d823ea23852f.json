{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport router from '@/router';\nexport default {\n  name: \"LoginPage\",\n  data() {\n    return {\n      isSignup: false,\n      email: \"\",\n      password: \"\",\n      isFilled: false\n    };\n  },\n  methods: {\n    toggleMode() {\n      this.isSignup = !this.isSignup;\n      this.email = \"\";\n      this.password = \"\";\n      this.isFilled = false;\n    },\n    checkFilled() {\n      this.isFilled = this.email.trim() !== \"\" && this.password.trim() !== \"\";\n    },\n    async handleSubmit() {\n      const endpoint = this.isSignup ? '/api/auth/register' : '/api/auth/login';\n      try {\n        const response = await axios.post(endpoint, {\n          email: this.email,\n          password: this.password\n        });\n        alert(this.isSignup ? \"Signup successful\" : \"Login successful\");\n        if (!this.isSignup) {\n          // Navigate to admin dashboard if admin\n          if (this.email === 'admin@gmail.com' && this.password === 'admin') {\n            router.push('/admin');\n          } else {\n            router.push('/');\n          }\n        } else {\n          this.toggleMode(); // Switch to login mode after successful signup\n        }\n      } catch (err) {\n        alert(err.response?.data?.msg || \"An error occurred.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","router","name","data","isSignup","email","password","isFilled","methods","toggleMode","checkFilled","trim","handleSubmit","endpoint","response","post","alert","push","err","msg"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"login-container\">\r\n    <!-- Left: Login or Signup Form -->\r\n    <div class=\"login-form-section\">\r\n      <div :class=\"['login-box', isFilled ? 'box-filled' : '']\">\r\n        <img src=\"@/assets/logo.jpeg\" alt=\"Logo\" class=\"logo\" />\r\n        <h2 class=\"form-title\">{{ isSignup ? 'Sign Up' : 'Login' }}</h2>\r\n        <p class=\"signup-link\">\r\n          {{ isSignup ? 'Already have an account?' : 'Don’t have an account?' }}\r\n          <a href=\"#\" @click.prevent=\"toggleMode\">{{ isSignup ? 'Login' : 'Create an account' }}</a>\r\n        </p>\r\n\r\n        <form class=\"login-form\" @submit.prevent=\"handleSubmit\" @input=\"checkFilled\">\r\n          <label for=\"email\">Email</label>\r\n          <input type=\"text\" id=\"email\" v-model=\"email\" placeholder=\"Enter email\" />\r\n\r\n          <label for=\"password\">Password</label>\r\n          <input type=\"password\" id=\"password\" v-model=\"password\" placeholder=\"Enter password\" />\r\n\r\n          <a v-if=\"!isSignup\" href=\"#\" class=\"forgot-password\">Forgot your password?</a>\r\n\r\n          <button type=\"submit\" class=\"login-button\">\r\n            {{ isSignup ? 'Sign Up' : 'Login' }}\r\n          </button>\r\n        </form>\r\n\r\n        <img src=\"@/assets/delivery-sticker.png\" alt=\"Delivery Sticker\" class=\"delivery-sticker\" />\r\n\r\n        <footer class=\"form-footer\">\r\n          <small>© 2025 YourCompany. All rights reserved.</small>\r\n        </footer>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Right: Info Section -->\r\n    <div class=\"info-section\">\r\n      <img src=\"@/assets/login-bg.jpg\" alt=\"Background\" class=\"background-image\" />\r\n      <div class=\"paper-card\">\r\n        <h2>Spin Your Way to Higher Conversions Today!</h2>\r\n        <p>Instantly grow conversions, boost email subscribers and reduce cart abandonment with our fun Coupon Wheels.</p>\r\n        <p>Everyone loves to win something! OptinMonster’s Coupon Wheel campaigns get visitors excited about the chance to win a prize.</p>\r\n        <p><em>Coupon Wheels are available with any OptinMonster Growth subscription.</em></p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport router from '@/router';\r\n\r\nexport default {\r\n  name: \"LoginPage\",\r\n  data() {\r\n    return {\r\n      isSignup: false,\r\n      email: \"\",\r\n      password: \"\",\r\n      isFilled: false,\r\n    };\r\n  },\r\n  methods: {\r\n    toggleMode() {\r\n      this.isSignup = !this.isSignup;\r\n      this.email = \"\";\r\n      this.password = \"\";\r\n      this.isFilled = false;\r\n    },\r\n    checkFilled() {\r\n      this.isFilled = this.email.trim() !== \"\" && this.password.trim() !== \"\";\r\n    },\r\n    async handleSubmit() {\r\n      const endpoint = this.isSignup ? '/api/auth/register' : '/api/auth/login';\r\n\r\n      try {\r\n        const response = await axios.post(endpoint, {\r\n          email: this.email,\r\n          password: this.password,\r\n        });\r\n\r\n        alert(this.isSignup ? \"Signup successful\" : \"Login successful\");\r\n\r\n        if (!this.isSignup) {\r\n          // Navigate to admin dashboard if admin\r\n          if (this.email === 'admin@gmail.com' && this.password === 'admin') {\r\n            router.push('/admin');\r\n          } else {\r\n            router.push('/');\r\n          }\r\n        } else {\r\n          this.toggleMode(); // Switch to login mode after successful signup\r\n        }\r\n      } catch (err) {\r\n        alert(err.response?.data?.msg || \"An error occurred.\");\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Your existing styles here */\r\n</style>\r\n"],"mappings":";AAgDA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,UAAU;AAE7B,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,KAAK;MACfC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACL,QAAO,GAAI,CAAC,IAAI,CAACA,QAAQ;MAC9B,IAAI,CAACC,KAAI,GAAI,EAAE;MACf,IAAI,CAACC,QAAO,GAAI,EAAE;MAClB,IAAI,CAACC,QAAO,GAAI,KAAK;IACvB,CAAC;IACDG,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACH,QAAO,GAAI,IAAI,CAACF,K