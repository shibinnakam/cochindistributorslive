{"ast":null,"code":"export default {\n  name: 'HomePage',\n  data() {\n    return {\n      isScrolled: false,\n      animate: false,\n      backgroundIndex: 0,\n      currentProductIndex: 0,\n      backgrounds: [require('@/assets/bg.jpg'), require('@/assets/login-bg.jpg')],\n      productImages: [require('@/assets/doritos.png'), require('@/assets/kurkure.png'), require('@/assets/lays.jpg'), require('@/assets/cheetos1.png')]\n    };\n  },\n  computed: {\n    backgroundStyle() {\n      return {\n        backgroundImage: `url(${this.backgrounds[this.backgroundIndex]})`\n      };\n    }\n  },\n  mounted() {\n    window.addEventListener('scroll', this.handleScroll);\n    setTimeout(() => {\n      this.animate = true;\n    }, 100);\n    this.bgInterval = setInterval(() => {\n      this.backgroundIndex = (this.backgroundIndex + 1) % this.backgrounds.length;\n    }, 5000);\n    this.productInterval = setInterval(() => {\n      this.currentProductIndex = (this.currentProductIndex + 1) % this.productImages.length;\n    }, 3000);\n  },\n  beforeUnmount() {\n    window.removeEventListener('scroll', this.handleScroll);\n    clearInterval(this.bgInterval);\n    clearInterval(this.productInterval);\n  },\n  methods: {\n    handleScroll() {\n      this.isScrolled = window.scrollY > 50;\n    },\n    getProductClass(index) {\n      const total = this.productImages.length;\n      const prevIndex = (this.currentProductIndex - 1 + total) % total;\n      const nextIndex = (this.currentProductIndex + 1) % total;\n      if (index === this.currentProductIndex) return 'center';\n      if (index === prevIndex) return 'left';\n      if (index === nextIndex) return 'right';\n      return 'hidden';\n    }\n  }\n};","map":{"version":3,"names":["name","data","isScrolled","animate","backgroundIndex","currentProductIndex","backgrounds","require","productImages","computed","backgroundStyle","backgroundImage","mounted","window","addEventListener","handleScroll","setTimeout","bgInterval","setInterval","length","productInterval","beforeUnmount","removeEventListener","clearInterval","methods","scrollY","getProductClass","index","total","prevIndex","nextIndex"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\Home.vue"],"sourcesContent":["<template>\r\n  <div class=\"home-page\" :style=\"backgroundStyle\">\r\n    <!-- Navbar and Hero -->\r\n    <header :class=\"{ scrolled: isScrolled }\">\r\n      <!-- ...existing navbar code... -->\r\n    </header>\r\n\r\n    <!-- Hero Section -->\r\n    <section class=\"hero\">\r\n      <h1>Welcome to My Website</h1>\r\n      <p>Explore our products and services</p>\r\n    </section>\r\n\r\n    <!-- Cochin Distributors Section -->\r\n    <section class=\"cochin-section\">\r\n      <div class=\"cochin-container\">\r\n        <!-- Left: Image -->\r\n        <div class=\"cochin-images\" :class=\"{ 'slide-left': animate }\">\r\n          <img src=\"@/assets/bg.jpg\" class=\"img-large\" alt=\"Main\" />\r\n          <div class=\"img-small-column\">\r\n            <img src=\"@/assets/login-bg.jpg\" class=\"img-small\" alt=\"Login\" />\r\n            <img src=\"@/assets/logo.png\" class=\"img-small\" alt=\"Logo\" />\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Right: Text -->\r\n        <div class=\"cochin-content\" :class=\"{ 'slide-right': animate }\">\r\n          <h3 class=\"cochinhead\">\r\n            <span class=\"welcome-red\">WELCOME TO</span><br />\r\n            <strong>Cochin Distributors</strong>\r\n          </h3>\r\n          <p class=\"cochintext\">\r\n            At Cochin Distributors, we believe in delivering the finest quality baking products...\r\n          </p>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- Product Animation Section -->\r\n    <section class=\"product-carousel\">\r\n      <div class=\"product-wrapper\">\r\n        <div\r\n          v-for=\"(image, index) in productImages\"\r\n          :key=\"index\"\r\n          :class=\"['product-image', getProductClass(index)]\"\r\n        >\r\n          <img :src=\"image\" alt=\"Product\" />\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'HomePage',\r\n  data() {\r\n    return {\r\n      isScrolled: false,\r\n      animate: false,\r\n      backgroundIndex: 0,\r\n      currentProductIndex: 0,\r\n      backgrounds: [\r\n        require('@/assets/bg.jpg'),\r\n        require('@/assets/login-bg.jpg')\r\n      ],\r\n      productImages: [\r\n        require('@/assets/doritos.png'),\r\n        require('@/assets/kurkure.png'),\r\n        require('@/assets/lays.jpg'),\r\n        require('@/assets/cheetos1.png')\r\n      ]\r\n    };\r\n  },\r\n  computed: {\r\n    backgroundStyle() {\r\n      return {\r\n        backgroundImage: `url(${this.backgrounds[this.backgroundIndex]})`\r\n      };\r\n    }\r\n  },\r\n  mounted() {\r\n    window.addEventListener('scroll', this.handleScroll);\r\n    setTimeout(() => {\r\n      this.animate = true;\r\n    }, 100);\r\n\r\n    this.bgInterval = setInterval(() => {\r\n      this.backgroundIndex = (this.backgroundIndex + 1) % this.backgrounds.length;\r\n    }, 5000);\r\n\r\n    this.productInterval = setInterval(() => {\r\n      this.currentProductIndex = (this.currentProductIndex + 1) % this.productImages.length;\r\n    }, 3000);\r\n  },\r\n  beforeUnmount() {\r\n    window.removeEventListener('scroll', this.handleScroll);\r\n    clearInterval(this.bgInterval);\r\n    clearInterval(this.productInterval);\r\n  },\r\n  methods: {\r\n    handleScroll() {\r\n      this.isScrolled = window.scrollY > 50;\r\n    },\r\n    getProductClass(index) {\r\n      const total = this.productImages.length;\r\n      const prevIndex = (this.currentProductIndex - 1 + total) % total;\r\n      const nextIndex = (this.currentProductIndex + 1) % total;\r\n\r\n      if (index === this.currentProductIndex) return 'center';\r\n      if (index === prevIndex) return 'left';\r\n      if (index === nextIndex) return 'right';\r\n      return 'hidden';\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.home-page {\r\n  min-height: 100vh;\r\n  background-size: cover;\r\n  background-position: center;\r\n  background-repeat: no-repeat;\r\n  overflow-x: hidden;\r\n  transition: background-image 1.2s ease-in-out;\r\n}\r\n\r\n/* Navbar */\r\nheader {\r\n  position: fixed;\r\n  width: 100%;\r\n  top: 0;\r\n  z-index: 10;\r\n  transition: background-color 0.3s ease;\r\n  padding: 0.8rem 2rem;\r\n}\r\nheader.scrolled {\r\n  background-color: white;\r\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n/* Hero */\r\n.hero {\r\n  height: 100vh;\r\n  color: white;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  padding-top: 4rem;\r\n  text-shadow: 1px 1px 5px rgba(0, 0, 0, 0.6);\r\n}\r\n\r\n/* Cochin Section */\r\n.cochin-section {\r\n  padding: 2rem;\r\n  background-color: #fff;\r\n}\r\n.cochin-container {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  gap: 2rem;\r\n  align-items: flex-start;\r\n  justify-content: center;\r\n}\r\n.cochin-images {\r\n  display: flex;\r\n  gap: 1rem;\r\n  opacity: 0;\r\n  transform: translateX(-100%);\r\n  transition: all 1s ease;\r\n}\r\n.cochin-content {\r\n  max-width: 600px;\r\n  opacity: 0;\r\n  transform: translateX(100%);\r\n  transition: all 1s ease;\r\n}\r\n.cochinhead {\r\n  margin: 0;\r\n  font-size: 1.8rem;\r\n  line-height: 1.4;\r\n}\r\n.welcome-red {\r\n  color: red;\r\n  font-weight: bold;\r\n}\r\n.cochintext {\r\n  font-size: 1rem;\r\n  line-height: 1.6;\r\n  color: #333;\r\n  text-align: justify;\r\n}\r\n.img-large {\r\n  width: 220px;\r\n  height: 350px;\r\n  object-fit: cover;\r\n  border-radius: 8px;\r\n}\r\n.img-small-column {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 1rem;\r\n}\r\n.img-small {\r\n  width: 220px;\r\n  height: 165px;\r\n  object-fit: contain;\r\n  background-color: #f5f5f5;\r\n  border-radius: 8px;\r\n}\r\n\r\n/* Slide-in Animation */\r\n.slide-left {\r\n  transform: translateX(0);\r\n  opacity: 1;\r\n}\r\n.slide-right {\r\n  transform: translateX(0);\r\n  opacity: 1;\r\n}\r\n\r\n/* Product Carousel */\r\n.product-carousel {\r\n  height: 300px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  overflow: hidden;\r\n  margin-top: 2rem;\r\n  position: relative;\r\n}\r\n.product-wrapper {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  position: relative;\r\n  width: 100%;\r\n  max-width: 800px;\r\n  height: 100%;\r\n}\r\n.product-image {\r\n  position: absolute;\r\n  transition: all 1s ease;\r\n  opacity: 0;\r\n  transform: scale(0.5);\r\n}\r\n.product-image img {\r\n  max-height: 250px;\r\n  max-width: 300px;\r\n  object-fit: contain;\r\n}\r\n\r\n/* Animations based on position */\r\n.product-image.center {\r\n  opacity: 1;\r\n  transform: scale(1.2) translateX(0);\r\n  z-index: 3;\r\n}\r\n.product-image.left {\r\n  opacity: 0.5;\r\n  transform: scale(0.8) translateX(-150%);\r\n  z-index: 2;\r\n}\r\n.product-image.right {\r\n  opacity: 0.5;\r\n  transform: scale(0.8) translateX(150%);\r\n  z-index: 2;\r\n}\r\n.product-image.hidden {\r\n  opacity: 0;\r\n  transform: scale(0.5) translateX(1000%);\r\n  z-index: 1;\r\n}\r\n</style>\r\n"],"mappings":"AAsDA,eAAe;EACbA,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,KAAK;MACjBC,OAAO,EAAE,KAAK;MACdC,eAAe,EAAE,CAAC;MAClBC,mBAAmB,EAAE,CAAC;MACtBC,WAAW,EAAE,CACXC,OAAO,CAAC,iBAAiB,CAAC,EAC1BA,OAAO,CAAC,uBAAuB,EAChC;MACDC,aAAa,EAAE,CACbD,OAAO,CAAC,sBAAsB,CAAC,EAC/BA,OAAO,CAAC,sBAAsB,CAAC,EAC/BA,OAAO,CAAC,mBAAmB,CAAC,EAC5BA,OAAO,CAAC,uBAAuB;IAEnC,CAAC;EACH,CAAC;EACDE,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,OAAO;QACLC,eAAe,EAAE,OAAO,IAAI,CAACL,WAAW,CAAC,IAAI,CAACF,eAAe,CAAC;MAChE,CAAC;IACH;EACF,CAAC;EACDQ,OAAOA,CAAA,EAAG;IACRC,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAE,IAAI,CAACC,YAAY,CAAC;IACpDC,UAAU,CAAC,MAAM;MACf,IAAI,CAACb,OAAM,GAAI,IAAI;IACrB,CAAC