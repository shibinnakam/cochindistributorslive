{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'LoginPage',\n  data() {\n    return {\n      email: '',\n      password: '',\n      isSignup: false,\n      error: ''\n    };\n  },\n  methods: {\n    async handleSubmit() {\n      this.error = '';\n      try {\n        const endpoint = this.isSignup ? '/api/auth/register' : '/api/auth/login';\n        const {\n          data\n        } = await axios.post(endpoint, {\n          email: this.email,\n          password: this.password\n        });\n        if (!this.isSignup) {\n          // Store token or user info\n          localStorage.setItem('token', data.token);\n          alert('Login successful!');\n          this.$router.push('/admin'); // Redirect after login\n        } else {\n          alert('Account created! Please login.');\n          this.isSignup = false;\n        }\n      } catch (err) {\n        this.error = err.response?.data?.msg || 'Something went wrong';\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","isSignup","error","methods","handleSubmit","endpoint","post","localStorage","setItem","token","alert","$router","push","err","response","msg"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"login-container\">\r\n    <div class=\"login-form\">\r\n      <h2>{{ isSignup ? 'Create an Account' : 'Login to Your Account' }}</h2>\r\n      <form @submit.prevent=\"handleSubmit\">\r\n        <input type=\"email\" v-model=\"email\" placeholder=\"Email\" required />\r\n        <input type=\"password\" v-model=\"password\" placeholder=\"Password\" required />\r\n        <button type=\"submit\">\r\n          {{ isSignup ? 'Sign Up' : 'Login' }}\r\n        </button>\r\n      </form>\r\n      <p class=\"toggle-text\">\r\n        {{ isSignup ? 'Already have an account?' : 'Donâ€™t have an account?' }}\r\n        <a @click=\"isSignup = !isSignup\">\r\n          {{ isSignup ? 'Login' : 'Create Account' }}\r\n        </a>\r\n      </p>\r\n      <p v-if=\"error\" class=\"error-msg\">{{ error }}</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'LoginPage',\r\n  data() {\r\n    return {\r\n      email: '',\r\n      password: '',\r\n      isSignup: false,\r\n      error: '',\r\n    };\r\n  },\r\n  methods: {\r\n    async handleSubmit() {\r\n      this.error = '';\r\n      try {\r\n        const endpoint = this.isSignup ? '/api/auth/register' : '/api/auth/login';\r\n        const { data } = await axios.post(endpoint, {\r\n          email: this.email,\r\n          password: this.password,\r\n        });\r\n\r\n        if (!this.isSignup) {\r\n          // Store token or user info\r\n          localStorage.setItem('token', data.token);\r\n          alert('Login successful!');\r\n          this.$router.push('/admin'); // Redirect after login\r\n        } else {\r\n          alert('Account created! Please login.');\r\n          this.isSignup = false;\r\n        }\r\n      } catch (err) {\r\n        this.error = err.response?.data?.msg || 'Something went wrong';\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.login-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  height: 100vh;\r\n  background: url('@/assets/login-bg.jpg') no-repeat center center;\r\n  background-size: cover;\r\n}\r\n\r\n.login-form {\r\n  background: white;\r\n  padding: 30px;\r\n  border-radius: 12px;\r\n  box-shadow: 0 0 20px rgba(0, 0, 0, 0.2);\r\n  width: 300px;\r\n  text-align: center;\r\n}\r\n\r\n.login-form input {\r\n  width: 100%;\r\n  padding: 10px;\r\n  margin: 10px 0;\r\n  border: 1px solid #aaa;\r\n  border-radius: 6px;\r\n}\r\n\r\n.login-form button {\r\n  background-color: #0044cc;\r\n  color: white;\r\n  border: none;\r\n  width: 100%;\r\n  padding: 10px;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n}\r\n\r\n.login-form .toggle-text {\r\n  margin-top: 10px;\r\n}\r\n\r\n.login-form .toggle-text a {\r\n  color: #0044cc;\r\n  cursor: pointer;\r\n}\r\n\r\n.error-msg {\r\n  color: red;\r\n  margin-top: 10px;\r\n}\r\n</style>\r\n"],"mappings":";AAuBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,KAAK;MACfC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,IAAI,CAACF,KAAI,GAAI,EAAE;MACf,IAAI;QACF,MAAMG,QAAO,GAAI,IAAI,CAACJ,QAAO,GAAI,oBAAmB,GAAI,iBAAiB;QACzE,MAAM;UAAEH;QAAK,IAAI,MAAMF,KAAK,CAACU,IAAI,CAACD,QAAQ,EAAE;UAC1CN,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QAEF,IAAI,CAAC,IAAI,CAACC,QAAQ,EAAE;UAClB;UACAM,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEV,IAAI,CAACW,KAAK,CAAC;UACzCC,KAAK,CAAC,mBAAmB,CAAC;UAC1B,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC,EAAE;QAC/B,OAAO;UACLF,KAAK,CAAC,gCAAgC,CAAC;UACvC,IAAI,CAACT,QAAO,GAAI,KAAK;QACvB;MACF,EAAE,OAAOY,GAAG,EAAE;QACZ,IAAI,CAACX,KAAI,GAAIW,GAAG,CAACC,QAAQ,EAAEhB,IAAI,EAAEiB,GAAE,IAAK,sBAAsB;MAChE;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}