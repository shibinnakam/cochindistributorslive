{"ast":null,"code":"export default {\n  name: 'HomePage',\n  data() {\n    return {\n      isScrolled: false,\n      animate: false,\n      backgroundIndex: 0,\n      currentProductIndex: 0,\n      backgrounds: [require('@/assets/bg.jpg'), require('@/assets/login-bg.jpg')],\n      productImages: [require('@/assets/doritos.png'), require('@/assets/kurkure.png'), require('@/assets/lays.jpg'), require('@/assets/cheetos1.png')]\n    };\n  },\n  computed: {\n    backgroundStyle() {\n      return {\n        backgroundImage: `url(${this.backgrounds[this.backgroundIndex]})`\n      };\n    }\n  },\n  mounted() {\n    window.addEventListener('scroll', this.handleScroll);\n    setTimeout(() => {\n      this.animate = true;\n    }, 100);\n\n    // Background slide every 5 seconds\n    this.bgInterval = setInterval(() => {\n      this.backgroundIndex = (this.backgroundIndex + 1) % this.backgrounds.length;\n    }, 5000);\n\n    // Product slide every 4 seconds\n    this.productInterval = setInterval(() => {\n      this.currentProductIndex = (this.currentProductIndex + 1) % this.productImages.length;\n    }, 4000);\n  },\n  beforeUnmount() {\n    window.removeEventListener('scroll', this.handleScroll);\n    clearInterval(this.bgInterval);\n    clearInterval(this.productInterval);\n  },\n  methods: {\n    handleScroll() {\n      this.isScrolled = window.scrollY > 50;\n    }\n  }\n};","map":{"version":3,"names":["name","data","isScrolled","animate","backgroundIndex","currentProductIndex","backgrounds","require","productImages","computed","backgroundStyle","backgroundImage","mounted","window","addEventListener","handleScroll","setTimeout","bgInterval","setInterval","length","productInterval","beforeUnmount","removeEventListener","clearInterval","methods","scrollY"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\Home.vue"],"sourcesContent":["<template>\r\n  <div class=\"home-page\" :style=\"backgroundStyle\">\r\n    <!-- Navbar and Hero -->\r\n    <header :class=\"{ scrolled: isScrolled }\">\r\n      <nav class=\"navbar\">\r\n        <div class=\"logo-area\">\r\n          <img src=\"@/assets/logo.png\" alt=\"Logo\" class=\"site-logo\" />\r\n          <span class=\"site-name\">MyWebsite</span>\r\n        </div>\r\n        <ul class=\"nav-links\">\r\n          <li><router-link to=\"/\" exact>Home</router-link></li>\r\n          <li><router-link to=\"/about\">About</router-link></li>\r\n          <li><a href=\"#products\">Products</a></li>\r\n          <li><a href=\"#contact\">Contact</a></li>\r\n          <li><router-link to=\"/login\" class=\"login-link\">Login</router-link></li>\r\n        </ul>\r\n      </nav>\r\n    </header>\r\n\r\n    <!-- Hero Section -->\r\n    <section class=\"hero\">\r\n      <h1>Welcome to My Website</h1>\r\n      <p>Explore our products and services</p>\r\n    </section>\r\n\r\n    <!-- Cochin Distributors Section -->\r\n    <section class=\"cochin-section\">\r\n      <div class=\"cochin-container\">\r\n        <!-- Left: Image -->\r\n        <div class=\"cochin-images\" :class=\"{ 'slide-left': animate }\">\r\n          <img src=\"@/assets/bg.jpg\" class=\"img-large\" alt=\"Main\" />\r\n          <div class=\"img-small-column\">\r\n            <img src=\"@/assets/login-bg.jpg\" class=\"img-small\" alt=\"Login\" />\r\n            <img src=\"@/assets/logo.png\" class=\"img-small\" alt=\"Logo\" />\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Right: Text -->\r\n        <div class=\"cochin-content\" :class=\"{ 'slide-right': animate }\">\r\n          <h3 class=\"cochinhead\">\r\n            <span class=\"welcome-red\">WELCOME TO</span><br />\r\n            <strong>Cochin Distributors</strong>\r\n          </h3>\r\n          <p class=\"cochintext\">\r\n            At Cochin Distributors, we believe in delivering the finest quality baking products to every kitchen, whether you're a home baker or a commercial chef.\r\n            Our selection includes premium flours, high-quality baking agents, and essential tools that make your baking easier and more delicious.\r\n            With years of trusted service and a commitment to excellence, Cochin Distributors is your reliable partner in every recipe.\r\n          </p>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- Product Animation Section -->\r\n    <section class=\"product-carousel\">\r\n      <transition-group name=\"product-slide\" tag=\"div\" class=\"carousel-track\">\r\n        <img\r\n          v-for=\"(image, index) in visibleProduct\"\r\n          :key=\"index\"\r\n          :src=\"image\"\r\n          class=\"carousel-image\"\r\n/>\r\n      </transition-group>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'HomePage',\r\n  data() {\r\n    return {\r\n      isScrolled: false,\r\n      animate: false,\r\n      backgroundIndex: 0,\r\n      currentProductIndex: 0,\r\n      backgrounds: [\r\n        require('@/assets/bg.jpg'),\r\n        require('@/assets/login-bg.jpg')\r\n      ],\r\n      productImages: [\r\n        require('@/assets/doritos.png'),\r\n        require('@/assets/kurkure.png'),\r\n        require('@/assets/lays.jpg'),\r\n        require('@/assets/cheetos1.png')\r\n      ]\r\n    };\r\n  },\r\n  computed: {\r\n    backgroundStyle() {\r\n      return {\r\n        backgroundImage: `url(${this.backgrounds[this.backgroundIndex]})`\r\n      };\r\n    }\r\n  },\r\n  mounted() {\r\n    window.addEventListener('scroll', this.handleScroll);\r\n\r\n    setTimeout(() => {\r\n      this.animate = true;\r\n    }, 100);\r\n\r\n    // Background slide every 5 seconds\r\n    this.bgInterval = setInterval(() => {\r\n      this.backgroundIndex = (this.backgroundIndex + 1) % this.backgrounds.length;\r\n    }, 5000);\r\n\r\n    // Product slide every 4 seconds\r\n    this.productInterval = setInterval(() => {\r\n      this.currentProductIndex = (this.currentProductIndex + 1) % this.productImages.length;\r\n    }, 4000);\r\n  },\r\n  beforeUnmount() {\r\n    window.removeEventListener('scroll', this.handleScroll);\r\n    clearInterval(this.bgInterval);\r\n    clearInterval(this.productInterval);\r\n  },\r\n  methods: {\r\n    handleScroll() {\r\n      this.isScrolled = window.scrollY > 50;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Background transitions */\r\n.home-page {\r\n  min-height: 100vh;\r\n  background-size: cover;\r\n  background-position: center;\r\n  background-repeat: no-repeat;\r\n  overflow-x: hidden;\r\n  transition: background-image 1.2s ease-in-out;\r\n}\r\n\r\n/* Navbar styles */\r\nheader {\r\n  position: fixed;\r\n  width: 100%;\r\n  top: 0;\r\n  z-index: 10;\r\n  transition: background-color 0.3s ease;\r\n  padding: 0.8rem 2rem;\r\n}\r\nheader.scrolled {\r\n  background-color: white;\r\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\r\n}\r\n.navbar {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n.logo-area {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 0.5rem;\r\n}\r\n.site-logo {\r\n  height: 40px;\r\n  width: 40px;\r\n}\r\n.site-name {\r\n  font-weight: bold;\r\n  font-size: 1.5rem;\r\n  color: white;\r\n}\r\nheader.scrolled .site-name {\r\n  color: #333;\r\n}\r\n.nav-links {\r\n  list-style: none;\r\n  display: flex;\r\n  gap: 1.5rem;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n.nav-links a,\r\n.nav-links .router-link-exact-active,\r\n.nav-links .router-link-active {\r\n  text-decoration: none;\r\n  font-weight: 500;\r\n  color: white;\r\n  transition: color 0.3s ease;\r\n}\r\nheader.scrolled .nav-links a,\r\nheader.scrolled .nav-links .router-link-exact-active,\r\nheader.scrolled .nav-links .router-link-active {\r\n  color: #333;\r\n}\r\n.login-link {\r\n  padding: 6px 12px;\r\n  border: 1px solid white;\r\n  border-radius: 4px;\r\n  transition: all 0.3s ease;\r\n}\r\nheader.scrolled .login-link {\r\n  border-color: #333;\r\n}\r\n.login-link:hover {\r\n  background-color: #007bff;\r\n  color: white !important;\r\n  border-color: #007bff;\r\n}\r\n\r\n/* Hero Section */\r\n.hero {\r\n  height: 100vh;\r\n  color: white;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  padding-top: 4rem;\r\n  text-shadow: 1px 1px 5px rgba(0, 0, 0, 0.6);\r\n}\r\n\r\n/* Cochin Section */\r\n.cochin-section {\r\n  padding: 2rem;\r\n  background-color: #fff;\r\n}\r\n.cochin-container {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  gap: 2rem;\r\n  align-items: flex-start;\r\n  justify-content: center;\r\n}\r\n.cochin-images {\r\n  display: flex;\r\n  gap: 1rem;\r\n  opacity: 0;\r\n  transform: translateX(-100%);\r\n  transition: all 1s ease;\r\n}\r\n.cochin-content {\r\n  max-width: 600px;\r\n  opacity: 0;\r\n  transform: translateX(100%);\r\n  transition: all 1s ease;\r\n}\r\n.cochinhead {\r\n  margin: 0;\r\n  font-size: 1.8rem;\r\n  line-height: 1.4;\r\n}\r\n.welcome-red {\r\n  color: red;\r\n  font-weight: bold;\r\n}\r\n.cochintext {\r\n  font-size: 1rem;\r\n  line-height: 1.6;\r\n  color: #333;\r\n  text-align: justify; \r\n}\r\n.img-large {\r\n  width: 220px;\r\n  height: 350px;\r\n  object-fit: cover;\r\n  border-radius: 8px;\r\n}\r\n.img-small-column {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 1rem;\r\n}\r\n.img-small {\r\n  width: 220px;\r\n  height: 165px;\r\n  object-fit: contain;\r\n  background-color: #f5f5f5;\r\n  border-radius: 8px;\r\n}\r\n\r\n/* Slide-in animations */\r\n.slide-left {\r\n  transform: translateX(0);\r\n  opacity: 1;\r\n}\r\n.slide-right {\r\n  transform: translateX(0);\r\n  opacity: 1;\r\n}\r\n\r\n/* Product Carousel Styles */\r\n.product-carousel {\r\n  height: 300px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  overflow: hidden;\r\n  margin-top: 2rem;\r\n  position: relative;\r\n}\r\n.carousel-track {\r\n  position: relative;\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n.carousel-image {\r\n  position: absolute;\r\n  max-height: 250px;\r\n  max-width: 300px;\r\n  object-fit: contain;\r\n  opacity: 0;\r\n  transform: scale(0.5) translateX(100%);\r\n  transition: all 1s ease;\r\n  margin: auto;\r\n  left: 0;\r\n  right: 0;\r\n}\r\n\r\n/* Transition Effects */\r\n.product-slide-enter-active,\r\n.product-slide-leave-active {\r\n  transition: all 1s ease;\r\n}\r\n.product-slide-enter-from {\r\n  opacity: 0;\r\n  transform: scale(0.5) translateX(100%);\r\n}\r\n.product-slide-enter-to {\r\n  opacity: 1;\r\n  transform: scale(1.2) translateX(0);\r\n  z-index: 2;\r\n}\r\n.product-slide-leave-from {\r\n  opacity: 1;\r\n  transform: scale(1.2) translateX(0);\r\n}\r\n.product-slide-leave-to {\r\n  opacity: 0;\r\n  transform: scale(0.5) translateX(-100%);\r\n}\r\n</style>\r\n"],"mappings":"AAmEA,eAAe;EACbA,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,KAAK;MACjBC,OAAO,EAAE,KAAK;MACdC,eAAe,EAAE,CAAC;MAClBC,mBAAmB,EAAE,CAAC;MACtBC,WAAW,EAAE,CACXC,OAAO,CAAC,iBAAiB,CAAC,EAC1BA,OAAO,CAAC,uBAAuB,EAChC;MACDC,aAAa,EAAE,CACbD,OAAO,CAAC,sBAAsB,CAAC,EAC/BA,OAAO,CAAC,sBAAsB,CAAC,EAC/BA,OAAO,CAAC,mBAAmB,CAAC,EAC5BA,OAAO,CAAC,uBAAuB;IAEnC,CAAC;EACH,CAAC;EACDE,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,OAAO;QACLC,eAAe,EAAE,OAAO,IAAI,CAACL,WAAW,CAAC,IAAI,CAACF,eAAe,CAAC;MAChE,CAAC;IACH;EACF,CAAC;EACDQ,OAAOA,CAAA,EAAG;IACRC,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAE,IAAI,CAACC,YAAY,CAAC;IAEpDC,UAAU,CAAC,MAAM;MACf,IAAI,CAACb,OAAM,GAAI,IAAI;IACrB,CAAC,EAAE,GAAG,CAAC;;IAEP;IACA,IAAI,CAACc,UAAS,GAAIC,WAAW,CAAC,MAAM;MAClC,IAAI,CAACd,eAAc,GAAI,CAAC,IAAI,CAACA,eAAc,GAAI,CAAC,IAAI,IAAI,CAACE,WAAW,CAACa,MAAM;IAC7E,CAAC,EAAE,IAAI,CAAC;;IAER;IACA,IAAI,CAACC,eAAc,GAAIF,WAAW,CAAC,MAAM;MACvC,IAAI,CAACb,mBAAkB,GAAI,CAAC,IAAI,CAACA,mBAAkB,GAAI,CAAC,IAAI,IAAI,CAACG,aAAa,CAACW,MAAM;IACvF,CAAC,EAAE,IAAI,CAAC;EACV,CAAC;EACDE,aAAaA,CAAA,EAAG;IACdR,MAAM,CAACS,mBAAmB,CAAC,QAAQ,EAAE,IAAI,CAACP,YAAY,CAAC;IACvDQ,aAAa,CAAC,IAAI,CAACN,UAAU,CAAC;IAC9BM,aAAa,CAAC,IAAI,CAACH,eAAe,CAAC;EACrC,CAAC;EAC