{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"ProductEdit\",\n  data() {\n    return {\n      form: {\n        name: \"\",\n        description: \"\",\n        originalPrice: 0,\n        discountPrice: 0,\n        quantity: 0,\n        category: \"\",\n        image: \"\" // üëà keep old image for preview\n      },\n      imageFile: null,\n      categories: []\n    };\n  },\n  async mounted() {\n    await this.fetchCategories();\n    await this.fetchProduct();\n  },\n  methods: {\n    async fetchCategories() {\n      try {\n        const res = await axios.get(\"http://localhost:5000/api/category/getcategories\");\n        if (res.data.success) {\n          this.categories = res.data.categories;\n        }\n      } catch (err) {\n        console.error(\"Error fetching categories:\", err.response?.data || err.message);\n      }\n    },\n    async fetchProduct() {\n      const id = this.$route.params.id;\n      try {\n        const res = await axios.get(`http://localhost:5000/api/products/getproduct/${id}`);\n        if (res.data.success) {\n          const product = res.data.product;\n          this.form = {\n            ...product,\n            // ‚úÖ handle both cases: object or string\n            category: product.category?._id || product.category,\n            image: product.image || \"\"\n          };\n        }\n      } catch (err) {\n        console.error(\"Error fetching product:\", err.response?.data || err.message);\n      }\n    },\n    handleFileUpload(e) {\n      this.imageFile = e.target.files[0];\n    },\n    async updateProduct() {\n      const id = this.$route.params.id;\n      try {\n        const formData = new FormData();\n        Object.keys(this.form).forEach(key => {\n          if (key !== \"image\") {\n            formData.append(key, this.form[key]);\n      