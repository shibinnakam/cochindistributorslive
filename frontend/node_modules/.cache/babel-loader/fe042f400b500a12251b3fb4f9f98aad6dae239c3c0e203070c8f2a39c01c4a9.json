uct-carousel-overlay\">\r\n        <transition-group name=\"product-slide\" tag=\"div\" class=\"carousel-track\">\r\n          <img\r\n            v-for=\"(image, index) in visibleProduct\"\r\n            :key=\"index\"\r\n            :src=\"image\"\r\n            class=\"carousel-image\"\r\n          />\r\n        </transition-group>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'HomePage',\r\n  data() {\r\n    return {\r\n      isScrolled: false,\r\n      animate: false,\r\n      backgroundIndex: 0,\r\n      backgrounds: [\r\n        require('@/assets/bg.jpg'),\r\n        require('@/assets/login-bg.jpg'),\r\n      ],\r\n      visibleProduct: [],\r\n      productImages: [\r\n        require('@/assets/doritor.png'),\r\n        require('@/assets/lays.jpg'),\r\n        require('@/assets/kurkure.png'),\r\n        require('@/assets/cheetos1.png'),\r\n      ],\r\n      currentProductIndex: 0,\r\n    };\r\n  },\r\n  computed: {\r\n    backgroundStyle() {\r\n      return {\r\n        backgroundImage: `url(${this.backgrounds[this.backgroundIndex]})`,\r\n        backgroundSize: 'cover',\r\n        backgroundPosition: 'center',\r\n        height: '100vh',\r\n        position: 'relative',\r\n      };\r\n    },\r\n  },\r\n  mounted() {\r\n    window.addEventListener('scroll', this.handleScroll);\r\n\r\n    // Animate content\r\n    setTimeout(() => {\r\n      this.animate = true;\r\n    }, 100);\r\n\r\n    // Background slideshow\r\n    this.bgInterval = setInterval(() => {\r\n      this.backgroundIndex = (this.backgroundIndex + 1) % this.backgrounds.length;\r\n    }, 5000);\r\n\r\n    // Product slideshow\r\n    this.visibleProduct = [this.productImages[0]];\r\n    this.productInterval = setInterval(() => {\r\n      this.currentProductIndex = (this.currentProductIndex + 1) % this.productImages.length;\r\n      this.visibleProduct = [this.productImages[this.currentProductIndex]];\r\n    }, 3000);\r\n  },\r\n  beforeUnmount() {\r\n    window.removeEventListener('scroll', this.handleScroll);\r\n    clearInterval(this.bgInterval);\r\n    clearInterval(this.productInterval);\r\n  },\r\n  methods: {\r\n    handleScroll() {\r\n      this.isScrolled = window.scrollY > 50;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.home-page {\r\n  font-family: 'Arial', sans-serif;\r\n  color: #fff;\r\n  overflow: hidden;\r\n  position: relative;\r\n}\r\n\r\n/* Navbar */\r\n.navbar {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  padding: 1rem 2rem;\r\n  position: fixed;\r\n  width: 100%;\r\n  z-index: 1000;\r\n  transition: background 0.3s ease;\r\n}\r\n\r\n.navbar img {\r\n  height: 40px;\r\n}\r\n\r\n.nav-links {\r\n  display: flex;\r\n  gap: 2rem;\r\n  list-style: none;\r\n}\r\n\r\n.nav-links li a {\r\n  color: #fff;\r\n  text-decoration: none;\r\n  font-weight: bold;\r\n}\r\n\r\nheader.scrolled {\r\n  background: rgba(0, 0, 0, 0.7);\r\n}\r\n\r\n/* Hero */\r\n.hero {\r\n  height: 100vh;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  text-align: center;\r\n  position: relative;\r\n  z-index: 1;\r\n}\r\n\r\n.title {\r\n  font-size: 3rem;\r\n  font-weight: bold;\r\n  margin-bottom: 1rem;\r\n  animation: fadeInDown 1s ease;\r\n}\r\n\r\n.description {\r\n  font-size: 1.5rem;\r\n  animation: fadeInUp 1s ease;\r\n}\r\n\r\n/* Product Animation */\r\n.product-carousel-overlay {\r\n  position: absolute;\r\n  bottom: 10%;\r\n  display: flex;\r\n  justify-content: center;\r\n  width: 100%;\r\n  pointer-events: none;\r\n}\r\n\r\n.carousel-track {\r\n  display: flex;\r\n  gap: 1rem;\r\n  justify-content: center;\r\n}\r\n\r\n.carousel-image {\r\n  width: 120px;\r\n  height: 120px;\r\n  object-fit: contain;\r\n  animation: zoomInOut 3s ease-in-out infinite;\r\n  transition: transform 0.5s ease;\r\n}\r\n\r\n@keyframes zoomInOut {\r\n  0% {\r\n    transform: scale(1);\r\n    opacity: 0;\r\n  }\r\n  50% {\r\n    transform: scale(1.2);\r\n    opacity: 1;\r\n  }\r\n  100% {\r\n    transform: scale(1);\r\n    opacity: 0;\r\n  }\r\n}\r\n\r\n/* Animations */\r\n@keyframes fadeInDown {\r\n  from {\r\n    opacity: 0;\r\n    transform: translateY(-30px);\r\n  }\r\n  to {\r\n    opacity: 1;\r\n    transform: translateY(0);\r\n  }\r\n}\r\n\r\n@keyframes fadeInUp {\r\n  from {\r\n    opacity: 0;\r\n    transform: translateY(30px);\r\n  }\r\n  to {\r\n    opacity: 1;\r\n    transform: translateY(0);\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";OAMeA,UAAuB;;EAYzBC,KAAK,EAAC;AAAM;;EAOdA,KAAK,EAAC;AAA0B;;;uBAxBzCC,mBAAA,CAmCM;IAnCDD,KAAK,EAAC,WAAW;IAAEE,KAAK,EAAAC,eAAA,CAAEC,QAAA,CAAAC,eAAe;MAC5CC,mBAAA,YAAe,EACfC,mBAAA,CAYS;IAZAP,KAAK,EAAAQ,eAAA;MAAAC,QAAA,EAAcC,KAAA,CAAAC;IAAU;shBActCL,mBAAA,kBAAqB,EACrBC,mBAAA,CAiBU,WAjBVK,UAiBU,G,0BAhBRL,mBAAA,CAGM;IAHDP,KAAK,EAAC;EAAc,IACvBO,mBAAA,CAAqD;IAAjDP,KAAK,EAAC;EAAO,GAAC,gCAA8B,GAChDO,mBAAA,CAAwE;IAArEP,KAAK,EAAC;EAAa,GAAC,+CAA6C,E,qBAGtEM,mBAAA,+BAAkC,EAClCC,mBAAA,CASM,OATNM,UASM,GARJC,YAAA,CAOmBC,gBAAA;IAPDC,IAAI,EAAC,eAAe;IAACC,GAAG,EAAC,KAAK;IAACjB,KAAK,EAAC;;sBAEnD,MAAwC,E,kBAD1CC,mBAAA,CAKEiB,SAAA,QAAAC,WAAA,CAJyBT,KAAA,CAAAU,cAAc,GAA/BC,KAAK,EAAEC,KAAK;2BADtBrB,mBAAA,CAKE;QAHCsB,GAAG,EAAED,KAAK;QACVE,GAAG,EAAEH,KAAK;QACXrB,KAAK,EAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}