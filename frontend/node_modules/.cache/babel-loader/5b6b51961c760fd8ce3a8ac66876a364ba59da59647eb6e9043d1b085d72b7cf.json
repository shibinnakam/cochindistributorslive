{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport axios from \"axios\";\nimport \"@/assets/styles/AdminPage.css\";\nexport default {\n  name: \"AdminPage\",\n  data() {\n    return {\n      selectedMenu: \"dashboard\",\n      products: [],\n      form: {\n        name: \"\",\n        description: \"\",\n        originalPrice: \"\",\n        discountPrice: \"\",\n        image: null\n      }\n    };\n  },\n  computed: {\n    headerTitle() {\n      switch (this.selectedMenu) {\n        case \"dashboard\":\n          return \"Dashboard\";\n        case \"addProduct\":\n          return \"Add Product\";\n        case \"products\":\n          return \"Products\";\n        default:\n          return \"Dashboard\";\n      }\n    }\n  },\n  methods: {\n    async fetchProducts() {\n      try {\n        const res = await axios.get(\"http://localhost:5000/api/products/getproduct\");\n        this.products = res.data.products;\n      } catch (err) {\n        console.error(\"Error fetching products:\", err);\n      }\n    },\n    async deleteProduct(id) {\n      if (!confirm(\"Are you sure you want to delete this product?\")) return;\n      try {\n        await axios.delete(`http://localhost:5000/api/products/deleteproduct/${id}`);\n        this.fetchProducts();\n      } catch (err) {\n        console.error(\"Error deleting product:\", err);\n      }\n    },\n    handleFileUpload(e) {\n      this.form.image = e.target.files[0];\n    },\n    async addProduct() {\n      try {\n        const fd = new FormData();\n        Object.entries(this.form).forEach(([k, v]) => fd.append(k, v));\n        await axios.post(\"http://localhost:5000/api/products/addproduct\", fd, {\n          headers: {\n            \"Content-Type\": \"multipart/form-data\"\n          }\n        });\n        alert(\"Product added successfully!\");\n        this.selectedMenu = \"products\";\n        this.fetchProducts();\n        this.form = {\n          name: \"\",\n          description: \"\",\n          originalPrice: \"\",\n          discountPrice: \"\",\n          image: null\n        };\n      } catch (err) {\n        console.error(\"Error adding product:\", err);\n      }\n    },\n    logout() {\n      localStorage.removeItem(\"token\");\n      localStorage.removeItem(\"user\");\n      this.$router.push(\"/login\");\n    }\n  },\n  watch: {\n    selectedMenu(val) {\n      if (val === \"products\") this.fetchProducts();\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","selectedMenu","products","form","description","originalPrice","discountPrice","image","computed","headerTitle","methods","fetchProducts","res","get","err","console","error","deleteProduct","id","confirm","delete","handleFileUpload","e","target","files","addProduct","fd","FormData","Object","entries","forEach","k","v","append","post","headers","alert","logout","localStorage","removeItem","$router","push","watch","val"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\AdminPage.vue"],"sourcesContent":["<template>\n  <div class=\"admin-wrapper\">\n    <!-- Sidebar -->\n    <aside class=\"sidebar\">\n      <h2 class=\"sidebar-title\">Master Admin</h2>\n      <ul class=\"menu\">\n        <li><a href=\"#\" @click=\"selectedMenu = 'dashboard'\">Dashboard</a></li>\n        <li><a href=\"#\" @click=\"selectedMenu = 'addProduct'\">Add Products</a></li>\n        <li><a href=\"#\" @click=\"selectedMenu = 'products'\">View Products</a></li>\n        <li><a href=\"#\">Settings</a></li>\n        <li><a href=\"#\">Page Management</a></li>\n        <li><a href=\"#\">Metadata</a></li>\n        <li><a href=\"#\">Slider Management</a></li>\n        <li><a href=\"#\">Team Management</a></li>\n        <li><a href=\"#\">Services Management</a></li>\n        <li><a href=\"#\">Portfolio Management</a></li>\n        <li><a href=\"#\">Enquiry Management</a></li>\n      </ul>\n    </aside>\n\n    <!-- Main -->\n    <div class=\"main\">\n      <!-- Header -->\n      <header class=\"header\">\n        <h1>\n          {{ headerTitle }}\n          <span v-if=\"selectedMenu === 'dashboard'\" class=\"small\">Control panel</span>\n        </h1>\n        <div class=\"admin-menu\">\n          <span>Laravel Admin ▾</span>\n          <button class=\"logout-btn\" @click=\"logout\">\n            <i class=\"bi bi-box-arrow-right\"></i> Logout\n          </button>\n        </div>\n      </header>\n\n      <!-- Content -->\n      <main class=\"content\">\n        <!-- Dashboard View -->\n        <div v-if=\"selectedMenu === 'dashboard'\" class=\"dashboard-boxes\">\n          <div class=\"card blue\"><h2>4</h2><p>Total Static Page</p><span class=\"info\">More info →</span></div>\n          <div class=\"card orange\"><h2>15</h2><p>Total Slider</p><span class=\"info\">More info →</span></div>\n          <div class=\"card red\"><h2>70</h2><p>Total Team</p><span class=\"info\">More info →</span></div>\n          <div class=\"card green\"><h2>20</h2><p>Total Services</p><span class=\"info\">More info →</span></div>\n          <div class=\"card purple\"><h2>30</h2><p>Total Portfolio</p><span class=\"info\">More info →</span></div>\n          <div class=\"card pink\"><h2>50</h2><p>Total User</p><span class=\"info\">More info →</span></div>\n          <div class=\"card teal\"><h2>65</h2><p>Total Enquiries</p><span class=\"info\">More info →</span></div>\n        </div>\n\n        <!-- Add Product Form -->\n        <section v-if=\"selectedMenu === 'addProduct'\" class=\"form-section\">\n          <h2>Add New Product</h2>\n          <form @submit.prevent=\"addProduct\" enctype=\"multipart/form-data\">\n            <div class=\"form-group\">\n              <label>Name</label>\n              <input type=\"text\" v-model=\"form.name\" required />\n            </div>\n            <div class=\"form-group\">\n              <label>Description</label>\n              <textarea v-model=\"form.description\" required></textarea>\n            </div>\n            <div class=\"form-group\">\n              <label>Original Price</label>\n              <input type=\"number\" v-model=\"form.originalPrice\" required />\n            </div>\n            <div class=\"form-group\">\n              <label>Discount Price</label>\n              <input type=\"number\" v-model=\"form.discountPrice\" required />\n            </div>\n            <div class=\"form-group\">\n              <label>Image</label>\n              <input type=\"file\" @change=\"handleFileUpload\" required />\n            </div>\n            <button type=\"submit\" class=\"submit-btn\">Add Product</button>\n          </form>\n        </section>\n\n        <!-- Products View -->\n        <section v-if=\"selectedMenu === 'products'\" class=\"products-section\">\n          <h2>Uploaded Products</h2>\n          <table class=\"products-table\" v-if=\"products.length > 0\">\n            <thead>\n              <tr>\n                <th>Image</th>\n                <th>Name</th>\n                <th>Description</th>\n                <th>Original Price</th>\n                <th>Discount Price</th>\n                <th>Actions</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"product in products\" :key=\"product._id\">\n                <td><img :src=\"`http://localhost:5000${product.image}`\" alt=\"Product\" class=\"product-img\" /></td>\n                <td>{{ product.name }}</td>\n                <td>{{ product.description }}</td>\n                <td>₹{{ product.originalPrice }}</td>\n                <td>₹{{ product.discountPrice }}</td>\n                <td><button class=\"delete-btn\" @click=\"deleteProduct(product._id)\">Delete</button></td>\n              </tr>\n            </tbody>\n          </table>\n          <p v-else>No products uploaded yet.</p>\n        </section>\n      </main>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport \"@/assets/styles/AdminPage.css\";\n\nexport default {\n  name: \"AdminPage\",\n  data() {\n    return {\n      selectedMenu: \"dashboard\",\n      products: [],\n      form: { name: \"\", description: \"\", originalPrice: \"\", discountPrice: \"\", image: null },\n    };\n  },\n  computed: {\n    headerTitle() {\n      switch (this.selectedMenu) {\n        case \"dashboard\": return \"Dashboard\";\n        case \"addProduct\": return \"Add Product\";\n        case \"products\": return \"Products\";\n        default: return \"Dashboard\";\n      }\n    },\n  },\n  methods: {\n    async fetchProducts() {\n      try {\n        const res = await axios.get(\"http://localhost:5000/api/products/getproduct\");\n        this.products = res.data.products;\n      } catch (err) {\n        console.error(\"Error fetching products:\", err);\n      }\n    },\n    async deleteProduct(id) {\n      if (!confirm(\"Are you sure you want to delete this product?\")) return;\n      try {\n        await axios.delete(`http://localhost:5000/api/products/deleteproduct/${id}`);\n        this.fetchProducts();\n      } catch (err) {\n        console.error(\"Error deleting product:\", err);\n      }\n    },\n    handleFileUpload(e) {\n      this.form.image = e.target.files[0];\n    },\n    async addProduct() {\n      try {\n        const fd = new FormData();\n        Object.entries(this.form).forEach(([k, v]) => fd.append(k, v));\n        await axios.post(\"http://localhost:5000/api/products/addproduct\", fd, { headers: { \"Content-Type\": \"multipart/form-data\" } });\n        alert(\"Product added successfully!\");\n        this.selectedMenu = \"products\";\n        this.fetchProducts();\n        this.form = { name: \"\", description: \"\", originalPrice: \"\", discountPrice: \"\", image: null };\n      } catch (err) {\n        console.error(\"Error adding product:\", err);\n      }\n    },\n    logout() {\n      localStorage.removeItem(\"token\");\n      localStorage.removeItem(\"user\");\n      this.$router.push(\"/login\");\n    },\n  },\n  watch: {\n    selectedMenu(val) {\n      if (val === \"products\") this.fetchProducts();\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Keep your sidebar, header, cards, table, and form styling */\n.form-section, .products-section {\n  margin: 20px;\n  padding: 20px;\n  background: #fff;\n  border-radius: 12px;\n  box-shadow: 0 3px 8px rgba(0,0,0,0.1);\n}\n.form-group { margin-bottom: 15px; }\n.form-group label { font-weight: bold; margin-bottom: 6px; display: block; }\n.form-group input, .form-group textarea {\n  width: 100%; padding: 8px; border-radius: 6px; border: 1px solid #ccc;\n}\n.submit-btn { background: #4caf50; color: white; padding: 10px 16px; border: none; border-radius: 8px; cursor: pointer; }\n.products-table { width: 100%; border-collapse: collapse; }\n.products-table th, .products-table td { padding: 12px; border-bottom: 1px solid #ddd; text-align: center; }\n.product-img { width: 80px; height: 80px; object-fit: cover; }\n.delete-btn { background: red; color: #fff; padding: 6px 12px; border: none; border-radius: 6px; cursor: pointer; }\n</style>\n"],"mappings":";;;AA8GA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAO,+BAA+B;AAEtC,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,WAAW;MACzBC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE;QAAEJ,IAAI,EAAE,EAAE;QAAEK,WAAW,EAAE,EAAE;QAAEC,aAAa,EAAE,EAAE;QAAEC,aAAa,EAAE,EAAE;QAAEC,KAAK,EAAE;MAAK;IACvF,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAWA,CAAA,EAAG;MACZ,QAAQ,IAAI,CAACR,YAAY;QACvB,KAAK,WAAW;UAAE,OAAO,WAAW;QACpC,KAAK,YAAY;UAAE,OAAO,aAAa;QACvC,KAAK,UAAU;UAAE,OAAO,UAAU;QAClC;UAAS,OAAO,WAAW;MAC7B;IACF;EACF,CAAC;EACDS,OAAO,EAAE;IACP,MAAMC,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF,MAAMC,GAAE,GAAI,MAAMd,KAAK,CAACe,GAAG,CAAC,+CAA+C,CAAC;QAC5E,IAAI,CAACX,QAAO,GAAIU,GAAG,CAACZ,IAAI,CAACE,QAAQ;MACnC,EAAE,OAAOY,GAAG,EAAE;QACZC,OAAO,CAACC,KAAK,CAAC,0BAA0B,EAAEF,GAAG,CAAC;MAChD;IACF,CAAC;IACD,MAAMG,aAAaA,CAACC,EAAE,EAA