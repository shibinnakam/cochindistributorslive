{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nexport default {\n  data() {\n    return {\n      leaves: [],\n      searchQuery: \"\",\n      statusFilter: \"\",\n      toastMessage: \"\",\n      toastError: false,\n      showDeleteModal: false,\n      deleteId: null,\n      activeMenu: null\n    };\n  },\n  computed: {\n    filteredLeaves() {\n      return this.leaves.filter(leave => {\n        const matchesSearch = !this.searchQuery || leave.email?.toLowerCase().includes(this.searchQuery.toLowerCase());\n        const matchesStatus = !this.statusFilter || leave.status === this.statusFilter;\n        return matchesSearch && matchesStatus;\n      });\n    }\n  },\n  async created() {\n    await this.fetchLeaves();\n  },\n  methods: {\n    async fetchLeaves() {\n      try {\n        const res = await fetch(\"http://localhost:5000/api/leaves/all\");\n        this.leaves = await res.json();\n      } catch (err) {\n        this.showToast(\"Failed to fetch leaves\", true);\n      }\n    },\n    async updateStatus(id, status) {\n      try {\n        const res = await fetch(`http://localhost:5000/api/leaves/update/${id}`, {\n          method: \"PATCH\",\n          headers: {\n            \"Content-Type\": \"application/json\"\n          },\n          body: JSON.stringify({\n            status\n          })\n        });\n        const updatedLeave = await res.json();\n        this.leaves = this.leaves.map(l => l._id === id ? updatedLeave : l);\n        this.showToast(`Leave ${status} successfully`);\n        this.activeMenu = null;\n      } catch (err) {\n        this.showToast(\"Failed to update leave\", true);\n      }\n    },\n    openDeleteModal(id) {\n      this.deleteId = id;\n      this.showDeleteModal = true;\n      this.activeMenu = null;\n    },\n    cancelDelete() {\n      this.showDeleteModal = false;\n      this.deleteId = null;\n    },\n    async confirmDelete() {\n      try {\n        await fetch(`http://localhost:5000/api/leaves/delete/${this.deleteId}`, {\n          method: \"DELETE\"\n        });\n        this.leaves = this.leaves.filter(l => l._id !== this.deleteId);\n        this.showToast(\"Leave deleted successfully\");\n        this.showDeleteModal = false;\n        this.deleteId = null;\n      } catch (err) {\n        this.showToast(\"Failed to delete leave\", true);\n      }\n    },\n    toggleMenu(id) {\n      this.activeMenu = this.activeMenu === id ? null : id;\n    },\n    showToast(message, isError = false) {\n      this.toastMessage = message;\n      this.toastError = isError;\n      setTimeout(() => {\n        this.toastMessage = \"\";\n      }, 3000);\n    }\n  }\n};","map":{"version":3,"names":["data","leaves","searchQuery","statusFilter","toastMessage","toastError","showDeleteModal","deleteId","activeMenu","computed","filteredLeaves","filter","leave","matchesSearch","email","toLowerCase","includes","matchesStatus","status","created","fetchLeaves","methods","res","fetch","json","err","showToast","updateStatus","id","method","headers","body","JSON","stringify","updatedLeave","map","l","_id","openDeleteModal","cancelDelete","confirmDelete","toggleMenu","message","isError","setTimeout"],"sources":["C:\\Users\\LENOVO\\Music\\OneDrive\\Pictures\\distribution-agency-project\\frontend\\src\\views\\AdminLeaves.vue"],"sourcesContent":["<template>\n  <div class=\"dashboard-wrapper\">\n    <!-- Sidebar -->\n    <aside class=\"sidebar\">\n      <div class=\"sidebar-header\">\n        <div class=\"sidebar-logo\">\n          <span class=\"logo-icon\">‚öôÔ∏è</span>\n          <span class=\"logo-text\">Admin</span>\n        </div>\n      </div>\n\n      <ul class=\"menu\">\n        <li>\n          <router-link to=\"/admin\" class=\"menu-link\">\n            <span class=\"menu-icon\">üìä</span>\n            <span>Dashboard</span>\n          </router-link>\n        </li>\n        <li>\n          <router-link to=\"/admin/products\" class=\"menu-link\" active-class=\"active\">\n            <span class=\"menu-icon\">üì¶</span>\n            <span>Products</span>\n          </router-link>\n        </li>\n        <li>\n          <router-link to=\"/admin/add-category\" class=\"menu-link\" active-class=\"active\">\n            <span class=\"menu-icon\">üè∑Ô∏è</span>\n            <span>Categories</span>\n          </router-link>\n        </li>\n        <li>\n          <router-link to=\"/admin/staff-management\" class=\"menu-link\" active-class=\"active\">\n            <span class=\"menu-icon\">üë•</span>\n            <span>Staff</span>\n          </router-link>\n        </li>\n        <li>\n          <router-link to=\"/admin/leaves\" class=\"menu-link active\">\n            <span class=\"menu-icon\">üìÖ</span>\n            <span>Leave Requests</span>\n          </router-link>\n        </li>\n      </ul>\n\n      <div class=\"sidebar-footer\">\n        <button @click=\"logout\" class=\"logout-btn\">\n          <span class=\"menu-icon\">üö™</span>\n          <span>Logout</span>\n        </button>\n      </div>\n    </aside>\n\n    <!-- Main Content -->\n    <div class=\"admin-leaves\">\n    <!-- Top Header -->\n    <div class=\"top-header\">\n      <h1>LEAVE REQUESTS</h1>\n    </div>\n\n    <!-- Header Section -->\n    <div class=\"header-section\">\n      <div class=\"header-controls\">\n        <div class=\"search-filter\">\n          <div class=\"search-box\">\n            <input\n              v-model=\"searchQuery\"\n              type=\"text\"\n              placeholder=\"Search by email...\"\n              class=\"search-input\"\n            />\n            <i class=\"search-icon\">üîç</i>\n          </div>\n          <select v-model=\"statusFilter\" class=\"status-filter\">\n            <option value=\"\">All Status</option>\n            <option value=\"Pending\">Pending</option>\n            <option value=\"Approved\">Approved</option>\n            <option value=\"Rejected\">Rejected</option>\n          </select>\n        </div>\n      </div>\n    </div>\n\n    <!-- Toast Notification -->\n    <div\n      v-if=\"toastMessage\"\n      :class=\"['toast', toastError ? 'error' : 'success']\"\n    >\n      {{ toastMessage }}\n    </div>\n\n    <!-- Table Container -->\n    <div class=\"table-container\">\n      <table class=\"leave-table\">\n        <thead>\n          <tr>\n            <th>Email</th>\n            <th>Reason</th>\n            <th>Requested Date</th>\n            <th>Status</th>\n            <th>Approved Date</th>\n            <th>Rejected Date</th>\n            <th>Actions</th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr v-for=\"leave in filteredLeaves\" :key=\"leave._id\">\n            <td class=\"email-cell\">{{ leave.email }}</td>\n            <td class=\"reason-cell\">{{ leave.reason }}</td>\n            <td class=\"date-cell\">\n              {{ new Date(leave.date).toLocaleDateString() }}\n            </td>\n            <td class=\"status-cell\">\n              <span :class=\"['status-badge', leave.status.toLowerCase()]\">\n                {{ leave.status }}\n              </span>\n            </td>\n            <td class=\"date-cell\">\n              {{\n                leave.approvedDate\n                  ? new Date(leave.approvedDate).toLocaleDateString()\n                  : \"-\"\n              }}\n            </td>\n            <td class=\"date-cell\">\n              {{\n                leave.rejectedDate\n                  ? new Date(leave.rejectedDate).toLocaleDateString()\n                  : \"-\"\n              }}\n            </td>\n            <td class=\"actions-cell\">\n              <div class=\"action-menu\">\n                <button\n                  @click=\"toggleMenu(leave._id)\"\n                  class=\"menu-trigger\"\n                  title=\"More options\"\n                >\n                  ‚ãÆ\n                </button>\n                <div v-if=\"activeMenu === leave._id\" class=\"dropdown-menu\">\n                  <button\n                    v-if=\"leave.status === 'Pending'\"\n                    @click=\"updateStatus(leave._id, 'Approved')\"\n                    class=\"menu-item approve\"\n                  >\n                    ‚úì Approve\n                  </button>\n                  <button\n                    v-if=\"leave.status === 'Pending'\"\n                    @click=\"updateStatus(leave._id, 'Rejected')\"\n                    class=\"menu-item reject\"\n                  >\n                    ‚úï Reject\n                  </button>\n                  <button\n                    @click=\"openDeleteModal(leave._id)\"\n                    class=\"menu-item delete\"\n                  >\n                    üóëÔ∏è Delete\n                  </button>\n                </div>\n              </div>\n            </td>\n          </tr>\n        </tbody>\n      </table>\n      <div v-if=\"filteredLeaves.length === 0\" class=\"no-data\">\n        No leave requests found\n      </div>\n    </div>\n\n    <!-- Delete Confirmation Modal -->\n    <div v-if=\"showDeleteModal\" class=\"modal-overlay\" @click=\"cancelDelete\">\n      <div class=\"modal-delete\" @click.stop>\n        <div class=\"delete-icon\">‚ö†Ô∏è</div>\n        <h3>Delete Leave Request</h3>\n        <p>Are you sure you want to delete this leave request?</p>\n        <p class=\"warning\">This action cannot be undone.</p>\n        <div class=\"modal-actions-delete\">\n          <button @click=\"confirmDelete\" class=\"btn-delete-confirm\">\n            Delete\n          </button>\n          <button @click=\"cancelDelete\" class=\"btn-delete-cancel\">\n            Cancel\n          </button>\n        </div>\n      </div>\n    </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      leaves: [],\n      searchQuery: \"\",\n      statusFilter: \"\",\n      toastMessage: \"\",\n      toastError: false,\n      showDeleteModal: false,\n      deleteId: null,\n      activeMenu: null,\n    };\n  },\n  computed: {\n    filteredLeaves() {\n      return this.leaves.filter((leave) => {\n        const matchesSearch =\n          !this.searchQuery ||\n          leave.email?.toLowerCase().includes(this.searchQuery.toLowerCase());\n\n        const matchesStatus =\n          !this.statusFilter || leave.status === this.statusFilter;\n\n        return matchesSearch && matchesStatus;\n      });\n    },\n  },\n  async created() {\n    await this.fetchLeaves();\n  },\n  methods: {\n    async fetchLeaves() {\n      try {\n        const res = await fetch(\"http://localhost:5000/api/leaves/all\");\n        this.leaves = await res.json();\n      } catch (err) {\n        this.showToast(\"Failed to fetch leaves\", true);\n      }\n    },\n    async updateStatus(id, status) {\n      try {\n        const res = await fetch(\n          `http://localhost:5000/api/leaves/update/${id}`,\n          {\n            method: \"PATCH\",\n            headers: { \"Content-Type\": \"application/json\" },\n            body: JSON.stringify({ status }),\n          }\n        );\n        const updatedLeave = await res.json();\n        this.leaves = this.leaves.map((l) => (l._id === id ? updatedLeave : l));\n        this.showToast(`Leave ${status} successfully`);\n        this.activeMenu = null;\n      } catch (err) {\n        this.showToast(\"Failed to update leave\", true);\n      }\n    },\n    openDeleteModal(id) {\n      this.deleteId = id;\n      this.showDeleteModal = true;\n      this.activeMenu = null;\n    },\n    cancelDelete() {\n      this.showDeleteModal = false;\n      this.deleteId = null;\n    },\n    async confirmDelete() {\n      try {\n        await fetch(\n          `http://localhost:5000/api/leaves/delete/${this.deleteId}`,\n          {\n            method: \"DELETE\",\n          }\n        );\n        this.leaves = this.leaves.filter((l) => l._id !== this.deleteId);\n        this.showToast(\"Leave deleted successfully\");\n        this.showDeleteModal = false;\n        this.deleteId = null;\n      } catch (err) {\n        this.showToast(\"Failed to delete leave\", true);\n      }\n    },\n    toggleMenu(id) {\n      this.activeMenu = this.activeMenu === id ? null : id;\n    },\n    showToast(message, isError = false) {\n      this.toastMessage = message;\n      this.toastError = isError;\n      setTimeout(() => {\n        this.toastMessage = \"\";\n      }, 3000);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.admin-leaves {\n  padding: 0;\n  font-family: -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, sans-serif;\n  background: #ffffff;\n  min-height: 100vh;\n}\n\n.top-header {\n  padding: 24px 32px;\n  background: white;\n  border-bottom: 1px solid #f0f1f3;\n}\n\n.top-header h1 {\n  margin: 0;\n  font-size: 22px;\n  font-weight: 800;\n  color: #0f172a;\n  letter-spacing: 0.5px;\n}\n\n.header-section {\n  background: white;\n  padding: 20px 32px;\n  border-bottom: 1px solid #f0f1f3;\n}\n\n.header-controls {\n  display: flex;\n  justify-content: flex-start;\n  align-items: center;\n  gap: 20px;\n  flex-wrap: wrap;\n  margin: 0;\n}\n\n.search-filter {\n  display: flex;\n  gap: 14px;\n  flex: 1;\n  min-width: 320px;\n}\n\n.search-box {\n  position: relative;\n  flex: 1;\n}\n\n.search-input {\n  width: 100%;\n  padding: 10px 16px 10px 40px;\n  border: 1px solid #d1d5db;\n  border-radius: 8px;\n  font-size: 14px;\n  transition: all 0.3s ease;\n  background: white;\n  color: #1e293b;\n}\n\n.search-input::placeholder {\n  color: #9ca3af;\n}\n\n.search-input:focus {\n  outline: none;\n  border-color: #3b82f6;\n  box-shadow: 0 0 0 3px rgba(59, 130, 246, 0.05);\n}\n\n.search-icon {\n  position: absolute;\n  left: 14px;\n  top: 50%;\n  transform: translateY(-50%);\n  color: #64748b;\n  cursor: text;\n  font-size: 16px;\n}\n\n.status-filter {\n  padding: 10px 12px;\n  border: 1px solid #d1d5db;\n  border-radius: 8px;\n  font-size: 14px;\n  background: white;\n  cursor: pointer;\n  transition: all 0.3s ease;\n  color: #1e293b;\n  font-weight: 500;\n}\n\n.status-filter:hover {\n  border-color: #9ca3af;\n}\n\n.status-filter:focus {\n  outline: none;\n  border-color: #3b82f6;\n  box-shadow: 0 0 0 3px rgba(59, 130, 246, 0.05);\n}\n\n.table-container {\n  background: white;\n  border-radius: 0;\n  box-shadow: none;\n  overflow: visible;\n  border: none;\n  margin: 0;\n}\n\n.leave-table {\n  width: 100%;\n  border-collapse: collapse;\n}\n\n.leave-table thead {\n  background: #f9fafb;\n  border-bottom: 1px solid #e5e7eb;\n}\n\n.leave-table th {\n  padding: 16px 32px;\n  text-align: left;\n  font-size: 13px;\n  font-weight: 700;\n  color: #1e293b;\n  letter-spacing: 0;\n  text-transform: none;\n}\n\n.leave-table td {\n  padding: 18px 32px;\n  border-bottom: 1px solid #f3f4f6;\n  font-size: 14px;\n  color: #1e293b;\n}\n\n.leave-table tbody tr {\n  transition: all 0.2s ease;\n  overflow: visible;\n}\n\n.leave-table tbody tr:hover {\n  background: #f9fafb;\n  box-shadow: inset 0 0 12px rgba(59, 130, 246, 0.04);\n}\n\n.email-cell {\n  color: #1e293b;\n  font-weight: 500;\n}\n\n.reason-cell {\n  color: #64748b;\n}\n\n.date-cell {\n  color: #1e293b;\n  font-weight: 500;\n}\n\n.status-cell {\n  text-align: center;\n}\n\n.status-badge {\n  display: inline-flex;\n  align-items: center;\n  justify-content: center;\n  padding: 6px 14px;\n  border-radius: 20px;\n  font-size: 12px;\n  font-weight: 600;\n}\n\n.status-badge.pending {\n  background: #fef3c7;\n  color: #b45309;\n}\n\n.status-badge.approved {\n  background: #dcfce7;\n  color: #15803d;\n}\n\n.status-badge.rejected {\n  background: #fee2e2;\n  color: #b91c1c;\n}\n\n.actions-cell {\n  text-align: center;\n  overflow: visible;\n  position: relative;\n}\n\n.action-menu {\n  position: relative;\n  display: inline-block;\n  z-index: 200;\n}\n\n.menu-trigger {\n  background: none;\n  border: none;\n  color: #94a3b8;\n  font-size: 20px;\n  cursor: pointer;\n  