eaders"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\ProductList.vue"],"sourcesContent":["<template>\n  <div class=\"product-list-wrapper\">\n    <h2>All Products</h2>\n\n    <div v-if=\"loading\" class=\"loading\">Loading products...</div>\n\n    <div v-else-if=\"products.length === 0\" class=\"no-products\">\n      No products found.\n    </div>\n\n    <div v-else class=\"products-grid\">\n      <div v-for=\"product in products\" :key=\"product._id\" class=\"product-card\">\n        <img :src=\"getImageUrl(product.image)\" alt=\"Product Image\" />\n        <h3>{{ product.name }}</h3>\n        <p>{{ product.description }}</p>\n        <p>Original Price: ₹{{ product.originalPrice }}</p>\n        <p>Discount Price: ₹{{ product.discountPrice }}</p>\n        <p>Quantity: {{ product.quantity }}</p>\n        <p>Category: {{ product.category?.name }}</p>\n\n        <!-- Buttons -->\n        <button\n          @click=\"deleteProduct(product._id)\"\n          :disabled=\"deletingId === product._id\"\n          class=\"delete-btn\"\n        >\n          {{ deletingId === product._id ? \"Deleting...\" : \"Delete\" }}\n        </button>\n\n        <button @click=\"startUpdate(product)\" class=\"update-btn\">\n          Update\n        </button>\n      </div>\n    </div>\n\n    <!-- ✅ Update Modal -->\n    <div v-if=\"editingProduct\" class=\"modal\">\n      <div class=\"modal-content\">\n        <h3>Update Product</h3>\n        <input v-model=\"editingProduct.name\" placeholder=\"Name\" />\n        <input v-model.number=\"editingProduct.originalPrice\" type=\"number\" placeholder=\"Original Price\" />\n        <input v-model.number=\"editingProduct.discountPrice\" type=\"number\" placeholder=\"Discount Price\" />\n        <input v-model.number=\"editingProduct.quantity\" type=\"number\" placeholder=\"Quantity\" />\n        <textarea v-model=\"editingProduct.description\" placeholder=\"Description\"></textarea>\n\n        <!-- Update Image -->\n        <input type=\"file\" @change=\"handleImageUpload\" />\n        <div v-if=\"preview\">\n          <img :src=\"preview\" style=\"max-width:100px;\" />\n        </div>\n\n        <button @click=\"updateProduct\">Save</button>\n        <button @click=\"cancelUpdate\">Cancel</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"ProductList\",\n  data() {\n    return {\n      products: [],\n      loading: false,\n      deletingId: null,\n      editingProduct: null,\n      preview: null,\n      newImage: null,\n    };\n  },\n  mounted() {\n    this.fetchProducts();\n  },\n  methods: {\n    getImageUrl(path) {\n      return path.startsWith(\"/\") ? `http://localhost:5000${path}` : path;\n    },\n    async fetchProducts() {\n      this.loading = true;\n      try {\n        const res = await axios.get(\"http://localhost:5000/api/products/getproduct\");\n        if (res.data.success) {\n          this.products = res.data.products;\n        }\n      } catch (err) {\n        console.error(\"Error fetching products:\", err.response?.data || err.message);\n      } finally {\n        this.loading = false;\n      }\n    },\n    async deleteProduct(id) {\n      if (!confirm(\"Are you sure you want to delete this product?\")) return;\n      this.deletingId = id;\n      try {\n        const res = await axios.delete(`http://localhost:5000/api/products/deleteproduct/${id}`);\n        if (res.data.success) {\n          this.products = this.products.filter((p) => p._id !== id);\n          alert(\"✅ Product deleted successfully\");\n        }\n      } catch (err) {\n        alert(\"❌ Error deleting product\");\n        console.error(\"Delete product error:\", err.response?.data || err.message);\n      } finally {\n        this.deletingId = null;\n      }\n    },\n    startUpdate(product) {\n      this.editingProduct = { ...product }; // copy product data\n    },\n    cancelUpdate() {\n      this.editingProduct = null;\n      this.preview = null;\n      this.newImage = null;\n    },\n    handleImageUpload(e) {\n      const file = e.target.files[0];\n      if (file) {\n        this.newImage = file;\n        this.preview = URL.createObjectURL(file);\n      }\n    },\n    async updateProduct() {\n      try {\n        const formData = new FormData();\n        formData.append(\"name\", this.editingProduct.name);\n        formData.append(\"description\", this.editingProduct.description);\n        formData.append(\"originalPrice\", this.editingProduct.originalPrice);\n        formData.append(\"discountPrice\", this.editingProduct.discountPrice);\n        formData.append(\"quantity\", this.editingProduct.quantity);\n        formData.append(\"category\", this.editingProduct.category._id || this.editingProduct.category);\n        if (this.newImage) formData.append(\"image\", this.newImage);\n\n        const res = await axios.put(\n          `http://localhost:5000/api/products/updateproduct/${this.editingProduct._id}`,\n          formData,\n          { headers: { \"Content-Type\": \"multipart/form-data\" } }\n        );\n\n        if (res.data.success) {\n          alert(\"✅ Product updated successfully!\");\n          this.fetchProducts(); // reload list\n          this.cancelUpdate();\n        }\n      } catch (err) {\n        alert(\"❌ Error updating product\");\n        console.error(\"Update error:\", err.response?.data || err.message);\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.product-card button {\n  margin-top: 10px;\n  padding: 8px 12px;\n  border: none;\n  border-radius: 6px;\n  color: #fff;\n  cursor: pointer;\n  transition: 0.3s;\n}\n.delete-btn {\n  background-color: #cc0000;\n}\n.delete-btn:hover {\n  background-color: #990000;\n}\n.update-btn {\n  background-color: #0066cc;\n  margin-left: 8px;\n}\n.update-btn:hover {\n  background-color: #004c99;\n}\n\n/* ✅ Simple modal styling */\n.modal {\n  position: fixed;\n  top: 0;\n  left: 0;\n  