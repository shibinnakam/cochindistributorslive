{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"ProductEdit\",\n  data() {\n    return {\n      form: {\n        name: \"\",\n        description: \"\",\n        originalPrice: 0,\n        discountPrice: 0,\n        quantity: 0,\n        category: \"\"\n      },\n      imageFile: null,\n      categories: []\n    };\n  },\n  mounted() {\n    this.fetchCategories();\n    this.fetchProduct();\n  },\n  methods: {\n    async fetchCategories() {\n      try {\n        const res = await axios.get(\"http://localhost:5000/api/category/getcategories\");\n        if (res.data.success) {\n          this.categories = res.data.categories;\n        }\n      } catch (err) {\n        console.error(\"Error fetching categories:\", err.response?.data || err.message);\n      }\n    },\n    async fetchProduct() {\n      const id = this.$route.params.id;\n      try {\n        const res = await axios.get(`http://localhost:5000/api/products/getproduct/${id}`);\n        if (res.data.success) {\n          this.form = {\n            ...res.data.product,\n            category: res.data.product.category._id // ✅ only store ID\n          };\n        }\n      } catch (err) {\n        console.error(\"Error fetching product:\", err.response?.data || err.message);\n      }\n    },\n    handleFileUpload(e) {\n      this.imageFile = e.target.files[0];\n    },\n    async updateProduct() {\n      const id = this.$route.params.id;\n      try {\n        const formData = new FormData();\n        Object.keys(this.form).forEach(key => {\n          formData.append(key, this.form[key]);\n        });\n        if (this.imageFile) {\n          formData.append(\"image\", this.imageFile);\n        }\n        const res = await axios.put(`http://localhost:5000/api/products/updateproduct/${id}`, formData, {\n          headers: {\n            \"Content-Type\": \"multipart/form-data\"\n          }\n        });\n        if (res.data.success) {\n          alert(\"✅ Product updated successfully!\");\n          this.$router.push(\"/products\");\n        }\n      } catch (err) {\n        alert(\"❌ Error updating product\");\n        console.error(err.response?.data || err.message);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","form","description","originalPrice","discountPrice","quantity","category","imageFile","categories","mounted","fetchCategories","fetchProduct","methods","res","get","success","err","console","error","response","message","id","$route","params","product","_id","handleFileUpload","e","target","files","updateProduct","formData","FormData","Object","keys","forEach","key","append","put","headers","alert","$router","push"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\ProductEdit.vue"],"sourcesContent":["<template>\n  <div class=\"edit-wrapper\">\n    <div class=\"form-card\">\n      <h2>Edit Product</h2>\n\n      <form @submit.prevent=\"updateProduct\" enctype=\"multipart/form-data\">\n        <!-- Name -->\n        <div class=\"form-group\">\n          <label>Name</label>\n          <input v-model=\"form.name\" type=\"text\" required />\n        </div>\n\n        <!-- Description -->\n        <div class=\"form-group\">\n          <label>Description</label>\n          <textarea v-model=\"form.description\" required></textarea>\n        </div>\n\n        <!-- Original Price -->\n        <div class=\"form-group\">\n          <label>Original Price</label>\n          <input v-model.number=\"form.originalPrice\" type=\"number\" required />\n        </div>\n\n        <!-- Discount Price -->\n        <div class=\"form-group\">\n          <label>Discount Price</label>\n          <input v-model.number=\"form.discountPrice\" type=\"number\" required />\n        </div>\n\n        <!-- Quantity -->\n        <div class=\"form-group\">\n          <label>Quantity</label>\n          <input v-model.number=\"form.quantity\" type=\"number\" required />\n        </div>\n\n        <!-- Category -->\n        <div class=\"form-group\">\n          <label>Category</label>\n          <select v-model=\"form.category\" required>\n            <option v-for=\"cat in categories\" :key=\"cat._id\" :value=\"cat._id\">\n              {{ cat.name }}\n            </option>\n          </select>\n        </div>\n\n        <!-- Image -->\n        <div class=\"form-group\">\n          <label>Product Image</label>\n          <input type=\"file\" @change=\"handleFileUpload\" />\n        </div>\n\n        <button type=\"submit\" class=\"btn-submit\">Update Product</button>\n      </form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"ProductEdit\",\n  data() {\n    return {\n      form: {\n        name: \"\",\n        description: \"\",\n        originalPrice: 0,\n        discountPrice: 0,\n        quantity: 0,\n        category: \"\",\n      },\n      imageFile: null,\n      categories: [],\n    };\n  },\n  mounted() {\n    this.fetchCategories();\n    this.fetchProduct();\n  },\n  methods: {\n    async fetchCategories() {\n      try {\n        const res = await axios.get(\"http://localhost:5000/api/category/getcategories\");\n        if (res.data.success) {\n          this.categories = res.data.categories;\n        }\n      } catch (err) {\n        console.error(\"Error fetching categories:\", err.response?.data || err.message);\n      }\n    },\n    async fetchProduct() {\n      const id = this.$route.params.id;\n      try {\n        const res = await axios.get(\n          `http://localhost:5000/api/products/getproduct/${id}`\n        );\n        if (res.data.success) {\n          this.form = {\n            ...res.data.product,\n            category: res.data.product.category._id, // ✅ only store ID\n          };\n        }\n      } catch (err) {\n        console.error(\"Error fetching product:\", err.response?.data || err.message);\n      }\n    },\n    handleFileUpload(e) {\n      this.imageFile = e.target.files[0];\n    },\n    async updateProduct() {\n      const id = this.$route.params.id;\n      try {\n        const formData = new FormData();\n        Object.keys(this.form).forEach((key) => {\n          formData.append(key, this.form[key]);\n        });\n        if (this.imageFile) {\n          formData.append(\"image\", this.imageFile);\n        }\n\n        const res = await axios.put(\n          `http://localhost:5000/api/products/updateproduct/${id}`,\n          formData,\n          { headers: { \"Content-Type\": \"multipart/form-data\" } }\n        );\n\n        if (res.data.success) {\n          alert(\"✅ Product updated successfully!\");\n          this.$router.push(\"/products\");\n        }\n      } catch (err) {\n        alert(\"❌ Error updating product\");\n        console.error(err.response?.data || err.message);\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.edit-wrapper {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  padding: 30px;\n}\n\n.form-card {\n  background: #fff;\n  padding: 25px 30px;\n  border-radius: 12px;\n  box-shadow: 0 6px 20px rgba(0, 0, 0, 0.15);\n  width: 450px;\n}\n\nh2 {\n  text-align: center;\n  margin-bottom: 20px;\n  color: #333;\n}\n\n.form-group {\n  margin-bottom: 15px;\n}\n\nlabel {\n  display: block;\n  font-weight: 600;\n  margin-bottom: 6px;\n  color: #444;\n}\n\ninput,\ntextarea,\nselect {\n  width: 100%;\n  padding: 10px 12px;\n  border: 1px solid #ddd;\n  border-radius: 8px;\n  font-size: 14px;\n}\n\ntextarea {\n  resize: none;\n  min-height: 70px;\n}\n\n.btn-submit {\n  width: 100%;\n  padding: 12px;\n  border: none;\n  background: #007bff;\n  color: #fff;\n  font-weight: bold;\n  font-size: 15px;\n  border-radius: 8px;\n  cursor: pointer;\n  transition: 0.3s;\n}\n\n.btn-submit:hover {\n  background: #0056b3;\n}\n</style>\n"],"m