{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'LoginPage',\n  data() {\n    return {\n      email: '',\n      password: '',\n      isSignup: false\n    };\n  },\n  methods: {\n    async handleSubmit() {\n      try {\n        const endpoint = this.isSignup ? '/api/auth/register' : '/api/auth/login';\n        const {\n          data\n        } = await axios.post(endpoint, {\n          email: this.email,\n          password: this.password\n        });\n        if (this.isSignup) {\n          alert('Account created! Please login.');\n          this.isSignup = false;\n        } else {\n          localStorage.setItem('token', data.token);\n          this.$router.push('/admin');\n        }\n      } catch (err) {\n        alert(err.response?.data?.msg || 'Something went wrong');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","isSignup","methods","handleSubmit","endpoint","post","alert","localStorage","setItem","token","$router","push","err","response","msg"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"login-page\">\r\n    <!-- Left Side: Login Form -->\r\n    <div class=\"login-left\" :class=\"{ active: email && password }\">\r\n      <div class=\"form-box\">\r\n        <h2>{{ isSignup ? 'Create an Account' : 'Login to Your Account' }}</h2>\r\n        <form @submit.prevent=\"handleSubmit\">\r\n          <input v-model=\"email\" type=\"email\" placeholder=\"Email\" required />\r\n          <input v-model=\"password\" type=\"password\" placeholder=\"Password\" required />\r\n          <button type=\"submit\">{{ isSignup ? 'Sign Up' : 'Login' }}</button>\r\n        </form>\r\n        <p class=\"toggle\">\r\n          {{ isSignup ? 'Already have an account?' : 'Donâ€™t have an account?' }}\r\n          <span @click=\"isSignup = !isSignup\">\r\n            {{ isSignup ? 'Login' : 'Create Account' }}\r\n          </span>\r\n        </p>\r\n        <img src=\"@/assets/bike-sticker.png\" alt=\"bike\" class=\"bike-img\" />\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Right Side: Animated Content -->\r\n    <div class=\"login-right\">\r\n      <div class=\"right-content\">\r\n        <div class=\"text-box\">\r\n          <h1>Spin your way to higher conversions!</h1>\r\n          <p>Delivering efficiency through technology and automation.</p>\r\n        </div>\r\n        <div class=\"van-animation\">\r\n          <img src=\"@/assets/van.png\" alt=\"van\" class=\"van\" />\r\n          <div class=\"smoke\"></div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'LoginPage',\r\n  data() {\r\n    return {\r\n      email: '',\r\n      password: '',\r\n      isSignup: false,\r\n    };\r\n  },\r\n  methods: {\r\n    async handleSubmit() {\r\n      try {\r\n        const endpoint = this.isSignup ? '/api/auth/register' : '/api/auth/login';\r\n        const { data } = await axios.post(endpoint, {\r\n          email: this.email,\r\n          password: this.password,\r\n        });\r\n\r\n        if (this.isSignup) {\r\n          alert('Account created! Please login.');\r\n          this.isSignup = false;\r\n        } else {\r\n          localStorage.setItem('token', data.token);\r\n          this.$router.push('/admin');\r\n        }\r\n      } catch (err) {\r\n        alert(err.response?.data?.msg || 'Something went wrong');\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.login-page {\r\n  display: flex;\r\n  height: 100vh;\r\n  background: url('@/assets/login-bg.jpg') no-repeat center center;\r\n  background-size: cover;\r\n}\r\n\r\n/* Left: Login Form */\r\n.login-left {\r\n  width: 50%;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  background: rgba(255, 255, 255, 0.9);\r\n  transition: background 0.3s;\r\n}\r\n\r\n.login-left.active {\r\n  background: #e0f0ff;\r\n}\r\n\r\n.form-box {\r\n  width: 80%;\r\n  max-width: 400px;\r\n  background: white;\r\n  padding: 2rem;\r\n  border-radius: 12px;\r\n  box-shadow: 0 0 20px rgba(0, 0, 0, 0.2);\r\n  position: relative;\r\n}\r\n\r\ninput {\r\n  display: block;\r\n  width: 100%;\r\n  padding: 12px;\r\n  margin: 12px 0;\r\n  border-radius: 6px;\r\n  border: 1px solid #ccc;\r\n}\r\n\r\nbutton {\r\n  width: 100%;\r\n  padding: 12px;\r\n  background-color: #0044cc;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n}\r\n\r\n.toggle {\r\n  margin-top: 10px;\r\n  text-align: center;\r\n}\r\n\r\n.toggle span {\r\n  color: #0044cc;\r\n  cursor: pointer;\r\n  font-weight: bold;\r\n}\r\n\r\n.bike-img {\r\n  width: 80px;\r\n  position: absolute;\r\n  bottom: -20px;\r\n  right: 10px;\r\n}\r\n\r\n/* Right: Text and Van Animation */\r\n.login-right {\r\n  width: 50%;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  position: relative;\r\n}\r\n\r\n.right-content {\r\n  max-width: 400px;\r\n  background: rgba(255, 255, 255, 0.85);\r\n  padding: 2rem;\r\n  border-radius: 12px;\r\n  text-align: center;\r\n  animation: slideInRight 1s ease-in-out;\r\n}\r\n\r\n.text-box h1 {\r\n  font-size: 1.8rem;\r\n  margin-bottom: 1rem;\r\n}\r\n\r\n.van-animation {\r\n  position: relative;\r\n  margin-top: 30px;\r\n}\r\n\r\n.van {\r\n  width: 120px;\r\n  animation: moveVan 3s linear infinite;\r\n}\r\n\r\n.smoke {\r\n  width: 30px;\r\n  height: 30px;\r\n  background: rgba(200, 200, 200, 0.4);\r\n  border-radius: 50%;\r\n  position: absolute;\r\n  top: 50%;\r\n  left: -20px;\r\n  animation: puff 1.5s infinite;\r\n}\r\n\r\n/* Animations */\r\n@keyframes moveVan {\r\n  0% { transform: translateX(0); }\r\n  50% { transform: translateX(10px); }\r\n  100% { transform: translateX(0); }\r\n}\r\n\r\n@keyframes puff {\r\n  0% { opacity: 1; transform: scale(1); }\r\n  100% { opacity: 0; transform: scale(2); }\r\n}\r\n\r\n@keyframes slideInRight {\r\n  from { opacity: 0; transform: translateX(50px); }\r\n  to { opacity: 1; transform: translateX(0); }\r\n}\r\n</style>\r\n"],"mappings":";AAsCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,IAAI;QACF,MAAMC,QAAO,GAAI,IAAI,CAACH,QAAO,GAAI,oBAAmB,GAAI,iBAAiB;QACzE,MAAM;UAAEH;QAAK,IAAI,MAAMF,KAAK,CAACS,IAAI,CAACD,QAAQ,EAAE;UAC1CL,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QAEF,IAAI,IAAI,CAACC,QAAQ,EAAE;UACjBK,KAAK,CAAC,gCAAgC,CAAC;UACvC,IAAI,CAACL,QAAO,GAAI,KAAK;QACvB,OAAO;UACLM,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEV,IAAI,CAACW,KAAK,CAAC;UACzC,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;QAC7B;MACF,EAAE,OAAOC,GAAG,EAAE;QACZN,KAAK,CAACM,GAAG,CAACC,QAAQ,EAAEf,IAAI,EAAEgB,GAAE,IAAK,sBAAsB,CAAC;MAC1D;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}