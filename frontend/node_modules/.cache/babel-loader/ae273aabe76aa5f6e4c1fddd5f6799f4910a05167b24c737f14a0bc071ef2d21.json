{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'LoginPage',\n  data() {\n    return {\n      email: '',\n      password: '',\n      isLogin: true\n    };\n  },\n  methods: {\n    toggleMode() {\n      this.isLogin = !this.isLogin;\n    },\n    async handleSubmit() {\n      try {\n        const url = `http://localhost:5000/api/auth/${this.isLogin ? 'login' : 'register'}`;\n        const response = await axios.post(url, {\n          email: this.email,\n          password: this.password\n        });\n        alert(this.isLogin ? 'Login Successful' : 'Registered Successfully');\n        if (this.isLogin) {\n          localStorage.setItem('token', response.data.token);\n          this.$router.push('/admin');\n        } else {\n          this.isLogin = true;\n        }\n      } catch (err) {\n        alert(err.response?.data?.msg || 'Error occurred');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","isLogin","methods","toggleMode","handleSubmit","url","response","post","alert","localStorage","setItem","token","$router","push","err","msg"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"login-page\">\r\n    <div class=\"login-container\">\r\n      <div class=\"left-box\">\r\n        <h2>{{ isLogin ? 'Login' : 'Create an Account' }}</h2>\r\n        <form @submit.prevent=\"handleSubmit\">\r\n          <input v-model=\"email\" type=\"email\" placeholder=\"Email\" required />\r\n          <input v-model=\"password\" type=\"password\" placeholder=\"Password\" required />\r\n          <button :class=\"{ 'active': email && password }\" type=\"submit\">\r\n            {{ isLogin ? 'Login' : 'Register' }}\r\n          </button>\r\n        </form>\r\n        <p @click=\"toggleMode\">\r\n          {{ isLogin ? \"Don't have an account? Create one\" : \"Already have an account? Login\" }}\r\n        </p>\r\n      </div>\r\n      <div class=\"right-box\">\r\n        <div class=\"white-box\">\r\n          <h3>Spin your way to higher conversions with lightning-fast delivery!</h3>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'LoginPage',\r\n  data() {\r\n    return {\r\n      email: '',\r\n      password: '',\r\n      isLogin: true,\r\n    };\r\n  },\r\n  methods: {\r\n    toggleMode() {\r\n      this.isLogin = !this.isLogin;\r\n    },\r\n    async handleSubmit() {\r\n      try {\r\n        const url = `http://localhost:5000/api/auth/${this.isLogin ? 'login' : 'register'}`;\r\n        const response = await axios.post(url, {\r\n          email: this.email,\r\n          password: this.password,\r\n        });\r\n\r\n        alert(this.isLogin ? 'Login Successful' : 'Registered Successfully');\r\n\r\n        if (this.isLogin) {\r\n          localStorage.setItem('token', response.data.token);\r\n          this.$router.push('/admin');\r\n        } else {\r\n          this.isLogin = true;\r\n        }\r\n      } catch (err) {\r\n        alert(err.response?.data?.msg || 'Error occurred');\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.login-page {\r\n  height: 100vh;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  background: url('@/assets/login-bg.jpg') no-repeat center center/cover;\r\n}\r\n\r\n.login-container {\r\n  display: flex;\r\n  width: 90%;\r\n  max-width: 1000px;\r\n  background: rgba(255, 255, 255, 0.95);\r\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\r\n  border-radius: 20px;\r\n  overflow: hidden;\r\n}\r\n\r\n.left-box {\r\n  width: 50%;\r\n  padding: 40px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n}\r\n\r\n.left-box h2 {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.left-box form {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.left-box input {\r\n  margin-bottom: 15px;\r\n  padding: 12px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 8px;\r\n}\r\n\r\n.left-box button {\r\n  padding: 12px;\r\n  border: none;\r\n  background-color: #ccc;\r\n  border-radius: 8px;\r\n  color: white;\r\n  font-weight: bold;\r\n  cursor: pointer;\r\n}\r\n\r\n.left-box button.active {\r\n  background-color: lightblue;\r\n}\r\n\r\n.left-box p {\r\n  margin-top: 15px;\r\n  color: blue;\r\n  cursor: pointer;\r\n}\r\n\r\n.right-box {\r\n  width: 50%;\r\n  position: relative;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.white-box {\r\n  background: rgba(255, 255, 255, 0.7);\r\n  padding: 30px;\r\n  border-radius: 16px;\r\n  text-align: center;\r\n  animation: fadeIn 1s ease-in-out;\r\n}\r\n\r\n.white-box h3 {\r\n  font-size: 18px;\r\n}\r\n\r\n@keyframes fadeIn {\r\n  from {\r\n    opacity: 0;\r\n    transform: translateY(20px);\r\n  }\r\n  to {\r\n    opacity: 1;\r\n    transform: translateY(0);\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";AA0BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACF,OAAM,GAAI,CAAC,IAAI,CAACA,OAAO;IAC9B,CAAC;IACD,MAAMG,YAAYA,CAAA,EAAG;MACnB,IAAI;QACF,MAAMC,GAAE,GAAI,kCAAkC,IAAI,CAACJ,OAAM,GAAI,OAAM,GAAI,UAAU,EAAE;QACnF,MAAMK,QAAO,GAAI,MAAMV,KAAK,CAACW,IAAI,CAACF,GAAG,EAAE;UACrCN,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QAEFQ,KAAK,CAAC,IAAI,CAACP,OAAM,GAAI,kBAAiB,GAAI,yBAAyB,CAAC;QAEpE,IAAI,IAAI,CAACA,OAAO,EAAE;UAChBQ,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEJ,QAAQ,CAACR,IAAI,CAACa,KAAK,CAAC;UAClD,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;QAC7B,OAAO;UACL,IAAI,CAACZ,OAAM,GAAI,IAAI;QACrB;MACF,EAAE,OAAOa,GAAG,EAAE;QACZN,KAAK,CAACM,GAAG,CAACR,QAAQ,EAAER,IAAI,EAAEiB,GAAE,IAAK,gBAAgB,CAAC;MACpD;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"so