ry.token;\n      if (!token) {\n        this.message = \"Invalid or expired link\";\n        return;\n      }\n\n      try {\n        const formData = new FormData();\n        formData.append(\"token\", token);\n        formData.append(\"email\", this.email);\n        formData.append(\"name\", this.name);\n        formData.append(\"address\", this.address);\n        formData.append(\"phone\", this.phone);\n        formData.append(\"password\", this.password);\n        if (this.photoFile) {\n          formData.append(\"photo\", this.photoFile);\n        }\n\n        const res = await axios.post(\n          \"http://localhost:5000/api/staff/register\",\n          formData,\n          {\n            headers: { \"Content-Type\": \"multipart/form-data\" },\n          }\n        );\n\n        this.message = res.data.message;\n      } catch (err) {\n        this.message =\n          err.response?.data?.message || \"Error submitting registration\";\n      }\n    },\n  },\n};\n</script>\n"],"mappings":"AAwBA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,QAAO,QAAS,YAAY;AAErC,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIF,QAAQ,CAAC,CAAC;IACxB,OAAO;MAAEE;IAAM,CAAC;EAClB,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE,EAAE;MACRC,OAAO,EAAE,EAAE;MACXC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,IAAI;MACfC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAMC,KAAI,GAAI,IAAI,CAACV,KAAK,CAACW,KAAK,CAACD,KAAK;IACpC,IAAIA,KAAK,EAAE;MACT,IAAI;QACF;QACA,MAAME,GAAE,GAAI,MAAMf,KAAK,CAACgB,IAAI,CAC1B,8CAA8C,EAC9C;UAAEH;QAAM,CACV,CAAC;QACD,IAAI,CAACR,KAAI,GAAIU,GAAG,CAACX,IAAI,CAACC,KAAK;MAC7B,EAAE,OAAOY,GAAG,EAAE;QACZ,IAAI,CAACN,OAAM,GAAI,yBAAyB;MAC1C;IACF;EACF,CAAC;EACDO,OAAO,EAAE;IACPC,YAAYA,CAACC,CAAC,EAAE;MACd,IAAI,CAACV,SAAQ,GAAIU,CAAC,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;IACpC,CAAC;IACD,MAAMC,QAAQA,CAAA,EAAG;MACf,MAAMV,KAAI,GAAI,IAAI,CAACV,KAAK,CAACW,KAAK,CAACD,KAAK;MACpC,IAAI,CAACA,KAAK,EAAE;QACV,IAAI,CAACF,OAAM,GAAI,yBAAyB;QACxC;MACF;MAEA,IAAI;QA