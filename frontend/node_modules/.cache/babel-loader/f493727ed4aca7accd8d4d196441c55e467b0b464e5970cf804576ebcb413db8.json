{"ast":null,"code":"import axios from \"axios\";\nimport { useRoute } from \"vue-router\";\nexport default {\n  setup() {\n    const route = useRoute();\n    return {\n      route\n    };\n  },\n  data() {\n    return {\n      email: \"\",\n      name: \"\",\n      address: \"\",\n      phone: \"\",\n      password: \"\",\n      photoFile: null,\n      message: \"\"\n    };\n  },\n  async mounted() {\n    const token = this.route.query.token;\n    if (token) {\n      try {\n        // Call backend to decode token and fetch email\n        const res = await axios.post(\"http://localhost:5000/api/staff/verify-token\", {\n          token\n        });\n        this.email = res.data.email;\n      } catch (err) {\n        this.message = \"Invalid or expired link\";\n      }\n    }\n  },\n  methods: {\n    onFileChange(e) {\n      this.photoFile = e.target.files[0];\n    },\n    async register() {\n      const token = this.route.query.token;\n      if (!token) {\n        this.message = \"Invalid or expired link\";\n        return;\n      }\n      try {\n        const formData = new FormData();\n        formData.append(\"token\", token);\n        formData.append(\"email\", this.email);\n        formData.append(\"name\", this.name);\n        formData.append(\"address\", this.address);\n        formData.append(\"phone\", this.phone);\n        formData.append(\"password\", this.password);\n        if (this.photoFile) {\n          formData.append(\"photo\", this.photoFile);\n        }\n        const res = await axios.post(\"http://localhost:5000/api/staff/register\", formData, {\n          headers: {\n            \"Content-Type\": \"multipart/form-data\"\n          }\n        });\n        this.message = res.data.message;\n      } catch (err) {\n        this.message = err.response?.data?.message || \"Error submitting registration\";\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","useRoute","setup","route","data","email","name","address","phone","password","photoFil