{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"ProductEdit\",\n  data() {\n    return {\n      form: {\n        name: \"\",\n        description: \"\",\n        originalPrice: 0,\n        discountPrice: 0,\n        quantity: 0,\n        category: \"\"\n      },\n      imageFile: null\n    };\n  },\n  mounted() {\n    this.fetchProduct();\n  },\n  methods: {\n    async fetchProduct() {\n      const id = this.$route.params.id;\n      try {\n        const res = await axios.get(`http://localhost:5000/api/products/getproduct/${id}`);\n        if (res.data.success) {\n          this.form = res.data.product;\n        }\n      } catch (err) {\n        console.error(\"Error fetching product:\", err.response?.data || err.message);\n      }\n    },\n    handleFileUpload(e) {\n      this.imageFile = e.target.files[0];\n    },\n    async updateProduct() {\n      const id = this.$route.params.id;\n      try {\n        const formData = new FormData();\n        Object.keys(this.form).forEach(key => {\n          formData.append(key, this.form[key]);\n        });\n        if (this.imageFile) {\n          formData.append(\"image\", this.imageFile);\n        }\n        const res = await axios.put(`http://localhost:5000/api/products/updateproduct/${id}`, formData, {\n          headers: {\n            \"Content-Type\": \"multipart/form-data\"\n          }\n        });\n        if (res.data.success) {\n          alert(\"✅ Product updated successfully!\");\n          this.$router.push(\"/products\");\n        }\n      } catch (err) {\n        alert(\"❌ Error updating product\");\n        console.error(err.response?.data || err.message);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","form","description","originalPrice","discountPrice","quantity","category","imageFile","mounted","fetchProduct","methods","id","$route","params","res","get","success","product","err","console","error","response","message","handleFileUpload","e","target","files","updateProduct","formData","FormData","Object","keys","forEach","key","append","put","headers","alert","$router","push"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\ProductEdit.vue"],"sourcesContent":["<template>\r\n  <div class=\"edit-wrapper\">\r\n    <h2>Edit Product</h2>\r\n\r\n    <form @submit.prevent=\"updateProduct\" enctype=\"multipart/form-data\">\r\n      <div>\r\n        <label>Name:</label>\r\n        <input v-model=\"form.name\" type=\"text\" required />\r\n      </div>\r\n\r\n      <div>\r\n        <label>Description:</label>\r\n        <textarea v-model=\"form.description\" required></textarea>\r\n      </div>\r\n\r\n      <div>\r\n        <label>Original Price:</label>\r\n        <input v-model.number=\"form.originalPrice\" type=\"number\" required />\r\n      </div>\r\n\r\n      <div>\r\n        <label>Discount Price:</label>\r\n        <input v-model.number=\"form.discountPrice\" type=\"number\" required />\r\n      </div>\r\n\r\n      <div>\r\n        <label>Quantity:</label>\r\n        <input v-model.number=\"form.quantity\" type=\"number\" required />\r\n      </div>\r\n\r\n      <div>\r\n        <label>Category ID:</label>\r\n        <input v-model=\"form.category\" type=\"text\" required />\r\n      </div>\r\n\r\n      <div>\r\n        <label>Product Image:</label>\r\n        <input type=\"file\" @change=\"handleFileUpload\" />\r\n      </div>\r\n\r\n      <button type=\"submit\">Update Product</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"ProductEdit\",\r\n  data() {\r\n    return {\r\n      form: {\r\n        name: \"\",\r\n        description: \"\",\r\n        originalPrice: 0,\r\n        discountPrice: 0,\r\n        quantity: 0,\r\n        category: \"\",\r\n      },\r\n      imageFile: null,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchProduct();\r\n  },\r\n  methods: {\r\n    async fetchProduct() {\r\n      const id = this.$route.params.id;\r\n      try {\r\n        const res = await axios.get(`http://localhost:5000/api/products/getproduct/${id}`);\r\n        if (res.data.success) {\r\n          this.form = res.data.product;\r\n        }\r\n      } catch (err) {\r\n        console.error(\"Error fetching product:\", err.response?.data || err.message);\r\n      }\r\n    },\r\n    handleFileUpload(e) {\r\n      this.imageFile = e.target.files[0];\r\n    },\r\n    async updateProduct() {\r\n      const id = this.$route.params.id;\r\n      try {\r\n        const formData = new FormData();\r\n        Object.keys(this.form).forEach((key) => {\r\n          formData.append(key, this.form[key]);\r\n        });\r\n        if (this.imageFile) {\r\n          formData.append(\"image\", this.imageFile);\r\n        }\r\n\r\n        const res = await axios.put(\r\n          `http://localhost:5000/api/products/updateproduct/${id}`,\r\n          formData,\r\n          { headers: { \"Content-Type\": \"multipart/form-data\" } }\r\n        );\r\n\r\n        if (res.data.success) {\r\n          alert(\"✅ Product updated successfully!\");\r\n          this.$router.push(\"/products\");\r\n        }\r\n      } catch (err) {\r\n        alert(\"❌ Error updating product\");\r\n        console.error(err.response?.data || err.message);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":";;;AA8CA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJF,IAAI,EAAE,EAAE;QACRG,WAAW,EAAE,EAAE;QACfC,aAAa,EAAE,CAAC;QAChBC,aAAa,EAAE,CAAC;QAChBC,QAAQ,EAAE,CAAC;QACXC,QAAQ,EAAE;MACZ,CAAC;MACDC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,YAAY,CAAC,CAAC;EACrB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,YAAYA,CAAA,EAAG;MACnB,MAAME,EAAC,GAAI,IAAI,CAACC,MAAM,CAACC,MAA