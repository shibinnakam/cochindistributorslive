se}/api/staff/update`, formData, {\n        headers: {\n          Authorization: `Bearer ${token}`,\n          \"Content-Type\": \"multipart/form-data\"\n        }\n      }).then(res => {\n        this.message = res.data.message;\n        this.fetchProfile();\n      }).catch(err => this.message = err.response?.data?.message || \"Error updating profile\");\n    },\n    toggleLeaves() {\n      this.showLeaves = !this.showLeaves;\n    },\n    goToTab(tab) {\n      this.showLeaves = [\"apply-leave\", \"leave-status\"].includes(tab);\n      this.currentTab = tab;\n    },\n    logout() {\n      localStorage.removeItem(\"token\");\n      this.$router.push(\"/login\");\n    },\n    // Leave methods\n    submitLeave() {\n      const token = localStorage.getItem(\"token\");\n      axios.post(`${this.apiBase}/api/leaves/request`, {\n        ...this.newLeave,\n        email: this.staff.email\n      }, {\n        headers: {\n          Authorization: `Bearer ${token}`\n        }\n      }).then(() => {\n        this.leaveMessage = \"Leave request submitted!\";\n        this.newLeave = {\n          type: \"Sick Leave\",\n          from: \"\",\n          to: \"\",\n          reason: \"\"\n        };\n        this.fetchLeaveStatus();\n      }).catch(() => this.leaveMessage = \"Error submitting leave\");\n    },\n    fetchLeaveStatus() {\n      const token = localStorage.getItem(\"token\");\n      axios.get(`${this.apiBase}/api/leaves/my`, {\n        headers: {\n          Authorization: `Bearer ${token}`\n        }\n      }).then(res => this.leaveStatus = res.data).catch(err => console.error(err));\n    },\n    formatDate(dateStr) {\n      return new Date(dateStr).toLocaleDateString();\n    }\n  },\n  mounted() {\n    this.fetchProfile();\n    this.fetchStats();\n    this.fetchLeaveStatus();\n  }\n};","map":{"version":3,"names":["axios","data","currentTab","showLeaves","staff","name","email","phone","gender","address","pincode","profilePhoto","stats","totalItems","leavesTaken","pendin