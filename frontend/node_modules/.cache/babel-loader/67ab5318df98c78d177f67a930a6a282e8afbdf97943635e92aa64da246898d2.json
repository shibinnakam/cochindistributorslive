{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"ResetPassword\",\n  data() {\n    return {\n      password: \"\",\n      confirmPassword: \"\",\n      message: \"\",\n      error: \"\"\n    };\n  },\n  methods: {\n    async handleResetPassword() {\n      this.message = \"\";\n      this.error = \"\";\n      if (this.password !== this.confirmPassword) {\n        this.error = \"Passwords do not match!\";\n        return;\n      }\n      const token = this.$route.query.token; // Token from email link\n      try {\n        const res = await axios.post(\"/api/auth/reset-password\", {\n          token,\n          password: this.password\n        });\n        this.message = res.data.msg;\n      } catch (err) {\n        this.error = err.response?.data?.msg || \"Something went wrong\";\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","password","confirmPassword","message","error","methods","handleResetPassword","token","$route","query","res","post","msg","err","response"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\ResetPassword.vue"],"sourcesContent":["<template>\n  <div class=\"reset-container\">\n    <div class=\"reset-box\">\n      <h2>Reset Password</h2>\n      <form @submit.prevent=\"handleResetPassword\">\n        <input\n          type=\"password\"\n          v-model=\"password\"\n          placeholder=\"New password\"\n          required\n        />\n        <input\n          type=\"password\"\n          v-model=\"confirmPassword\"\n          placeholder=\"Confirm password\"\n          required\n        />\n        <button type=\"submit\">Reset Password</button>\n      </form>\n      <p v-if=\"message\" class=\"success\">{{ message }}</p>\n      <p v-if=\"error\" class=\"error\">{{ error }}</p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"ResetPassword\",\n  data() {\n    return {\n      password: \"\",\n      confirmPassword: \"\",\n      message: \"\",\n      error: \"\",\n    };\n  },\n  methods: {\n    async handleResetPassword() {\n      this.message = \"\";\n      this.error = \"\";\n      if (this.password !== this.confirmPassword) {\n        this.error = \"Passwords do not match!\";\n        return;\n      }\n      const token = this.$route.query.token; // Token from email link\n      try {\n        const res = await axios.post(\"/api/auth/reset-password\", {\n          token,\n          password: this.password,\n        });\n        this.message = res.data.msg;\n      } catch (err) {\n        this.error = err.response?.data?.msg || \"Something went wrong\";\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.reset-container {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  height: 100vh;\n  background: #f4f4f4;\n}\n.reset-box {\n  background: white;\n  padding: 2rem;\n  border-radius: 10px;\n  max-width: 350px;\n  width: 100%;\n  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);\n}\ninput {\n  width: 100%;\n  padding: 0.6rem;\n  margin-bottom: 1rem;\n  border-radius: 5px;\n  border: 1px solid #ccc;\n}\nbutton {\n  background: #1e3a8a;\n  color: white;\n  padding: 0.6rem;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n}\nbutton:hover {\n  background: #1d4ed8;\n}\n.success {\n  color: green;\n  font-size: 0.9rem;\n}\n.error {\n  color: red;\n  font-size: 0.9rem;\n}\n</style>\n"],"mappings":"AA0BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE,EAAE;MACnBC,OAAO,EAAE,EAAE;MACXC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,mBAAmBA,CAAA,EAAG;MAC1B,IAAI,CAACH,OAAM,GAAI,EAAE;MACjB,IAAI,CAACC,KAAI,GAAI,EAAE;MACf,IAAI,IAAI,CAACH,QAAO,KAAM,IAAI,CAACC,eAAe,EAAE;QAC1C,IAAI,CAACE,KAAI,GAAI,yBAAyB;QACtC;MACF;MACA,MAAMG,KAAI,GAAI,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,KAAK,EAAE;MACvC,IAAI;QACF,MAAMG,GAAE,GAAI,MAAMZ,KAAK,CAACa,IAAI,CAAC,0BAA0B,EAAE;UACvDJ,