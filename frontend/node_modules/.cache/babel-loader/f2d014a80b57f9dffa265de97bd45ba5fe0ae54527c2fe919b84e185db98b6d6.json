y=\"item._id\" class=\"cart-item\">\r\n            <div class=\"item-image\">\r\n              <img\r\n                :src=\"getImageUrl(item.product.image || item.product.imageFront)\"\r\n                :alt=\"item.product.name\"\r\n              />\r\n            </div>\r\n            <div class=\"item-details\">\r\n              <h3>{{ item.product.name }}</h3>\r\n              <p class=\"price\">₹{{ item.product.discountPrice }}</p>\r\n              <p class=\"original-price\">₹{{ item.product.originalPrice }}</p>\r\n              \r\n              <div class=\"quantity-controls\">\r\n                <button \r\n                  class=\"qty-btn\" \r\n                  @click=\"updateQuantity(item.product._id, -1)\"\r\n                  :disabled=\"item.quantity <= 1\"\r\n                >-</button>\r\n                <span class=\"qty-value\">{{ item.quantity }}</span>\r\n                <button \r\n                  class=\"qty-btn\" \r\n                  @click=\"updateQuantity(item.product._id, 1)\"\r\n                >+</button>\r\n              </div>\r\n            </div>\r\n            <div class=\"item-actions\">\r\n              <button class=\"remove-btn\" @click=\"removeItem(item.product._id)\">Remove</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"modal-footer\" v-if=\"cartItems.length > 0\">\r\n        <div class=\"total-section\">\r\n          <span>Total Amount:</span>\r\n          <span class=\"total-price\">₹{{ totalAmount }}</span>\r\n        </div>\r\n        <button class=\"place-order-btn\">Place Order</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"CartPage\",\r\n  data() {\r\n    return {\r\n      cartItems: [],\r\n      loading: false,\r\n    };\r\n  },\r\n  computed: {\r\n    totalAmount() {\r\n      return this.cartItems.reduce((total, item) => {\r\n        return total + (item.product.discountPrice * item.quantity);\r\n      }, 0);\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchCart();\r\n  },\r\n  methods: {\r\n    async fetchCart() {\r\n      this.loading = true;\r\n      try {\r\n        const token = localStorage.getItem(\"token\");\r\n        const res = await axios.get(\"http://localhost:5000/api/cart\", {\r\n          headers: { Authorization: `Bearer ${token}` },\r\n        });\r\n        this.cartItems = res.data;\r\n      } catch (err) {\r\n        console.error(\"Error fetching cart:\", err);\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n    async updateQuantity(productId, change) {\r\n      try {\r\n        const token = localStorage.getItem(\"token\");\r\n        const res = await axios.post(\r\n          \"http://localhost:5000/api/cart/add\",\r\n          { productId, quantity: change },\r\n          { headers: { Authorization: `Bearer ${token}` } }\r\n        );\r\n        \r\n        // Update local state\r\n        this.cartItems = res.data.cart;\r\n        this.$emit('cart-updated', this.cartItems.length);\r\n        \r\n      } catch (err) {\r\n        if (err.response && err.response.data && err.response.data.msg) {\r\n          alert(err.response.data.msg);\r\n        } else {\r\n          console.error(\"Error updating quantity:\", err);\r\n          alert(\"Failed to update quantity\");\r\n        }\r\n      }\r\n    },\r\n    async removeItem(productId) {\r\n      if(!confirm(\"Are you sure you want to remove this item?\")) return;\r\n      \r\n      try {\r\n        const token = localStorage.getItem(\"token\");\r\n        const res = await axios.delete(\r\n          `http://localhost:5000/api/cart/remove/${productId}`,\r\n          { headers: { Authorization: `Bearer ${token}` } }\r\n        );\r\n        \r\n        this.cartItems = res.data.cart;\r\n        this.$emit('cart-updated', this.cartItems.length);\r\n        \r\n      } catch (err) {\r\n        console.erro