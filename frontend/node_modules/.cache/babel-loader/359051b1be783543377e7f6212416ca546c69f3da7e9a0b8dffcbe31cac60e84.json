{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      profile: null,\n      error: null,\n      loading: true // added loading state\n    };\n  },\n  async created() {\n    try {\n      const token = localStorage.getItem(\"token\"); // token should be stored at login\n      if (!token) {\n        this.error = \"No token found. Please log in again.\";\n        this.loading = false;\n        return;\n      }\n      const res = await axios.get(\"http://localhost:5000/api/profile\", {\n        headers: {\n          Authorization: `Bearer ${token}`\n        }\n      });\n      this.profile = res.data;\n    } catch (err) {\n      this.error = \"Failed to load profile\";\n      console.error(\"Profile fetch error:\", err);\n    } finally {\n      this.loading = false; // stop loading after request\n    }\n  }\n};","map":{"version":3,"names":["axios","data","profile","error","loading","created","token","localStorage","getItem","res","get","headers","Authorization","err","console"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\ProfilePage.vue"],"sourcesContent":["<template>\n  <div class=\"profile-page\">\n    <h1>Admin Profile</h1>\n\n    <div v-if=\"loading\">Loading profile...</div>\n    <div v-else-if=\"error\" class=\"error\">{{ error }}</div>\n    <div v-else class=\"profile-card\">\n      <p><strong>Username:</strong> {{ profile.username }}</p>\n      <p><strong>Email:</strong> {{ profile.email }}</p>\n      <p><strong>Date of Birth:</strong> {{ profile.dob }}</p>\n      <p><strong>Phone:</strong> {{ profile.phone }}</p>\n      <p><strong>Address:</strong> {{ profile.address }}</p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      profile: null,\n      error: null,\n      loading: true, // added loading state\n    };\n  },\n  async created() {\n    try {\n      const token = localStorage.getItem(\"token\"); // token should be stored at login\n      if (!token) {\n        this.error = \"No token found. Please log in again.\";\n        this.loading = false;\n        return;\n      }\n\n      const res = await axios.get(\"http://localhost:5000/api/profile\", {\n        headers: { Authorization: `Bearer ${token}` },\n      });\n\n      this.profile = res.data;\n    } catch (err) {\n      this.error = \"Failed to load profile\";\n      console.error(\"Profile fetch error:\", err);\n    } finally {\n      this.loading = false; // stop loading after request\n    }\n  },\n};\n</script>\n\n<style scoped>\n.profile-page {\n  padding: 30px;\n  background: #fff;\n  border-radius: 8px;\n  max-width: 500px;\n  margin: 20px auto;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n  text-align: left;\n}\n\n.profile-card p {\n  font-size: 16px;\n  margin: 8px 0;\n}\n\n.error {\n  color: red;\n  font-weight: bold;\n}\n</style>\n"],"mappings":"AAiBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,IAAI;MACbC,KAAK,EAAE,IAAI;MACXC,OAAO,EAAE,IAAI,CAAE;IACjB,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC,EAAE;MAC7C,IAAI,CAACF,KAAK,EAAE;QACV,IAAI,CAACH,KAAI,GAAI,sCAAsC;QACnD,IAAI,CAACC,OAAM,GAAI,KAAK;QACpB;MACF;MAEA,MAAMK,GAAE,GAAI,MAAMT,KAAK,CAACU,GAAG,CAAC,mCAAmC,EAAE;QAC/DC,OAAO,EAAE;UAAEC,aAAa,EAAE,UAAUN,KAAK;QAAG;MAC9C,CAAC,CAAC;MAEF,IAAI,CAACJ,OAAM,GAAIO,GAAG,CAACR,IAAI;IACzB,EAAE,OAAOY,GAAG,EAAE;MACZ,IAAI,CAACV,KAAI,GAAI,wBAAwB;MACrCW,OAAO,CAACX,KAAK,CAAC,sBAAsB,EAAEU,GAAG,CAAC;IAC5C,UAAU;MACR,IAAI,CAACT,OAAM,GAAI,KAAK,EAAE;IACxB;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}