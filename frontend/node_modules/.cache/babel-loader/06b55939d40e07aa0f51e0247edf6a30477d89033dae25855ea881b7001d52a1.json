{"ast":null,"code":"export default {\n  name: \"HomePage\",\n  data() {\n    return {\n      animate: false,\n      backgroundIndex: 0,\n      backgrounds: [require(\"@/assets/bakery1.jpg\"), require(\"@/assets/bakery2.jpg\")],\n      sponsorLogos: [require(\"@/assets/sponsors/parle.png\"), require(\"@/assets/sponsors/nestle.png\"), require(\"@/assets/sponsors/itc.png\"), require(\"@/assets/sponsors/b.png\"), require(\"@/assets/sponsors/jackhenry.png\"), require(\"@/assets/sponsors/atlassian.png\"), require(\"@/assets/sponsors/instacart.png\"), require(\"@/assets/sponsors/luma.png\"), require(\"@/assets/sponsors/philips.png\")]\n    };\n  },\n  mounted() {\n    setTimeout(() => {\n      this.animate = true;\n    }, 100);\n    this.interval = setInterval(() => {\n      this.backgroundIndex = (this.backgroundIndex + 1) % this.backgrounds.length;\n    }, 5000);\n  },\n  beforeUnmount() {\n    clearInterval(this.interval);\n  }\n};","map":{"version":3,"names":["name","data","animate","backgroundIndex","backgrounds","require","sponsorLogos","mounted","setTimeout","interval","setInterval","length","beforeUnmount","clearInterval"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\Home.vue"],"sourcesContent":["<template>\n  <div class=\"home-page\">\n    <!-- Background layers for smooth fade -->\n    <div\n      v-for=\"(bg, index) in backgrounds\"\n      :key=\"index\"\n      class=\"background-layer\"\n      :style=\"{ backgroundImage: `url(${bg})` }\"\n      :class=\"{ active: backgroundIndex === index }\"\n    ></div>\n\n    <!-- Hero Section -->\n    <section class=\"hero\">\n      <h1>Welcome to My Website</h1>\n      <p>Explore our products and services</p>\n    </section>\n\n    <!-- Sponsors Section -->\n    <section class=\"sponsors-section\">\n      <p class=\"sponsors-title\">\n        Donâ€™t take our word for it. Our trusted sponsors:\n      </p>\n      <div class=\"sponsors-marquee\">\n        <div class=\"sponsors-track\">\n          <img v-for=\"(logo, i) in sponsorLogos\" :key=\"i\" :src=\"logo\" alt=\"Sponsor Logo\" />\n          <!-- Duplicate logos for infinite loop -->\n          <img v-for=\"(logo, i) in sponsorLogos\" :key=\"'dup-' + i\" :src=\"logo\" alt=\"Sponsor Logo\" />\n        </div>\n      </div>\n    </section>\n\n    <!-- Cochin Distributors Section -->\n    <section class=\"cochin-section\">\n      <div class=\"cochin-container\">\n        <!-- Left: Image -->\n        <div class=\"cochin-images\" :class=\"{ 'slide-left': animate }\">\n          <img src=\"@/assets/bg.jpg\" class=\"img-large\" alt=\"Main\" />\n          <div class=\"img-small-column\">\n            <img src=\"@/assets/bakery1.jpg\" class=\"img-small\" alt=\"Login\" />\n            <img src=\"@/assets/logo.jpeg\" class=\"img-small\" alt=\"Logo\" />\n          </div>\n        </div>\n\n        <!-- Right: Text -->\n        <div class=\"cochin-content\" :class=\"{ 'slide-right': animate }\">\n          <h3 class=\"cochinhead\">\n            <span class=\"welcome-red\">WELCOME TO</span><br />\n            <strong>Cochin Distributors</strong>\n          </h3>\n          <p class=\"cochintext\">\n            At Cochin Distributors, we believe in delivering the finest quality\n            baking products to every kitchen, whether you're a home baker or a\n            commercial chef...\n          </p>\n        </div>\n      </div>\n    </section>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"HomePage\",\n  data() {\n    return {\n      animate: false,\n      backgroundIndex: 0,\n      backgrounds: [\n        require(\"@/assets/bakery1.jpg\"),\n        require(\"@/assets/bakery2.jpg\"),\n      ],\n      sponsorLogos: [\n        require(\"@/assets/sponsors/parle.png\"),\n        require(\"@/assets/sponsors/nestle.png\"),\n        require(\"@/assets/sponsors/itc.png\"),\n        require(\"@/assets/sponsors/b.png\"),\n        require(\"@/assets/sponsors/jackhenry.png\"),\n        require(\"@/assets/sponsors/atlassian.png\"),\n        require(\"@/assets/sponsors/instacart.png\"),\n        require(\"@/assets/sponsors/luma.png\"),\n        require(\"@/assets/sponsors/philips.png\"),\n      ],\n    };\n  },\n  mounted() {\n    setTimeout(() => {\n      this.animate = true;\n    }, 100);\n\n    this.interval = setInterval(() => {\n      this.backgroundIndex =\n        (this.backgroundIndex + 1) % this.backgrounds.length;\n    }, 5000);\n  },\n  beforeUnmount() {\n    clearInterval(this.interval);\n  },\n};\n</script>\n\n<style scoped>\n.home-page {\n  position: relative;\n  min-height: 100vh;\n  overflow-x: hidden;\n}\n\n/* Background fade layers */\n.background-layer {\n  position: absolute;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background-size: cover;\n  background-position: center;\n  opacity: 0;\n  transition: opacity 1.5s ease-in-out;\n}\n.background-layer.active {\n  opacity: 1;\n}\n\n.hero {\n  height: 100vh;\n  color: white;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  padding-top: 4rem;\n  text-shadow: 1px 1px 5px rgba(0, 0, 0, 0.6);\n  position: relative;\n  z-index: 1;\n}\n\n/* Sponsors Section */\n.sponsors-section {\n  background-color: #001233;\n  padding: 2rem 1rem;\n  text-align: center;\n}\n.sponsors-title {\n  color: white;\n  font-size: 1.1rem;\n  margin-bottom: 1.5rem;\n}\n.sponsors-marquee {\n  overflow: hidden;\n  position: relative;\n}\n.sponsors-track {\n  display: flex;\n  gap: 3rem;\n  animation: scroll-left 20s linear infinite;\n}\n.sponsors-track img {\n  height: 40px;\n  object-fit: contain;\n}\n@keyframes scroll-left {\n  0% {\n    transform: translateX(0);\n  }\n  100% {\n    transform: translateX(-50%);\n  }\n}\n\n/* Cochin Section */\n.cochin-section {\n  padding: 2rem;\n  background-color: #fff;\n  position: relative;\n  z-index: 1;\n}\n.cochin-container {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 2rem;\n  align-items: flex-start;\n  justify-content: center;\n}\n.cochin-images {\n  display: flex;\n  gap: 1rem;\n  opacity: 0;\n  transform: translateX(-100%);\n  transition: all 1s ease;\n}\n.cochin-content {\n  max-width: 600px;\n  opacity: 0;\n  transform: translateX(100%);\n  transition: all 1s ease;\n}\n.cochinhead {\n  margin: 0;\n  font-size: 1.8rem;\n  line-height: 1.4;\n}\n.welcome-red {\n  color: red;\n  font-weight: bold;\n}\n.cochintext {\n  font-size: 1rem;\n  line-height: 1.6;\n  color: #333;\n  text-align: justify;\n}\n.img-large {\n  width: 220px;\n  height: 350px;\n  object-fit: cover;\n  border-radius: 8px;\n}\n.img-small-column {\n  display: flex;\n  flex-direction: column;\n  gap: 1rem;\n}\n.img-small {\n  width: 220px;\n  height: 165px;\n  object-fit: contain;\n  background-color: #f5f5f5;\n  border-radius: 8px;\n}\n.slide-left {\n  transform: translateX(0);\n  opacity: 1;\n}\n.slide-right {\n  transform: translateX(0);\n  opacity: 1;\n}\n</style>\n"],"mappings":"AA6DA,eAAe;EACbA,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,KAAK;MACdC,eAAe,EAAE,CAAC;MAClBC,WAAW,EAAE,CACXC,OAAO,CAAC,sBAAsB,CAAC,EAC/BA,OAAO,CAAC,sBAAsB,CAAC,CAChC;MACDC,YAAY,EAAE,CACZD,OAAO,CAAC,6BAA6B,CAAC,EACtCA,OAAO,CAAC,8BAA8B,CAAC,EACvCA,OAAO,CAAC,2BAA2B,CAAC,EACpCA,OAAO,CAAC,yBAAyB,CAAC,EAClCA,OAAO,CAAC,iCAAiC,CAAC,EAC1CA,OAAO,CAAC,iCAAiC,CAAC,EAC1CA,OAAO,CAAC,iCAAiC,CAAC,EAC1CA,OAAO,CAAC,4BAA4B,CAAC,EACrCA,OAAO,CAAC,+BAA+B,CAAC;IAE5C,CAAC;EACH,CAAC;EACDE,OAAOA,CAAA,EAAG;IACRC,UAAU,CAAC,MAAM;MACf,IAAI,CAACN,OAAM,GAAI,IAAI;IACrB,CAAC,EAAE,GAAG,CAAC;IAEP,IAAI,CAACO,QAAO,GAAIC,WAAW,CAAC,MAAM;MAChC,IAAI,CAACP,eAAc,GACjB,CAAC,IAAI,CAACA,eAAc,GAAI,CAAC,IAAI,IAAI,CAACC,WAAW,CAACO,MAAM;IACxD,CAAC,EAAE,IAAI,CAAC;EACV,CAAC;EACDC,aAAaA,CAAA,EAAG;IACdC,aAAa,CAAC,IAAI,CAACJ,QAAQ,CAAC;EAC9B;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}