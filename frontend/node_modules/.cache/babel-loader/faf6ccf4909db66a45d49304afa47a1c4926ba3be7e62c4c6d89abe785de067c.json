{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: \"LoginPage\",\n  data() {\n    return {\n      isSignup: false,\n      email: \"\",\n      password: \"\",\n      isFilled: false\n    };\n  },\n  methods: {\n    toggleMode() {\n      this.isSignup = !this.isSignup;\n      this.email = \"\";\n      this.password = \"\";\n      this.isFilled = false;\n    },\n    checkFilled() {\n      this.isFilled = this.email.trim() !== \"\" && this.password.trim() !== \"\";\n    },\n    async handleSubmit() {\n      try {\n        // Use full URL or use proxy\n        const endpoint = this.isSignup ? '/api/auth/register' : '/api/auth/login';\n        const res = await axios.post(endpoint, {\n          email: this.email,\n          password: this.password\n        });\n        if (this.isSignup) {\n          alert('Registered successfully. Please login.');\n          this.toggleMode();\n        } else {\n          localStorage.setItem('token', res.data.token);\n          alert('Login successful!');\n          this.$router.push('/admin');\n        }\n      } catch (err) {\n        console.error('Login/Register error:', err);\n        alert(err.response?.data?.msg || err.message || 'An error occurred.');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","isSignup","email","password","isFilled","methods","toggleMode","checkFilled","trim","handleSubmit","endpoint","res","post","alert","localStorage","setItem","token","$router","push","err","console","error","response","msg","message"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"login-container\">\r\n    <!-- Left: Login/Signup Form -->\r\n    <div class=\"login-form-section\">\r\n      <div :class=\"['login-box', isFilled ? 'box-filled' : '']\">\r\n        <img src=\"@/assets/logo.jpeg\" alt=\"Logo\" class=\"logo\" />\r\n        <h2 class=\"form-title\">{{ isSignup ? 'Sign Up' : 'Login' }}</h2>\r\n        <p class=\"signup-link\">\r\n          {{ isSignup ? 'Already have an account?' : 'Don’t have an account?' }}\r\n          <a href=\"#\" @click.prevent=\"toggleMode\">{{ isSignup ? 'Login' : 'Create an account' }}</a>\r\n        </p>\r\n\r\n        <form class=\"login-form\" @submit.prevent=\"handleSubmit\" @input=\"checkFilled\">\r\n          <label for=\"email\">Email</label>\r\n          <input type=\"email\" id=\"email\" v-model=\"email\" placeholder=\"Enter email\" required />\r\n\r\n          <label for=\"password\">Password</label>\r\n          <input type=\"password\" id=\"password\" v-model=\"password\" placeholder=\"Enter password\" required />\r\n\r\n          <a v-if=\"!isSignup\" href=\"#\" class=\"forgot-password\">Forgot your password?</a>\r\n\r\n          <button type=\"submit\" class=\"login-button\">\r\n            {{ isSignup ? 'Sign Up' : 'Login' }}\r\n          </button>\r\n        </form>\r\n\r\n        <footer class=\"form-footer\">\r\n          <small>© 2025 YourCompany. All rights reserved.</small>\r\n        </footer>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Right: Info Section -->\r\n    <div class=\"info-section\">\r\n      <img src=\"@/assets/login-bg.jpg\" alt=\"Background\" class=\"background-image\" />\r\n      <div class=\"paper-card\">\r\n        <h2>Spin Your Way to Higher Conversions Today!</h2>\r\n        <p>Boost conversions, grow subscribers, reduce cart abandonment with Coupon Wheels.</p>\r\n        <p><em>Available on OptinMonster Growth plans.</em></p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: \"LoginPage\",\r\n  data() {\r\n    return {\r\n      isSignup: false,\r\n      email: \"\",\r\n      password: \"\",\r\n      isFilled: false,\r\n    };\r\n  },\r\n  methods: {\r\n    toggleMode() {\r\n      this.isSignup = !this.isSignup;\r\n      this.email = \"\";\r\n      this.password = \"\";\r\n      this.isFilled = false;\r\n    },\r\n    checkFilled() {\r\n      this.isFilled = this.email.trim() !== \"\" && this.password.trim() !== \"\";\r\n    },\r\n    async handleSubmit() {\r\n      try {\r\n        // Use full URL or use proxy\r\n        const endpoint = this.isSignup\r\n          ? '/api/auth/register'\r\n          : '/api/auth/login';\r\n\r\n        const res = await axios.post(endpoint, {\r\n          email: this.email,\r\n          password: this.password\r\n        });\r\n\r\n        if (this.isSignup) {\r\n          alert('Registered successfully. Please login.');\r\n          this.toggleMode();\r\n        } else {\r\n          localStorage.setItem('token', res.data.token);\r\n          alert('Login successful!');\r\n          this.$router.push('/admin');\r\n        }\r\n      } catch (err) {\r\n        console.error('Login/Register error:', err);\r\n        alert(err.response?.data?.msg || err.message || 'An error occurred.');\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.login-container {\r\n  display: flex;\r\n  height: 100vh;\r\n  font-family: 'Segoe UI', sans-serif;\r\n}\r\n\r\n.login-form-section {\r\n  width: 50%;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  background-color: #f8f9fa;\r\n}\r\n\r\n.login-box {\r\n  width: 80%;\r\n  max-width: 350px;\r\n  background-color: #ffffff;\r\n  padding: 2rem;\r\n  border-radius: 10px;\r\n  box-shadow: 0 6px 20px rgba(0, 0, 0, 0.1);\r\n  transition: border 0.3s ease, box-shadow 0.3s ease;\r\n}\r\n\r\n.box-filled {\r\n  border: 2px solid #3b82f6;\r\n  box-shadow: 0 6px 25px rgba(59, 130, 246, 0.2);\r\n}\r\n\r\n.logo {\r\n  display: block;\r\n  margin: 0 auto 1rem auto;\r\n  height: 60px;\r\n}\r\n\r\n.form-title {\r\n  text-align: center;\r\n  font-size: 1.6rem;\r\n  margin-bottom: 0.5rem;\r\n}\r\n\r\n.signup-link {\r\n  text-align: center;\r\n  font-size: 0.9rem;\r\n  margin-bottom: 1.5rem;\r\n}\r\n\r\n.signup-link a {\r\n  color: #007bff;\r\n  text-decoration: none;\r\n  cursor: pointer;\r\n}\r\n\r\n.login-form label {\r\n  display: block;\r\n  margin-bottom: 0.2rem;\r\n  font-weight: 600;\r\n}\r\n\r\n.login-form input {\r\n  width: 100%;\r\n  padding: 0.5rem;\r\n  margin-bottom: 1rem;\r\n  border: 1px solid #ced4da;\r\n  border-radius: 5px;\r\n}\r\n\r\n.forgot-password {\r\n  font-size: 0.85rem;\r\n  color: #007bff;\r\n  text-decoration: none;\r\n  display: block;\r\n  margin-bottom: 1rem;\r\n}\r\n\r\n.login-button {\r\n  width: 100%;\r\n  background-color: #1e3a8a;\r\n  color: white;\r\n  border: none;\r\n  padding: 0.6rem;\r\n  border-radius: 5px;\r\n  font-weight: bold;\r\n  cursor: pointer;\r\n  transition: background 0.3s ease;\r\n}\r\n\r\n.login-button:hover {\r\n  background-color: #1d4ed8;\r\n}\r\n\r\n.form-footer {\r\n  text-align: center;\r\n  margin-top: 1rem;\r\n  font-size: 0.8rem;\r\n  color: #6c757d;\r\n}\r\n\r\n.info-section {\r\n  width: 50%;\r\n  position: relative;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  overflow: hidden;\r\n}\r\n\r\n.background-image {\r\n  position: absolute;\r\n  inset: 0;\r\n  object-fit: cover;\r\n  width: 100%;\r\n  height: 100%;\r\n  z-index: 1;\r\n  filter: brightness(0.9);\r\n}\r\n\r\n.paper-card {\r\n  position: relative;\r\n  z-index: 2;\r\n  width: 80%;\r\n  max-width: 500px;\r\n  background-color: white;\r\n  padding: 2rem;\r\n  border-radius: 12px;\r\n  box-shadow: 0 8px 24px rgba(0, 0, 0, 0.2);\r\n  backdrop-filter: blur(6px);\r\n}\r\n\r\n.paper-card h2 {\r\n  font-size: 1.5rem;\r\n  margin-bottom: 1rem;\r\n  color: #111827;\r\n}\r\n\r\n.paper-card p {\r\n  font-size: 1rem;\r\n  line-height: 1.6;\r\n  color: #374151;\r\n}\r\n</style>\r\n"],"mappings":";AA6CA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,KAAK;MACfC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA