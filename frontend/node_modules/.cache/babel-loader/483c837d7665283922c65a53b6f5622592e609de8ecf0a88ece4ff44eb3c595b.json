{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"LoginPage\",\n  data() {\n    return {\n      isSignup: false,\n      email: \"\",\n      password: \"\",\n      isFilled: false\n    };\n  },\n  methods: {\n    toggleMode() {\n      this.isSignup = !this.isSignup;\n      this.email = \"\";\n      this.password = \"\";\n      this.isFilled = false;\n    },\n    checkFilled() {\n      this.isFilled = this.email.trim() !== \"\" && this.password.trim() !== \"\";\n    },\n    closePopup() {\n      alert(\"Popup closed! You can replace this with router navigation.\");\n    },\n    async handleSubmit() {\n      try {\n        const endpoint = this.isSignup ? \"/api/auth/register\" : \"/api/auth/login\";\n        const res = await axios.post(endpoint, {\n          email: this.email,\n          password: this.password\n        });\n        if (this.isSignup) {\n          alert(\"Registered successfully. Please login.\");\n          this.toggleMode();\n        } else {\n          localStorage.setItem(\"token\", res.data.token);\n          alert(\"Login successful!\");\n          this.$router.push(\"/admin\");\n        }\n      } catch (err) {\n        console.error(\"Login/Register error:\", err);\n        alert(err.response?.data?.msg || err.message || \"An error occurred.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","isSignup","email","password","isFilled","methods","toggleMode","checkFilled","trim","closePopup","alert","handleSubmit","endpoint","res","post","localStorage","setItem","token","$router","push","err","console","error","response","msg","message"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\Login.vue"],"sourcesContent":["<template>\n  <div class=\"login-container\">\n    <!-- Popup Login Form -->\n    <div class=\"login-popup\">\n      <!-- Close button -->\n      <button class=\"close-btn\" @click=\"closePopup\">×</button>\n\n      <img src=\"@/assets/logo.jpeg\" alt=\"Logo\" class=\"logo\" />\n      <h2 class=\"form-title\">{{ isSignup ? \"Sign Up\" : \"Login\" }}</h2>\n      <p class=\"signup-link\">\n        {{ isSignup ? \"Already have an account?\" : \"Don’t have an account?\" }}\n        <a href=\"#\" @click.prevent=\"toggleMode\">\n          {{ isSignup ? \"Login\" : \"Create an account\" }}\n        </a>\n      </p>\n\n      <form\n        class=\"login-form\"\n        @submit.prevent=\"handleSubmit\"\n        @input=\"checkFilled\"\n      >\n        <input\n          type=\"email\"\n          v-model=\"email\"\n          placeholder=\"Enter email\"\n          required\n        />\n\n        <input\n          type=\"password\"\n          v-model=\"password\"\n          placeholder=\"Enter password\"\n          required\n        />\n\n        <a v-if=\"!isSignup\" href=\"#\" class=\"forgot-password\">\n          Forgot your password?\n        </a>\n\n        <button type=\"submit\" class=\"login-button\">\n          {{ isSignup ? \"Sign Up\" : \"Login\" }}\n        </button>\n      </form>\n\n      <footer class=\"form-footer\">\n        <small>© 2025 YourCompany. All rights reserved.</small>\n      </footer>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"LoginPage\",\n  data() {\n    return {\n      isSignup: false,\n      email: \"\",\n      password: \"\",\n      isFilled: false,\n    };\n  },\n  methods: {\n    toggleMode() {\n      this.isSignup = !this.isSignup;\n      this.email = \"\";\n      this.password = \"\";\n      this.isFilled = false;\n    },\n    checkFilled() {\n      this.isFilled = this.email.trim() !== \"\" && this.password.trim() !== \"\";\n    },\n    closePopup() {\n      alert(\"Popup closed! You can replace this with router navigation.\");\n    },\n    async handleSubmit() {\n      try {\n        const endpoint = this.isSignup\n          ? \"/api/auth/register\"\n          : \"/api/auth/login\";\n\n        const res = await axios.post(endpoint, {\n          email: this.email,\n          password: this.password,\n        });\n\n        if (this.isSignup) {\n          alert(\"Registered successfully. Please login.\");\n          this.toggleMode();\n        } else {\n          localStorage.setItem(\"token\", res.data.token);\n          alert(\"Login successful!\");\n          this.$router.push(\"/admin\");\n        }\n      } catch (err) {\n        console.error(\"Login/Register error:\", err);\n        alert(err.response?.data?.msg || err.message || \"An error occurred.\");\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Full screen background */\n.login-container {\n  height: 100vh;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n\n  background: url('@/assets/cinema1.jpg') no-repeat center center;\n  background-size: cover;\n\n  font-family: \"Segoe UI\", sans-serif;\n  padding: 1rem;\n  box-sizing: border-box;\n}\n\n/* Center popup box */\n.login-popup {\n  position: relative;\n  max-width: 400px;\n  width: 100%;\n  background-color: rgba(255, 255, 255, 0.98);\n  padding: 2.5rem 3rem;\n  border-radius: 20px;\n  box-shadow: 0 8px 30px rgba(0, 0, 0, 0.3);\n}\n\n/* Close button (×) */\n.close-btn {\n  position: absolute;\n  top: 10px;\n  right: 15px;\n  border: none;\n  background: transparent;\n  font-size: 1.5rem;\n  cursor: pointer;\n  color: #555;\n}\n.close-btn:hover {\n  color: #000;\n}\n\n/* Logo styling */\n.logo {\n  height: 60px;\n  margin-bottom: 1rem;\n  display: block;\n  margin-left: auto;\n  margin-right: auto;\n}\n\n.form-title {\n  text-align: center;\n  font-size: 1.6rem;\n  margin-bottom: 0.5rem;\n}\n\n.signup-link {\n  text-align: center;\n  font-size: 0.9rem;\n  margin-bottom: 1.5rem;\n}\n\n.signup-link a {\n  color: #8b1538;\n  font-weight: 500;\n  text-decoration: none;\n}\n\n.login-form input {\n  width: 100%;\n  padding: 0.5rem;\n  margin-bottom: 1rem;\n  border: 1px solid #ced4da;\n  border-radius: 5px;\n}\n\n.login-form input:focus {\n  outline: none;\n  border-color: #8b1538;\n  box-shadow: 0 0 5px rgba(139, 21, 56, 0.4);\n}\n\n.forgot-password {\n  font-size: 0.85rem;\n  color: #8b1538;\n  text-decoration: none;\n  display: block;\n  margin-bottom: 1rem;\n}\n\n.login-button {\n  width: 100%;\n  background-color: #8b1538;\n  color: white;\n  border: none;\n  padding: 0.6rem;\n  border-radius: 5px;\n  font-weight: bold;\n  cursor: pointer;\n  transition: background 0.3s ease;\n}\n\n.login-button:hover {\n  background-color: #6e0f2d;\n}\n\n.form-footer {\n  text-align: center;\n  margin-top: 1rem;\n  font-size: 0.8rem;\n  color: #6c757d;\n}\n</style>\n"],"mappings":";AAoDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,KAAK;MACfC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACL,QAAO,GAAI,CAAC,IAAI,CAACA,QAAQ;MAC9B,IAAI,CAACC,KAAI,GAAI,EAAE;MACf,IAAI,CAACC,QAAO,GAAI,EAAE;MAClB,IAAI,CAACC,QAAO,GAAI,KAAK;IACvB,CAAC;IACDG,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACH,QAAO,GAAI,IAAI,CAACF,KAAK,CAACM,IAAI,CAAC,MAAM,EAAC,IAAK,IAAI,CAACL,QAAQ,CAACK,IAAI,CAAC,MAAM,EAAE;IACzE,CAAC;IACDC,UAAUA,CAAA,EAAG;MACXC,KAAK,CAAC,4DAA4D,CAAC;IACrE,CAAC;IACD,MAAMC,YAAYA,CAAA,EAAG;MACnB,IAAI;QACF,MAAMC,QAAO,GAAI,IAAI,CAACX,QAAO,GACzB,oBAAmB,GACnB,iBAAiB;QAErB,MAAMY,GAAE,GAAI,MAAMf,KAAK,CAACgB,IAAI,CAACF,QAAQ,EAAE;UACrCV,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QAEF,IAAI,IAAI,CAACF,QAAQ,EAAE;UACjBS,KAAK,CAAC,wCAAwC,CAAC;UAC/C,IAAI,CAACJ,UAAU,CAAC,CAAC;QACnB,OAAO;UACLS,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEH,GAAG,CAACb,IAAI,CAACiB,KAAK,CAAC;UAC7CP,KAAK,CAAC,mBAAmB,CAAC;UAC1B,IAAI,CAACQ,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;QAC7B;MACF,EAAE,OAAOC,GAAG,EAAE;QACZC,OAAO,CAACC,KAAK,CAAC,uBAAuB,EAAEF,GAAG,CAAC;QAC3CV,KAAK,CAACU,GAAG,CAACG,QAAQ,EAAEvB,IAAI,EAAEwB,GAAE,IAAKJ,GAAG,CAACK,OAAM,IAAK,oBAAoB,CAAC;MACvE;IACF;EACF;AACF,CAAC","ignoreList":