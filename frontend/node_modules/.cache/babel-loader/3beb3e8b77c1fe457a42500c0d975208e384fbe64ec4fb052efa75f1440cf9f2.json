{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      name: \"\",\n      categories: [],\n      editMode: false,\n      editId: null\n    };\n  },\n  methods: {\n    async handleSubmit() {\n      if (this.editMode) {\n        await this.updateCategory();\n      } else {\n        await this.addCategory();\n      }\n    },\n    async addCategory() {\n      try {\n        const response = await axios.post(\"http://localhost:5000/api/categories\", {\n          name: this.name\n        });\n        this.categories.push(response.data);\n        this.name = \"\";\n      } catch (err) {\n        console.error(err.response?.data || err);\n        alert(err.response?.data?.error || \"Error adding category\");\n      }\n    },\n    async fetchCategories() {\n      try {\n        const res = await axios.get(\"http://localhost:5000/api/categories\");\n        this.categories = res.data;\n      } catch (err) {\n        console.error(\"Error fetching categories:\", err);\n      }\n    },\n    startEdit(cat) {\n      this.name = cat.name;\n      this.editMode = true;\n      this.editId = cat._id;\n    },\n    cancelEdit() {\n      this.name = \"\";\n      this.editMode = false;\n      this.editId = null;\n    },\n    async updateCategory() {\n      try {\n        const res = await axios.put(`http://localhost:5000/api/categories/${this.editId}`, {\n          name: this.name\n        });\n        const index = this.categories.findIndex(c => c._id === this.editId);\n        if (index !== -1) this.categories[index] = res.data;\n        this.cancelEdit();\n      } catch (err) {\n        console.error(err.response?.data || err);\n        alert(err.response?.data?.error || \"Error updating category\");\n      }\n    },\n    async deleteCategory(id) {\n      try {\n        await axios.delete(`http://localhost:5000/api/categories/${id}`);\n        this.categories = this.categories.filter(c => c._id !== id);\n      } catch (err) {\n        console.error(err.response?.data || err);\n        alert(\"Error deleting category\");\n      }\n    }\n  },\n  mounted() {\n    this.fetchCategories();\n  }\n};","map":{"version":3,"names":["axios","data","name","categories","editMode","editId","methods","handleSubmit","updateCategory","addCategory","response","post","push","err","console","error","alert","fetchCategories","res","get","startEdit","cat","_id","cancelEdit","put","index","findIndex","c","deleteCategory","id","delete","filter","mounted"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\AddCategory.vue"],"sourcesContent":["<template>\n  <div class=\"add-category\">\n    <h2>Add / Update Category</h2>\n    <form @submit.prevent=\"handleSubmit\">\n      <input\n        v-model=\"name\"\n        type=\"text\"\n        placeholder=\"Enter category name\"\n        required\n      />\n      <button type=\"submit\">{{ editMode ? \"Update\" : \"Add\" }}</button>\n      <button v-if=\"editMode\" type=\"button\" @click=\"cancelEdit\">Cancel</button>\n    </form>\n\n    <h3>Categories</h3>\n    <ul>\n      <li v-for=\"cat in categories\" :key=\"cat._id\">\n        {{ cat.name }}\n        <button @click=\"startEdit(cat)\">Edit</button>\n        <button @click=\"deleteCategory(cat._id)\">Delete</button>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      name: \"\",\n      categories: [],\n      editMode: false,\n      editId: null,\n    };\n  },\n  methods: {\n    async handleSubmit() {\n      if (this.editMode) {\n        await this.updateCategory();\n      } else {\n        await this.addCategory();\n      }\n    },\n    async addCategory() {\n      try {\n        const response = await axios.post(\"http://localhost:5000/api/categories\", {\n          name: this.name,\n        });\n        this.categories.push(response.data);\n        this.name = \"\";\n      } catch (err) {\n        console.error(err.response?.data || err);\n        alert(err.response?.data?.error || \"Error adding category\");\n      }\n    },\n    async fetchCategories() {\n      try {\n        const res = await axios.get(\"http://localhost:5000/api/categories\");\n        this.categories = res.data;\n      } catch (err) {\n        console.error(\"Error fetching categories:\", err);\n      }\n    },\n    startEdit(cat) {\n      this.name = cat.name;\n      this.editMode = true;\n      this.editId = cat._id;\n    },\n    cancelEdit() {\n      this.name = \"\";\n      this.editMode = false;\n      this.editId = null;\n    },\n    async updateCategory() {\n      try {\n        const res = await axios.put(`http://localhost:5000/api/categories/${this.editId}`, {\n          name: this.name,\n        });\n        const index = this.categories.findIndex((c) => c._id === this.editId);\n        if (index !== -1) this.categories[index] = res.data;\n        this.cancelEdit();\n      } catch (err) {\n        console.error(err.response?.data || err);\n        alert(err.response?.data?.error || \"Error updating category\");\n      }\n    },\n    async deleteCategory(id) {\n      try {\n        await axios.delete(`http://localhost:5000/api/categories/${id}`);\n        this.categories = this.categories.filter((c) => c._id !== id);\n      } catch (err) {\n        console.error(err.response?.data || err);\n        alert(\"Error deleting category\");\n      }\n    },\n  },\n  mounted() {\n    this.fetchCategories();\n  },\n};\n</script>\n\n<style>\n.add-category {\n  max-width: 400px;\n  margin: auto;\n}\ninput {\n  padding: 8px;\n  margin-right: 5px;\n}\nbutton {\n  padding: 6px 10px;\n  margin-left: 5px;\n}\n</style>\n"],"mappings":";;;AA0BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,UAAU,EAAE,EAAE;MACd