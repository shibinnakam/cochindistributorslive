{"ast":null,"code":"export default {\n  name: \"HomePage\",\n  data() {\n    return {\n      backgroundImages: [require(\"@/assets/bg1.jpg\"), require(\"@/assets/bg2.jpg\"), require(\"@/assets/bg3.jpg\")],\n      currentBgIndex: 0,\n      products: [{\n        name: \"Doritos\",\n        image: require(\"@/assets/doritor.png\")\n      }, {\n        name: \"Kurkure\",\n        image: require(\"@/assets/kurkure.png\")\n      }, {\n        name: \"Lays\",\n        image: require(\"@/assets/lays.jpg\")\n      }, {\n        name: \"Cheetos\",\n        image: require(\"@/assets/cheetos1.png\")\n      }],\n      currentProductIndex: 0\n    };\n  },\n  computed: {\n    backgroundStyle() {\n      return {\n        backgroundImage: `url(${this.backgroundImages[this.currentBgIndex]})`,\n        backgroundSize: \"cover\",\n        backgroundPosition: \"center\",\n        height: \"100vh\",\n        transition: \"background-image 1s ease-in-out\"\n      };\n    }\n  },\n  mounted() {\n    setInterval(() => {\n      this.currentBgIndex = (this.currentBgIndex + 1) % this.backgroundImages.length;\n    }, 5000);\n    setInterval(() => {\n      this.currentProductIndex = (this.currentProductIndex + 1) % this.products.length;\n    }, 3000);\n  }\n};","map":{"version":3,"names":["name","data","backgroundImages","require","currentBgIndex","products","image","currentProductIndex","computed","backgroundStyle","backgroundImage","backgroundSize","backgroundPosition","height","transition","mounted","setInterval","length"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\Home.vue"],"sourcesContent":["<template>\n  <div class=\"home-page\" :style=\"backgroundStyle\">\n    <!-- Hero Section -->\n    <section class=\"hero\">\n      <h1>Welcome to Our Website</h1>\n      <p>Delivering the best products to your doorstep.</p>\n    </section>\n\n    <!-- Product Carousel -->\n    <section class=\"products\">\n      <h2>Our Products</h2>\n      <div class=\"carousel\">\n        <div\n          class=\"product\"\n          v-for=\"(product, index) in products\"\n          :key=\"index\"\n          :class=\"{ active: currentProductIndex === index }\"\n        >\n          <img :src=\"product.image\" :alt=\"product.name\" />\n          <p>{{ product.name }}</p>\n        </div>\n      </div>\n    </section>\n\n    <!-- About Section -->\n    <section class=\"about\">\n      <h2>About Us</h2>\n      <p>\n        We are committed to providing high-quality food products with reliable\n        delivery services.\n      </p>\n    </section>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"HomePage\",\n  data() {\n    return {\n      backgroundImages: [\n        require(\"@/assets/bg1.jpg\"),\n        require(\"@/assets/bg2.jpg\"),\n        require(\"@/assets/bg3.jpg\"),\n      ],\n      currentBgIndex: 0,\n      products: [\n        { name: \"Doritos\", image: require(\"@/assets/doritor.png\") },\n        { name: \"Kurkure\", image: require(\"@/assets/kurkure.png\") },\n        { name: \"Lays\", image: require(\"@/assets/lays.jpg\") },\n        { name: \"Cheetos\", image: require(\"@/assets/cheetos1.png\") },\n      ],\n      currentProductIndex: 0,\n    };\n  },\n  computed: {\n    backgroundStyle() {\n      return {\n        backgroundImage: `url(${this.backgroundImages[this.currentBgIndex]})`,\n        backgroundSize: \"cover\",\n        backgroundPosition: \"center\",\n        height: \"100vh\",\n        transition: \"background-image 1s ease-in-out\",\n      };\n    },\n  },\n  mounted() {\n    setInterval(() => {\n      this.currentBgIndex =\n        (this.currentBgIndex + 1) % this.backgroundImages.length;\n    }, 5000);\n\n    setInterval(() => {\n      this.currentProductIndex =\n        (this.currentProductIndex + 1) % this.products.length;\n    }, 3000);\n  },\n};\n</script>\n\n<style scoped>\n.hero {\n  padding: 50px;\n  color: white;\n  text-shadow: 2px 2px 5px rgba(0, 0, 0, 0.7);\n}\n\n.products {\n  background: white;\n  padding: 40px 0;\n}\n\n.carousel {\n  display: flex;\n  justify-content: center;\n  gap: 20px;\n}\n\n.product {\n  opacity: 0.5;\n  transition: opacity 0.5s ease-in-out, transform 0.5s ease-in-out;\n}\n\n.product.active {\n  opacity: 1;\n  transform: scale(1.1);\n}\n\n.about {\n  background: #f5f5f5;\n  padding: 40px;\n}\n</style>\n"],"mappings":"AAoCA,eAAe;EACbA,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,gBAAgB,EAAE,CAChBC,OAAO,CAAC,kBAAkB,CAAC,EAC3BA,OAAO,CAAC,kBAAkB,CAAC,EAC3BA,OAAO,CAAC,kBAAkB,CAAC,CAC5B;MACDC,cAAc,EAAE,CAAC;MACjBC,QAAQ,EAAE,CACR;QAAEL,IAAI,EAAE,SAAS;QAAEM,KAAK,EAAEH,OAAO,CAAC,sBAAsB;MAAE,CAAC,EAC3D;QAAEH,IAAI,EAAE,SAAS;QAAEM,KAAK,EAAEH,OAAO,CAAC,sBAAsB;MAAE,CAAC,EAC3D;QAAEH,IAAI,EAAE,MAAM;QAAEM,KAAK,EAAEH,OAAO,CAAC,mBAAmB;MAAE,CAAC,EACrD;QAAEH,IAAI,EAAE,SAAS;QAAEM,KAAK,EAAEH,OAAO,CAAC,uBAAuB;MAAE,CAAC,CAC7D;MACDI,mBAAmB,EAAE;IACvB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,OAAO;QACLC,eAAe,EAAE,OAAO,IAAI,CAACR,gBAAgB,CAAC,IAAI,CAACE,cAAc,CAAC,GAAG;QACrEO,cAAc,EAAE,OAAO;QACvBC,kBAAkB,EAAE,QAAQ;QAC5BC,MAAM,EAAE,OAAO;QACfC,UAAU,EAAE;MACd,CAAC;IACH;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACRC,WAAW,CAAC,MAAM;MAChB,IAAI,CAACZ,cAAa,GAChB,CAAC,IAAI,CAACA,cAAa,GAAI,CAAC,IAAI,IAAI,CAACF,gBAAgB,CAACe,MAAM;IAC5D,CAAC,EAAE,IAAI,CAAC;IAERD,WAAW,CAAC,MAAM;MAChB,IAAI,CAACT,mBAAkB,GACrB,CAAC,IAAI,CAACA,mBAAkB,GAAI,CAAC,IAAI,IAAI,CAACF,QAAQ,CAACY,MAAM;IACzD,CAAC,EAAE,IAAI,CAAC;EACV;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}