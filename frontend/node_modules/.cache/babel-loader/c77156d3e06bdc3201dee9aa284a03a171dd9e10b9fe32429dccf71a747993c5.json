{"ast":null,"code":"import \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"AddProduct\",\n  data() {\n    return {\n      name: \"\",\n      description: \"\",\n      originalPrice: \"\",\n      discountPrice: \"\",\n      image: null,\n      // file object\n      preview: null,\n      // preview URL\n      loading: false\n    };\n  },\n  methods: {\n    handleImageUpload(e) {\n      const file = e.target.files[0];\n      if (file) {\n        this.image = file;\n        this.preview = URL.createObjectURL(file);\n      }\n    },\n    async addProduct() {\n      if (!this.image) {\n        alert(\"⚠️ Please select an image\");\n        return;\n      }\n      this.loading = true;\n      try {\n        const formData = new FormData();\n        formData.append(\"name\", this.name);\n        formData.append(\"description\", this.description);\n        formData.append(\"originalPrice\", this.originalPrice);\n        formData.append(\"discountPrice\", this.discountPrice);\n        formData.append(\"image\", this.image);\n\n        // ✅ Corrected API endpoint\n        const res = await axios.post(\"http://localhost:5000/api/products/addproduct\", formData, {\n          headers: {\n            \"Content-Type\": \"multipart/form-data\"\n          }\n        });\n        alert(\"✅ Product added successfully!\");\n        console.log(res.data);\n\n        // Reset form\n        this.name = \"\";\n        this.description = \"\";\n        this.originalPrice = \"\";\n        this.discountPrice = \"\";\n        this.image = null;\n        this.preview = null;\n      } catch (err) {\n        alert(\"❌ Error adding product\");\n        console.error(\"AddProduct error:\", err.response?.data || err.message);\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","description","originalPrice","discountPrice","image","preview","loading","methods","handleImageUpload","e","file","target","files","URL","createObjectURL","addProduct","alert","formData","FormData","append","res","post","headers","console","log","err","error","response","message"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\AddProduct.vue"],"sourcesContent":["<template>\n  <div class=\"add-product\">\n    <h2>Add New Product</h2>\n    <form @submit.prevent=\"addProduct\">\n      <!-- Name -->\n      <div>\n        <label for=\"name\">Name:</label>\n        <input id=\"name\" v-model=\"name\" type=\"text\" required />\n      </div>\n\n      <!-- Description -->\n      <div>\n        <label for=\"description\">Description:</label>\n        <textarea id=\"description\" v-model=\"description\" required></textarea>\n      </div>\n\n      <!-- Original Price -->\n      <div>\n        <label for=\"originalPrice\">Original Price:</label>\n        <input\n          id=\"originalPrice\"\n          v-model.number=\"originalPrice\"\n          type=\"number\"\n          required\n        />\n      </div>\n\n      <!-- Discount Price -->\n      <div>\n        <label for=\"discountPrice\">Discount Price:</label>\n        <input\n          id=\"discountPrice\"\n          v-model.number=\"discountPrice\"\n          type=\"number\"\n          required\n        />\n      </div>\n\n      <!-- File Upload for Image -->\n      <div>\n        <label for=\"image\">Product Image:</label>\n        <input\n          id=\"image\"\n          type=\"file\"\n          @change=\"handleImageUpload\"\n          accept=\"image/*\"\n          required\n        />\n      </div>\n\n      <!-- Image Preview -->\n      <div v-if=\"preview\" class=\"image-preview\">\n        <p>Preview:</p>\n        <img :src=\"preview\" alt=\"Preview\" />\n      </div>\n\n      <!-- Submit -->\n      <button type=\"submit\" :disabled=\"loading\">\n        {{ loading ? \"Adding...\" : \"Add Product\" }}\n      </button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"AddProduct\",\n  data() {\n    return {\n      name: \"\",\n      description: \"\",\n      originalPrice: \"\",\n      discountPrice: \"\",\n      image: null, // file object\n      preview: null, // preview URL\n      loading: false,\n    };\n  },\n  methods: {\n    handleImageUpload(e) {\n      const file = e.target.files[0];\n      if (file) {\n        this.image = file;\n        this.preview = URL.createObjectURL(file);\n      }\n    },\n    async addProduct() {\n      if (!this.image) {\n        alert(\"⚠️ Please select an image\");\n        return;\n      }\n\n      this.loading = true;\n      try {\n        const formData = new FormData();\n        formData.append(\"name\", this.name);\n        formData.append(\"description\", this.description);\n        formData.append(\"originalPrice\", this.originalPrice);\n        formData.append(\"discountPrice\", this.discountPrice);\n        formData.append(\"image\", this.image);\n\n        // ✅ Corrected API endpoint\n        const res = await axios.post(\n          \"http://localhost:5000/api/products/addproduct\",\n          formData,\n          { headers: { \"Content-Type\": \"multipart/form-data\" } }\n        );\n\n        alert(\"✅ Product added successfully!\");\n        console.log(res.data);\n\n        // Reset form\n        this.name = \"\";\n        this.description = \"\";\n        this.originalPrice = \"\";\n        this.discountPrice = \"\";\n        this.image = null;\n        this.preview = null;\n      } catch (err) {\n        alert(\"❌ Error adding product\");\n        console.error(\"AddProduct error:\", err.response?.data || err.message);\n      } finally {\n        this.loading = false;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.add-product {\n  max-width: 450px;\n  margin: 20px auto;\n  padding: 20px;\n