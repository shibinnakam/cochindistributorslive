{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"LoginPage\",\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      error: \"\",\n      loading: false\n    };\n  },\n  methods: {\n    async handleLogin() {\n      this.error = \"\";\n      this.loading = true;\n      try {\n        const res = await axios.post(\"/api/auth/login\", {\n          email: this.email,\n          password: this.password\n        });\n        localStorage.setItem(\"token\", res.data.token);\n        this.$router.push(\"/\");\n      } catch (err) {\n        this.error = err.response?.data?.msg || \"Login failed\";\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","error","loading","methods","handleLogin","res","post","localStorage","setItem","token","$router","push","err","response","msg"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\Login.vue"],"sourcesContent":["<template>\n  <div class=\"login-container\">\n    <h2>Login</h2>\n    <form @submit.prevent=\"handleLogin\">\n      <input type=\"email\" v-model=\"email\" placeholder=\"Email\" required />\n      <input type=\"password\" v-model=\"password\" placeholder=\"Password\" required />\n\n      <p v-if=\"error\" class=\"error\">{{ error }}</p>\n\n      <button type=\"submit\" :disabled=\"loading\">\n        {{ loading ? \"Logging in...\" : \"Login\" }}\n      </button>\n\n      <router-link to=\"/forgetpassword\" class=\"forgot-password-link\">\n        Forgot password?\n      </router-link>\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"LoginPage\",\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      error: \"\",\n      loading: false,\n    };\n  },\n  methods: {\n    async handleLogin() {\n      this.error = \"\";\n      this.loading = true;\n      try {\n        const res = await axios.post(\"/api/auth/login\", {\n          email: this.email,\n          password: this.password,\n        });\n        localStorage.setItem(\"token\", res.data.token);\n        this.$router.push(\"/\");\n      } catch (err) {\n        this.error = err.response?.data?.msg || \"Login failed\";\n      } finally {\n        this.loading = false;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.login-container {\n  max-width: 400px;\n  margin: 3rem auto;\n  padding: 2rem;\n  border: 1px solid #ccc;\n  border-radius: 8px;\n}\n.forgot-password-link {\n  display: block;\n  margin-top: 1rem;\n  text-align: right;\n  color: #1e3a8a;\n  text-decoration: underline;\n  cursor: pointer;\n}\n.error {\n  color: red;\n  font-size: 0.9rem;\n}\n</style>\n"],"mappings":";AAqBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClB,IAAI,CAACH,KAAI,GAAI,EAAE;MACf,IAAI,CAACC,OAAM,GAAI,IAAI;MACnB,IAAI;QACF,MAAMG,GAAE,GAAI,MAAMT,KAAK,CAACU,IAAI,CAAC,iBAAiB,EAAE;UAC9CP,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QACFO,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEH,GAAG,CAACP,IAAI,CAACW,KAAK,CAAC;QAC7C,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC;MACxB,EAAE,OAAOC,GAAG,EAAE;QACZ,IAAI,CAACX,KAAI,GAAIW,GAAG,CAACC,QAAQ,EAAEf,IAAI,EAAEgB,GAAE,IAAK,cAAc;MACxD,UAAU;QACR,IAAI,CAACZ,OAAM,GAAI,KAAK;MACtB;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}