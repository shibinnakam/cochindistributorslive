{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.reduce.js\";\nimport axios from \"axios\";\nimport \"@/assets/styles/AdminPage.css\";\nimport AddProduct from \"@/views/AddProduct.vue\";\nimport ProductList from \"@/views/ProductList.vue\";\nimport ProductCategory from \"@/views/AddCategory.vue\";\nimport StaffManagement from \"@/views/StaffManagement.vue\";\nimport AdminLeaves from \"@/views/AdminLeaves.vue\";\nconst API_BASE_URL = \"http://localhost:5000/api\";\nexport default {\n  name: \"AdminPage\",\n  components: {\n    AddProduct,\n    ProductList,\n    ProductCategory,\n    StaffManagement,\n    AdminLeaves\n  },\n  data() {\n    return {\n      selectedMenu: \"dashboard\",\n      dashboardLoading: false,\n      totalProducts: 0,\n      totalCategories: 0,\n      totalStaff: 0,\n      activeStaff: 0,\n      pendingLeaves: 0,\n      approvedLeaves: 0,\n      totalInventory: 0\n    };\n  },\n  mounted() {\n    this.fetchDashboardStats();\n  },\n  methods: {\n    async fetchDashboardStats() {\n      this.dashboardLoading = true;\n      try {\n        const [productsRes, categoriesRes, staffRes, leavesRes] = await Promise.all([axios.get(`${API_BASE_URL}/products/getproduct`), axios.get(`${API_BASE_URL}/categories`), axios.get(`${API_BASE_URL}/staff`), axios.get(`${API_BASE_URL}/leaves/all`)]);\n        if (productsRes.data.success && productsRes.data.products) {\n          this.totalProducts = productsRes.data.products.length;\n          this.totalInventory = productsRes.data.products.reduce((sum, product) => sum + (product.quantity || 0), 0);\n        }\n        if (Array.isArray(categoriesRes.data)) {\n          this.totalCategories = categoriesRes.data.length;\n        }\n        if (staffRes.data.success && Array.isArray(staffRes.data.staff)) {\n          this.totalStaff = staffRes.data.staff.length;\n          this.activeStaff = staffRes.data.staff.filter(s => s.status === \"active\").length;\n        }\n        if (Array.isArray(leavesRes.data)) {\n          this.pendingLeaves = leavesRes.data.filter(l => l.status === \"Pending\").length;\n          this.approvedLeaves = leavesRes.data.filter(l => l.status === \"Approved\").length;\n        }\n      } catch (error) {\n        console.error(\"Error fetching dashboard stats:\", error);\n      } finally {\n        this.dashboardLoading = false;\n      }\n    },\n    logout() {\n      localStorage.removeItem(\"token\");\n      localStorage.removeItem(\"user\");\n      this.$router.push(\"/login\");\n    }\n  }\n};","map":{"version":3,"names":["axios","AddProduct","ProductList","ProductCategory","StaffManagement","AdminLeaves","API_BASE_URL","name","components","data","selectedMenu","dashboardLoading","totalProducts","totalCategories","totalStaff","activeStaff","pendingLeaves","approvedLeaves","totalInventory","mounted","fetchDashboardStats","methods","productsRes","categoriesRes","staffRes","leavesRes","Promise","all","get","success","products","length","reduce","sum","product","quantity","Array","isArray","staff","filter","s","status","l","error","console","logout","localStorage","removeItem","$router","push"],"sources":["C:\\Users\\LENOVO\\Music\\OneDrive\\Pictures\\distribution-agency-project\\frontend\\src\\views\\AdminPage.vue"],"sourcesContent":["<template>\n  <div class=\"admin-wrapper\">\n    <!-- Sidebar -->\n    <aside class=\"sidebar\">\n      <h2 class=\"sidebar-title\">Master Admin</h2>\n      <ul class=\"menu\">\n        <li>\n          <a\n            href=\"#\"\n            @click.prevent=\"selectedMenu = 'dashboard'\"\n            :class=\"{ active: selectedMenu === 'dashboard' }\"\n          >\n            Dashboard\n          </a>\n        </li>\n        <li>\n          <a\n            href=\"#\"\n            @click.prevent=\"selectedMenu = 'addProduct'\"\n            :class=\"{ active: selectedMenu === 'addProduct' }\"\n          >\n            Add Products\n          </a>\n        </li>\n        <li>\n          <a\n            href=\"#\"\n            @click.prevent=\"selectedMenu = 'addCategory'\"\n            :class=\"{ active: selectedMenu === 'addCategory' }\"\n          >\n            Add Category\n          </a>\n        </li>\n        <li>\n          <a\n            href=\"#\"\n            @click.prevent=\"selectedMenu = 'staffManagement'\"\n            :class=\"{ active: selectedMenu === 'staffManagement' }\"\n          >\n            Staff Management\n          </a>\n        </li>\n        <!-- Fixed Approve Leaves -->\n        <li>\n          <a\n            href=\"#\"\n            @click.prevent=\"selectedMenu = 'approve-leaves'\"\n            :class=\"{ active: selectedMenu === 'approve-leaves' }\"\n          >\n            Approve Leaves\n          </a>\n        </li>\n        <li><a href=\"#\">Page Management</a></li>\n        <li>\n          <a\n            href=\"#\"\n            @click.prevent=\"selectedMenu = 'listProducts'\"\n            :class=\"{ active: selectedMenu === 'listProducts' }\"\n          >\n            List Products\n          </a>\n        </li>\n      </ul>\n    </aside>\n\n    <!-- Main -->\n    <div class=\"main\">\n      <!-- Header -->\n      <header class=\"header\">\n        <h1>Dashboard <span class=\"small\">Control panel</span></h1>\n\n        <div class=\"admin-menu\">\n          <span>Laravel Admin ▾</span>\n          <button class=\"logout-btn\" @click=\"logout\">\n            <i class=\"bi bi-box-arrow-right\"></i> Logout\n          </button>\n        </div>\n      </header>\n\n      <!-- Main Content -->\n      <main class=\"content\">\n        <ProductCategory v-if=\"selectedMenu === 'addCategory'\" />\n        <ProductList v-if=\"selectedMenu === 'listProducts'\" />\n        <StaffManagement v-if=\"selectedMenu === 'staffManagement'\" />\n        <AdminLeaves v-if=\"selectedMenu === 'approve-leaves'\" />\n\n        <!-- Dashboard Cards -->\n        <template v-if=\"selectedMenu === 'dashboard' || !selectedMenu\">\n          <div v-if=\"dashboardLoading\" class=\"loading-dashboard\">\n            <p>Loading dashboard statistics...</p>\n          </div>\n          <div v-else>\n            <div class=\"card blue\">\n              <h2>{{ totalProducts }}</h2>\n              <p>Total Products</p>\n              <span class=\"info\">View all products →</span>\n            </div>\n            <div class=\"card orange\">\n              <h2>{{ totalCategories }}</h2>\n              <p>Total Categories</p>\n              <span class=\"info\">View all categories →</span>\n            </div>\n            <div class=\"card red\">\n              <h2>{{ totalStaff }}</h2>\n              <p>Total Staff</p>\n              <span class=\"info\">Manage staff →</span>\n            </div>\n            <div class=\"card green\">\n              <h2>{{ activeStaff }}</h2>\n              <p>Active Staff</p>\n              <span class=\"info\">More info →</span>\n            </div>\n            <div class=\"card purple\">\n              <h2>{{ pendingLeaves }}</h2>\n              <p>Pending Leaves</p>\n              <span class=\"info\">Approve leaves →</span>\n            </div>\n            <div class=\"card pink\">\n              <h2>{{ approvedLeaves }}</h2>\n              <p>Approved Leaves</p>\n              <span class=\"info\">More info →</span>\n            </div>\n            <div class=\"card teal\">\n              <h2>{{ totalInventory }}</h2>\n              <p>Total Inventory Items</p>\n              <span class=\"info\">More info →</span>\n            </div>\n          </div>\n        </template>\n      </main>\n    </div>\n  </div>\n</template>\n<script>\nimport axios from \"axios\";\nimport \"@/assets/styles/AdminPage.css\";\nimport AddProduct from \"@/views/AddProduct.vue\";\nimport ProductList from \"@/views/ProductList.vue\";\nimport ProductCategory from \"@/views/AddCategory.vue\";\nimport StaffManagement from \"@/views/StaffManagement.vue\";\nimport AdminLeaves from \"@/views/AdminLeaves.vue\";\n\nconst API_BASE_URL = \"http://localhost:5000/api\";\n\nexport default {\n  name: \"AdminPage\",\n  components: {\n    AddProduct,\n    ProductList,\n    ProductCategory,\n    StaffManagement,\n    AdminLeaves,\n  },\n  data() {\n    return {\n      selectedMenu: \"dashboard\",\n      dashboardLoading: false,\n      totalProducts: 0,\n      totalCategories: 0,\n      totalStaff: 0,\n      activeStaff: 0,\n      pendingLeaves: 0,\n      approvedLeaves: 0,\n      totalInventory: 0,\n    };\n  },\n  mounted() {\n    this.fetchDashboardStats();\n  },\n  methods: {\n    async fetchDashboardStats() {\n      this.dashboardLoading = true;\n      try {\n        const [productsRes, categoriesRes, staffRes, leavesRes] =\n          await Promise.all([\n            axios.get(`${API_BASE_URL}/products/getproduct`),\n            axios.get(`${API_BASE_URL}/categories`),\n            axios.get(`${API_BASE_URL}/staff`),\n            axios.get(`${API_BASE_URL}/leaves/all`),\n          ]);\n\n        if (productsRes.data.success && productsRes.data.products) {\n          this.totalProducts = productsRes.data.products.length;\n          this.totalInventory = productsRes.data.products.reduce(\n            (sum, product) => sum + (product.quantity || 0),\n            0\n          );\n        }\n\n        if (Array.isArray(categoriesRes.data)) {\n          this.totalCategories = categoriesRes.data.length;\n        }\n\n        if (staffRes.data.success && Array.isArray(staffRes.data.staff)) {\n          this.totalStaff = staffRes.data.staff.length;\n          this.activeStaff = staffRes.data.staff.filter(\n            (s) => s.status === \"active\"\n          ).length;\n        }\n\n        if (Array.isArray(leavesRes.data)) {\n          this.pendingLeaves = leavesRes.data.filter(\n            (l) => l.status === \"Pending\"\n          ).length;\n          this.approvedLeaves = leavesRes.data.filter(\n            (l) => l.status === \"Approved\"\n          ).length;\n        }\n      } catch (error) {\n        console.error(\"Error fetching dashboard stats:\", error);\n      } finally {\n        this.dashboardLoading = false;\n      }\n    },\n    logout() {\n      localStorage.removeItem(\"token\");\n      localStorage.removeItem(\"user\");\n      this.$router.push(\"/login\");\n    },\n  },\n};\n</script>\n\n<style scoped>\n.menu a.active {\n  font-weight: 600;\n  color: #ffffff;\n}\n\n.admin-wrapper {\n  background: #f8fafc;\n}\n\n.sidebar {\n  scrollbar-width: thin;\n  scrollbar-color: #475569 #e2e8f0;\n}\n\n.content {\n  background: linear-gradient(135deg, #f8fafc 0%, #f1f5f9 100%);\n}\n\n.loading-dashboard {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  min-height: 400px;\n  width: 100%;\n}\n\n.loading-dashboard p {\n  font-size: 18px;\n  color: #64748b;\n  font-weight: 500;\n  animation: pulse 1.5s ease-in-out infinite;\n}\n\n@keyframes pulse {\n  0%,\n  100% {\n    opacity: 0.6;\n  }\n  50% {\n    opacity: 1;\n  }\n}\n</style>\n"],"mappings":";;;;AAsIA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAO,+BAA+B;AACtC,OAAOC,UAAS,MAAO,wBAAwB;AAC/C,OAAOC,WAAU,MAAO,yBAAyB;AACjD,OAAOC,eAAc,MAAO,yBAAyB;AACrD,OAAOC,eAAc,MAAO,6BAA6B;AACzD,OAAOC,WAAU,MAAO,yBAAyB;AAEjD,MAAMC,YAAW,GAAI,2BAA2B;AAEhD,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACVP,UAAU;IACVC,WAAW;IACXC,eAAe;IACfC,eAAe;IACfC;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,WAAW;MACzBC,gBAAgB,EAAE,KAAK;MACvBC,aAAa,EAAE,CAAC;MAChBC,eAAe,EAAE,CAAC;MAClBC,UAAU,EAAE,CAAC;MACbC,WAAW,EAAE,CAAC;MACdC,aAAa,EAAE,CAAC;MAChBC,cAAc,EAAE,CAAC;MACjBC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,mBAAmB,CAAC,CAAC;EAC5B,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,mBAAmBA,CAAA,EAAG;MAC1B,IAAI,CAACT,gBAAe,GAAI,IAAI;MAC5B,IAAI;QACF,MAAM,CAACW,WAAW,EAAEC,aAAa,EAAEC,QAAQ,EAAEC,SAAS,IACpD,MAAMC,OAAO,CAACC,GAAG,CAAC,CAChB3B,KAAK,CAAC4B,GAAG,CAAC,GAAGtB,YAAY,