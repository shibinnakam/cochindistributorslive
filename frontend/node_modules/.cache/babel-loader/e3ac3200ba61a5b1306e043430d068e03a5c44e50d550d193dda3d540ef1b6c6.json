{"ast":null,"code":"import axios from 'axios';\nimport { useRoute } from 'vue-router';\nexport default {\n  setup() {\n    const route = useRoute();\n    return {\n      route\n    };\n  },\n  data() {\n    return {\n      name: '',\n      address: '',\n      phone: '',\n      password: '',\n      message: ''\n    };\n  },\n  methods: {\n    async register() {\n      const token = this.route.query.token; // get token from URL\n      if (!token) {\n        this.message = 'Invalid or expired link';\n        return;\n      }\n      try {\n        const res = await axios.post('http://localhost:5000/api/staff/register', {\n          token,\n          name: this.name,\n          address: this.address,\n          phone: this.phone,\n          password: this.password\n        });\n        this.message = res.data.message;\n      } catch (err) {\n        this.message = err.response?.data?.message || 'Error submitting registration';\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","useRoute","setup","route","data","name","address","phone","password","message","methods","register","token","query","res","post","err","response"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\StaffRegister.vue"],"sourcesContent":["<template>\r\n  <div class=\"staff-register\">\r\n    <h2>Complete Your Registration</h2>\r\n    <form @submit.prevent=\"register\">\r\n      <input v-model=\"name\" placeholder=\"Name\" required />\r\n      <input v-model=\"address\" placeholder=\"Address\" required />\r\n      <input v-model=\"phone\" placeholder=\"Phone\" required />\r\n      <input type=\"password\" v-model=\"password\" placeholder=\"Password\" required />\r\n      <button type=\"submit\">Submit</button>\r\n    </form>\r\n    <p v-if=\"message\">{{ message }}</p>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { useRoute } from 'vue-router';\r\n\r\nexport default {\r\n  setup() {\r\n    const route = useRoute();\r\n\r\n    return { route };\r\n  },\r\n  data() {\r\n    return {\r\n      name: '',\r\n      address: '',\r\n      phone: '',\r\n      password: '',\r\n      message: ''\r\n    };\r\n  },\r\n  methods: {\r\n    async register() {\r\n      const token = this.route.query.token; // get token from URL\r\n      if (!token) {\r\n        this.message = 'Invalid or expired link';\r\n        return;\r\n      }\r\n\r\n      try {\r\n        const res = await axios.post('http://localhost:5000/api/staff/register', {\r\n          token,\r\n          name: this.name,\r\n          address: this.address,\r\n          phone: this.phone,\r\n          password: this.password\r\n        });\r\n        this.message = res.data.message;\r\n      } catch (err) {\r\n        this.message = err.response?.data?.message || 'Error submitting registration';\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"mappings":"AAeA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,QAAO,QAAS,YAAY;AAErC,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIF,QAAQ,CAAC,CAAC;IAExB,OAAO;MAAEE;IAAM,CAAC;EAClB,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,OAAO,EAAE,EAAE;MACXC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,QAAQA,CAAA,EAAG;MACf,MAAMC,KAAI,GAAI,IAAI,CAACT,KAAK,CAACU,KAAK,CAACD,KAAK,EAAE;MACtC,IAAI,CAACA,KAAK,EAAE;QACV,IAAI,CAACH,OAAM,GAAI,yBAAyB;QACxC;MACF;MAEA,IAAI;QACF,MAAMK,GAAE,GAAI,MAAMd,KAAK,CAACe,IAAI,CAAC,0CAA0C,EAAE;UACvEH,KAAK;UACLP,IAAI,EAAE,IAAI,CAACA,IAAI;UACfC,OAAO,EAAE,IAAI,CAACA,OAAO;UACrBC,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QACF,IAAI,CAACC,OAAM,GAAIK,GAAG,CAACV,IAAI,CAACK,OAAO;MACjC,EAAE,OAAOO,GAAG,EAAE;QACZ,IAAI,CAACP,OAAM,GAAIO,GAAG,CAACC,QAAQ,EAAEb,IAAI,EAAEK,OAAM,IAAK,+BAA+B;MAC/E;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}