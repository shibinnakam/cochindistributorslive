{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"ResetPassword\",\n  data() {\n    return {\n      password: \"\",\n      confirmPassword: \"\",\n      message: \"\",\n      error: \"\",\n      loading: false\n    };\n  },\n  computed: {\n    token() {\n      return this.$route.query.token || \"\";\n    }\n  },\n  methods: {\n    async handleResetPassword() {\n      this.message = \"\";\n      this.error = \"\";\n      if (!this.token) {\n        this.error = \"Invalid reset token.\";\n        return;\n      }\n      if (!this.password || !this.confirmPassword) {\n        this.error = \"Please fill both password fields.\";\n        return;\n      }\n      if (this.password !== this.confirmPassword) {\n        this.error = \"Passwords do not match.\";\n        return;\n      }\n      try {\n        this.loading = true;\n        const res = await axios.post(\"/api/auth/reset-password\", {\n          token: this.token,\n          password: this.password\n        });\n        this.message = res.data.message || \"Password updated successfully.\";\n        // optionally redirect to login after short delay\n        setTimeout(() => {\n          this.$router.push(\"/login\");\n        }, 1500);\n      } catch (err) {\n        this.error = err.response?.data?.msg || \"Error resetting password.\";\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","password","confirmPassword","message","error","loading","computed","token","$route","query","methods","handleResetPassword","res","post","setTimeout","$router","push","err","response","msg"],"sources":["C:\\Users\\LENOVO\\Music\\distribution-agency-project\\frontend\\src\\views\\ResetPassword.vue"],"sourcesContent":["<template>\n  <div class=\"reset-container\">\n    <div class=\"reset-box\">\n      <h2>Reset Password</h2>\n\n      <form @submit.prevent=\"handleResetPassword\" novalidate>\n        <input\n          type=\"password\"\n          v-model.trim=\"password\"\n          placeholder=\"New password\"\n          required\n          autocomplete=\"new-password\"\n        />\n        <input\n          type=\"password\"\n          v-model.trim=\"confirmPassword\"\n          placeholder=\"Confirm new password\"\n          required\n          autocomplete=\"new-password\"\n        />\n\n        <button type=\"submit\" :disabled=\"loading\">\n          <span v-if=\"loading\">Updating...</span>\n          <span v-else>Reset Password</span>\n        </button>\n      </form>\n\n      <p v-if=\"message\" class=\"success\">{{ message }}</p>\n      <p v-if=\"error\" class=\"error\">{{ error }}</p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"ResetPassword\",\n  data() {\n    return {\n      password: \"\",\n      confirmPassword: \"\",\n      message: \"\",\n      error: \"\",\n      loading: false,\n    };\n  },\n  computed: {\n    token() {\n      return this.$route.query.token || \"\";\n    },\n  },\n  methods: {\n    async handleResetPassword() {\n      this.message = \"\";\n      this.error = \"\";\n\n      if (!this.token) {\n        this.error = \"Invalid reset token.\";\n        return;\n      }\n\n      if (!this.password || !this.confirmPassword) {\n        this.error = \"Please fill both password fields.\";\n        return;\n      }\n\n      if (this.password !== this.confirmPassword) {\n        this.error = \"Passwords do not match.\";\n        return;\n      }\n\n      try {\n        this.loading = true;\n        const res = await axios.post(\"/api/auth/reset-password\", {\n          token: this.token,\n          password: this.password,\n        });\n        this.message = res.data.message || \"Password updated successfully.\";\n        // optionally redirect to login after short delay\n        setTimeout(() => {\n          this.$router.push(\"/login\");\n        }, 1500);\n      } catch (err) {\n        this.error = err.response?.data?.msg || \"Error resetting password.\";\n      } finally {\n        this.loading = false;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.reset-container {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  height: 100vh;\n  background: rgba(0, 0, 0, 0.05);\n}\n\n.reset-box {\n  background: #fff;\n  padding: 2rem;\n  border-radius: 10px;\n  max-width: 380px;\n  width: 100%;\n  box-shadow: 0 6px 20px rgba(0, 0, 0, 0.08);\n}\n\ninput {\n  width: 100%;\n  padding: 0.6rem;\n  margin-bottom: 1rem;\n  border-radius: 6px;\n  border: 1px solid #ddd;\n  box-sizing: border-box;\n}\n\nbutton {\n  width: 100%;\n  padding: 0.6rem;\n  border-radius: 6px;\n  border: none;\n  background: #1e3a8a;\n  color: white;\n  cursor: pointer;\n}\n\n.success {\n  color: green;\n  font-size: 0.9rem;\n}\n\n.error {\n  color: red;\n  font-size: 0.9rem;\n}\n</style>\n"],"mappings":";AAkCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE,EAAE;MACnBC,OAAO,EAAE,EAAE;MACXC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,KAAKA,CAAA,EAAG;MACN,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,KAAI,IAAK,EAAE;IACtC;EACF,CAAC;EACDG,OAAO,EAAE;IACP,MAAMC,mBAAmBA,CAAA,EAAG;MAC1B,IAAI,CAACR,OAAM,GAAI,EAAE;MACjB,IAAI,CAACC,KAAI,GAAI,EAAE;MAEf,IAAI,CAAC,IAAI,CAACG,KAAK,EAAE;QACf,IAAI,CAACH,KAAI,GAAI,sBAAsB;QACnC;MACF;MAEA,IAAI,CAAC,IAAI,CAACH,QAAO,IAAK,CAAC,IAAI,CAACC,eAAe,EAAE;QAC3C,IAAI,CAACE,KAAI,GAAI,mCAAmC;QAChD;MACF;MAEA,IAAI,IAAI,CAACH,QAAO,KAAM,IAAI,CAACC,eAAe,EAAE;QAC1C,IAAI,CAACE,KAAI,GAAI,yBAAyB;QACtC;MACF;MAEA,IAAI;QACF,IAAI,CAACC,OAAM,GAAI,IAAI;QACnB,MAAMO,GAAE,GAAI,MAAMd,KAAK,CAACe,IAAI,CAAC,0BAA0B,EAAE;UACvDN,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBN,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QACF,IAAI,CAACE,OAAM,GAAIS,GAAG,CAACZ,IAAI,CAACG,OAAM,IAAK,gCAAgC;QACnE;QACAW,UAAU,CAAC,MAAM;UACf,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;QAC7